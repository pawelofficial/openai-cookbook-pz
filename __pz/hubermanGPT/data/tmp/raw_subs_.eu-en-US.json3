  }, {
    "tStartMs": 3613280,
    "dDurationMs": 1350,
    "segs": [ {
      "utf8": "jende gehienak atxikitzea."
    } ]
  }, {
    "tStartMs": 3614630,
    "dDurationMs": 2010,
    "segs": [ {
      "utf8": "Beraz, niretzat pantaila itzali egiten da,"
    } ]
  }, {
    "tStartMs": 3616640,
    "dDurationMs": 4430,
    "segs": [ {
      "utf8": "argi ahulak dira, eta hori da\n"
    } ]
  }, {
    "tStartMs": 3621070,
    "dDurationMs": 2150,
    "segs": [ {
      "utf8": "gaueko lo on batean lo egitearen alde egiten duena."
    } ]
  }, {
    "tStartMs": 3623220,
    "dDurationMs": 1470,
    "segs": [ {
      "utf8": "Lehen janariari buruz hitz egiten ari ginenez, lehen esandakoa"
    } ]
  }, {
    "tStartMs": 3624690,
    "dDurationMs": 2530,
    "segs": [ {
      "utf8": "apur bat errepasatuko dut\n."
    } ]
  }, {
    "tStartMs": 3627220,
    "dDurationMs": 3600,
    "segs": [ {
      "utf8": "Nire arratsaldeko bazkaria\nkarbohidrato gehiago izan ohi da."
    } ]
  }, {
    "tStartMs": 3630820,
    "dDurationMs": 2210,
    "segs": [ {
      "utf8": "Proteinak baditut,\narrautzak, arraina"
