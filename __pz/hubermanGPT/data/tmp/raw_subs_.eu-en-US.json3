    } ]
  }, {
    "tStartMs": 3797720,
    "dDurationMs": 1720,
    "segs": [ {
      "utf8": "beste aldagai batzuen berri."
    } ]
  }, {
    "tStartMs": 3799440,
    "dDurationMs": 2350,
    "segs": [ {
      "utf8": "Gogoratu, zientzia ona\naldagaiak isolatzea dela."
    } ]
  }, {
    "tStartMs": 3801790,
    "dDurationMs": 2890,
    "segs": [ {
      "utf8": "Eta, askotan,\nsare sozialetan ikusten duguna"
    } ]
  }, {
    "tStartMs": 3804680,
    "dDurationMs": 2370,
    "segs": [ {
      "utf8": "\naldagai bakarrak aurkezten zaizkigula"
    } ]
  }, {
    "tStartMs": 3807050,
    "dDurationMs": 1550,
    "segs": [ {
      "utf8": "eta manipulatzen ari diren beste aldagaien testuinguru osoa ez dugu ikusten"
    } ]
  }, {
    "tStartMs": 3808600,
    "dDurationMs": 2560,
    "segs": [ {
      "utf8": "\n."
    } ]
  }, {
    "tStartMs": 3811160,
    "dDurationMs": 2550,
    "segs": [ {
      "utf8": "Beraz, arratsaldean pasta eta arroza"
    } ]
  }, {
    "tStartMs": 3813710,
    "dDurationMs": 1670,
    "segs": [ {
      "utf8": "eta barazkiak eta\nhorrelakoak jaten ditut."
    } ]
  }, {
    "tStartMs": 3815380,
    "dDurationMs": 2790,
    "segs": [ {
      "utf8": "Gainera, besterik ez dut aurkitu, agian"
    } ]
  }, {
    "tStartMs": 3818170,
    "dDurationMs": 1830,
    "segs": [ {
      "utf8": "hori egiten duen azken pertsona bat bihurtzen ari naiz."
    } ]
  }, {
    "tStartMs": 3820000,
    "dDurationMs": 833,
    "segs": [ {
      "utf8": "Ezetz espero dudan arren."
    } ]
  }, {
    "tStartMs": 3820833,
    "dDurationMs": 2097,
    "segs": [ {
      "utf8": "Espero dut ni bezalako beste batzuk egotea."
    } ]
  }, {
    "tStartMs": 3824150,
    "dDurationMs": 1980,
    "segs": [ {
      "utf8": "Literatura guztietatik hitz egiten da"
    } ]
  }, {
    "tStartMs": 3826130,
    "dDurationMs": 1930,
    "segs": [ {
      "utf8": "karbohidratoek hori ez ezik,"
    } ]
  }, {
    "tStartMs": 3828060,
    "dDurationMs": 1040,
    "segs": [ {
      "utf8": ""
    } ]
  }, {
    "tStartMs": 3829100,
    "dDurationMs": 1990,
    "segs": [ {
      "utf8": "tiroideoaren funtzio osasuntsua mantentzen laguntzen dutela, eta abar."
    } ]
  }, {
    "tStartMs": 3831090,
    "dDurationMs": 1080,
    "segs": [ {
      "utf8": "Beraz, hori da nire alborapena."
    } ]
  }, {
    "tStartMs": 3832170,
    "dDurationMs": 833,
    "segs": [ {
      "utf8": "Horixe egiten dudana."
    } ]
  }, {
    "tStartMs": 3833003,
    "dDurationMs": 2787,
    "segs": [ {
      "utf8": "Kafeina eta bestelakoak saihesten ditut\narratsaldean."
    } ]
  }, {
    "tStartMs": 3835790,
    "dDurationMs": 1930,
    "segs": [ {
      "utf8": "Osagarriak hartzen ditut eta uneren batean pozik egongo naiz ateratzen ditudan osagarrien"
    } ]
  }, {
    "tStartMs": 3837720,
    "dDurationMs": 1630,
    "segs": [ {
      "utf8": "zerrenda osoa argitaratzeko"
    } ]
  }, {
    "tStartMs": 3839350,
    "dDurationMs": 2630,
    "segs": [ {
      "utf8": "."
    } ]
  }, {
    "tStartMs": 3841980,
    "dDurationMs": 3220,
    "segs": [ {
      "utf8": "Baina, oro har, hauek dira\negiten ditudan gauza nagusiak."
    } ]
  }, {
    "tStartMs": 3845200,
    "dDurationMs": 1569,
    "segs": [ {
      "utf8": "Eta denboran zehar egin dituzun galdera askorekin erlazionatzen dira"
    } ]
  }, {
    "tStartMs": 3846769,
    "dDurationMs": 2461,
    "segs": [ {
      "utf8": "."
    } ]
  }, {
    "tStartMs": 3849230,
    "dDurationMs": 2790,
    "segs": [ {
      "utf8": "\nDeskribatuko dudan hurrengo datu zientifikoak datu zientifiko"
    } ]
  }, {
    "tStartMs": 3852020,
    "dDurationMs": 2540,
    "segs": [ {
      "utf8": "oso garrantzitsuak dira\n,"
    } ]
  }, {
    "tStartMs": 3854560,
    "dDurationMs": 2260,
    "segs": [ {
      "utf8": "\nzure garuna nola optimizatu"
    } ]
  }, {
    "tStartMs": 3856820,
    "dDurationMs": 1120,
    "segs": [ {
      "utf8": "eta lo egiteko nola sartu ulertzeko."
    } ]
  }, {
    "tStartMs": 3857940,
    "dDurationMs": 4330,
    "segs": [ {
      "utf8": "Gainera,\nantsietate-arazo asko saihesten lagun dezake."
    } ]
  }, {
    "tStartMs": 3862270,
    "dDurationMs": 4020,
    "segs": [ {
      "utf8": "Eta hauek\nCharles Czeisler medikuaren datuekin lotzen dira."
    } ]
  }, {
    "tStartMs": 3866290,
    "dDurationMs": 3080,
    "segs": [ {
      "utf8": "Charles Czeisler doktorearen\nlaborategia da Harvardeko Medikuntza Eskolan.  Harvardeko Medikuntza Eskolako"
    } ]
  }, {
    "tStartMs": 3869370,
    "dDurationMs": 2090,
    "segs": [ {
      "utf8": "lo-laborategi bat zuzentzen du\n."
    } ]
  }, {
    "tStartMs": 3871460,
    "dDurationMs": 2690,
    "segs": [ {
      "utf8": "Aspalditik lan\noso ikusgarria du."
    } ]
  }, {
    "tStartMs": 3874150,
    "dDurationMs": 3740,
    "segs": [ {
      "utf8": "Eta erakutsi duena zera da:\n"
    } ]
  }, {
    "tStartMs": 3877890,
    "dDurationMs": 2650,
    "segs": [ {
      "utf8": "esna egiteko erloju zirkadianoaren irteera gailurra,"
    } ]
  }, {
    "tStartMs": 3880540,
    "dDurationMs": 2110,
    "segs": [ {
      "utf8": "hots,\ngure esnatasunaren gailurra"
    } ]
  }, {
    "tStartMs": 3882650,
    "dDurationMs": 3310,
    "segs": [ {
      "utf8": "eta loaren seinalea kentzea"
    } ]
  }, {
    "tStartMs": 3885960,
    "dDurationMs": 3190,
    "segs": [ {
      "utf8": "egunean oso berandu gertatzen dela."
    } ]
  }, {
    "tStartMs": 3889150,
    "dDurationMs": 3250,
    "segs": [ {
      "utf8": "Beraz, jarduera"
    } ]
  }, {
    "tStartMs": 3892400,
    "dDurationMs": 3100,
    "segs": [ {
      "utf8": "eta gorputz-tenperatura\nbaxuenak ditugu esnatu baino lehen."
    } ]
  }, {
    "tStartMs": 3895500,
    "dDurationMs": 2270,
    "segs": [ {
      "utf8": "Gero esnatzen garen heinean gure\ngorputz-tenperaturak gora egiten du"
    } ]
  }, {
    "tStartMs": 3897770,
    "dDurationMs": 880,
    "segs": [ {
      "utf8": "eta arratsaldean"
    } ]
  }, {
    "tStartMs": 3898650,
    "dDurationMs": 2910,
    "segs": [ {
      "utf8": "gora egiten jarraitzen du\neta gero"
    } ]
  }, {
    "tStartMs": 3901560,
    "dDurationMs": 2430,
    "segs": [ {
      "utf8": "arratsaldean eta oheratze aldera jaitsi ohi da."
    } ]
  }, {
    "tStartMs": 3903990,
    "dDurationMs": 4800,
    "segs": [ {
      "utf8": "Baina bada laburpen bat, garuneko lo-\n"
    } ]
  }, {
    "tStartMs": 3908790,
    "dDurationMs": 4280,
    "segs": [ {
      "utf8": "zentroetatik peptidoak eta beste substantzia batzuk askatzea\n,"
    } ]
  }, {
    "tStartMs": 3913070,
    "dDurationMs": 1540,
    "segs": [ {
      "utf8": "eta"
    } ]
  }, {
    "tStartMs": 3914610,
    "dDurationMs": 2960,
    "segs": [ {
      "utf8": "loaren zentroa\neremu preoptiko hori den nukleo suprakiasmatikoa,"
    } ]
  }, {
    "tStartMs": 3917570,
    "dDurationMs": 1560,
    "segs": [ {
      "utf8": "hori begiratu nahi baduzu."
    } ]
  }, {
    "tStartMs": 3919130,
    "dDurationMs": 2430,
    "segs": [ {
      "utf8": "Eremu preoptiko hau erloju\nzirkadianotik urrun dagoen erloju zirkadianotik ordubete inguru,"
    } ]
  }, {
    "tStartMs": 3921560,
    "dDurationMs": 1560,
    "segs": [ {
      "utf8": "erne"
    } ]
  }, {
    "tStartMs": 3923120,
    "dDurationMs": 3880,
    "segs": [ {
      "utf8": "eta esna-gailurra adierazten duena\n, oheratu baino lehen."
    } ]
  }, {
    "tStartMs": 3927000,
    "dDurationMs": 1380,
    "segs": [ {
      "utf8": "Eta esan zuen, zera, hori benetan arraroa da."
    } ]
  }, {
    "tStartMs": 3928380,
    "dDurationMs": 1530,
    "segs": [ {
      "utf8": "Baina jende asko ohean sartzen da."
    } ]
  }, {
    "tStartMs": 3929910,
    "dDurationMs": 2810,
    "segs": [ {
      "utf8": "Lo egiteko prest daude\neta esna daude."
    } ]
  }, {
    "tStartMs": 3932720,
    "dDurationMs": 1580,
    "segs": [ {
      "utf8": "Eta uste dute hori gauza ez-naturala dela"
    } ]
  }, {
    "tStartMs": 3934300,
    "dDurationMs": 2000,
    "segs": [ {
      "utf8": "edo zerbait gaizki dagoela."
    } ]
  }, {
    "tStartMs": 3936300,
    "dDurationMs": 1520,
    "segs": [ {
      "utf8": "Eta egia esan ez da."
    } ]
  }, {
    "tStartMs": 3937820,
    "dDurationMs": 2550,
    "segs": [ {
      "utf8": "Hau uste da,"
    } ]
  }, {
    "tStartMs": 3940370,
    "dDurationMs": 2570,
    "segs": [ {
      "utf8": "ez dakit, berriro ere,\ndiseinu fasean ez ninduten kontsultatu."
    } ]
  }, {
    "tStartMs": 3942940,
    "dDurationMs": 4410,
    "segs": [ {
      "utf8": "Baina uste da\n"
    } ]
  }, {
    "tStartMs": 3947350,
    "dDurationMs": 3020,
    "segs": [ {
      "utf8": "gizakientzat\nbaliabideak biltzen hasteko"
    } ]
  }, {
    "tStartMs": 3950370,
    "dDurationMs": 1390,
    "segs": [ {
      "utf8": "eta"
    } ]
  }, {
    "tStartMs": 3951760,
    "dDurationMs": 1970,
    "segs": [ {
      "utf8": "gau bateko lo egiteko lagungarria den seinalea dela, eta,\nbadakizue,"
    } ]
  }, {
    "tStartMs": 3953730,
    "dDurationMs": 2180,
    "segs": [ {
      "utf8": "historikoki oso zaurgarriak izan ginen\n"
    } ]
  }, {
    "tStartMs": 3955910,
    "dDurationMs": 3020,
    "segs": [ {
      "utf8": "beste gizakien eta\nanimalien eta abarren erasoen aurrean."
    } ]
  }, {
    "tStartMs": 3958930,
    "dDurationMs": 3250,
    "segs": [ {
      "utf8": "Eta, beraz,\nsukaldea lasterka eta garbiketa"
    } ]
  }, {
    "tStartMs": 3962180,
    "dDurationMs": 2000,
    "segs": [ {
      "utf8": "edo gauzak antolatzeko gogoa, edo,\nbesterik gabe,"
    } ]
  }, {
    "tStartMs": 3964180,
    "dDurationMs": 2730,
    "segs": [ {
      "utf8": "barne antsietate sentsazio orokorra, arratsaldean berandu,"
    } ]
  }, {
    "tStartMs": 3966910,
    "dDurationMs": 3080,
    "segs": [ {
      "utf8": "\n"
    } ]
  }, {
    "tStartMs": 3969990,
    "dDurationMs": 2410,
    "segs": [ {
      "utf8": "45 eta 60 minuturen buruan modu naturalean igarotzen den blip naturala da."
    } ]
  }, {
    "tStartMs": 3972400,
    "dDurationMs": 2890,
    "segs": [ {
      "utf8": "Orain, sarritan,\njendea"
    } ]
  }, {
    "tStartMs": 3975290,
    "dDurationMs": 1870,
    "segs": [ {
      "utf8": "\nhurrengo egunean zer egin behar duelako estresatzen hasten den unea da"
    } ]
  }, {
    "tStartMs": 3977160,
    "dDurationMs": 1327,
    "segs": [ {
      "utf8": "eta\nlo ezin izateaz kezkatzen da"
    } ]
  }, {
    "tStartMs": 3978487,
    "dDurationMs": 2363,
    "segs": [ {
      "utf8": "eta\ngauza multzo oso batean isur daiteke."
    } ]
  }, {
    "tStartMs": 3980850,
    "dDurationMs": 3890,
    "segs": [ {
      "utf8": "Beraz,\nnire egunean zehar egiten dudan beste gauza bat da badakit"
    } ]
  }, {
    "tStartMs": 3984740,
    "dDurationMs": 1540,
    "segs": [ {
      "utf8": "goizean goiz erne egongo naizela,"
    } ]
  }, {
    "tStartMs": 3986280,
    "dDurationMs": 1640,
    "segs": [ {
      "utf8": "arratsaldean lo samar egongo naizela."
    } ]
  }, {
    "tStartMs": 3987920,
    "dDurationMs": 2240,
    "segs": [ {
      "utf8": "Eta gero arratsaldea iristen den heinean,"
    } ]
  }, {
    "tStartMs": 3990160,
    "dDurationMs": 2380,
    "segs": [ {
      "utf8": "\negiten ari naizen beste gauza guztiak egiteaz gain,"
    } ]
  }, {
    "tStartMs": 3992540,
    "dDurationMs": 4750,
    "segs": [ {
      "utf8": "aurreikusten dut,\njardueran erne gailurra."
    } ]
  }, {
    "tStartMs": 3997290,
    "dDurationMs": 1590,
    "segs": [ {
      "utf8": "Eta ez naiz horregatik kezkatzen."
    } ]
  }, {
    "tStartMs": 3998880,
    "dDurationMs": 2240,
    "segs": [ {
      "utf8": "Agian\nhurrengo egunerako antolatzeko erabiltzen dut,"
    } ]
  }, {
    "tStartMs": 4001120,
    "dDurationMs": 1910,
    "segs": [ {
      "utf8": "baina, funtsean,"
    } ]
  }, {
    "tStartMs": 4003030,
    "dDurationMs": 2260,
    "segs": [ {
      "utf8": "ezer egingo badut,\n"
    } ]
  }, {
    "tStartMs": 4005290,
    "dDurationMs": 3290,
    "segs": [ {
      "utf8": "garbiketa edo\nia esfortzua eskatzen duten gauzak oso egunerokoak izango dira."
    } ]
  }, {
    "tStartMs": 4008580,
    "dDurationMs": 3150,
    "segs": [ {
      "utf8": "Eta ziurrenik\nnire garbiketa gaitasunez ere hitz egiten du."
    } ]
  }, {
    "tStartMs": 4011730,
    "dDurationMs": 1230,
    "segs": [ {
      "utf8": "Baina"
    } ]
  }, {
    "tStartMs": 4012960,
    "dDurationMs": 3770,
    "segs": [ {
      "utf8": "kontua da ez garela\nlotara joaten."
    } ]
  }, {
    "tStartMs": 4016730,
    "dDurationMs": 1990,
    "segs": [ {
      "utf8": "\nLo egin baino lehen ernetasun astindu hau dago,"
    } ]
  }, {
    "tStartMs": 4018720,
    "dDurationMs": 1670,
    "segs": [ {
      "utf8": "kognitiboki jakitea"
    } ]
  }, {
    "tStartMs": 4020390,
    "dDurationMs": 2030,
    "segs": [ {
      "utf8": "jendeari lagungarria izatea espero dut."
    } ]
  }, {
    "tStartMs": 4022420,
    "dDurationMs": 2610,
    "segs": [ {
      "utf8": "Eta horrek planteatzen du,"
    } ]
  }, {
    "tStartMs": 4025030,
    "dDurationMs": 2300,
    "segs": [ {
      "utf8": "nire ustez,\noso garrantzitsua izango den beste gai bat."
    } ]
  }, {
    "tStartMs": 4027330,
    "dDurationMs": 4250,
    "segs": [ {
      "utf8": "Hau da, mekanismo fisiologikoak."
    } ]
  }, {
    "tStartMs": 4031580,
    "dDurationMs": 2210,
    "segs": [ {
      "utf8": "Erne aldaketa hauek bezala."
    } ]
  }, {
    "tStartMs": 4033790,
    "dDurationMs": 1690,
    "segs": [ {
      "utf8": "Edo arnasketa tresnak erabiliz.  Etorkizuneko ataletan"
    } ]
  }, {
    "tStartMs": 4035480,
    "dDurationMs": 1830,
    "segs": [ {
      "utf8": "hitz egingo dugun zerbaiti\nburuz"
    } ]
  }, {
    "tStartMs": 4037310,
    "dDurationMs": 3190,
    "segs": [ {
      "utf8": "gure kitzikapen autonomiko maila aldatzeko."
    } ]
  }, {
    "tStartMs": 4040500,
    "dDurationMs": 3780,
    "segs": [ {
      "utf8": "Horiek fenomeno biologiko konkretuak dira."
    } ]
  }, {
    "tStartMs": 4044280,
    "dDurationMs": 1290,
    "segs": [ {
      "utf8": "Baraualdia ere bai."
    } ]
  }, {
    "tStartMs": 4045570,
    "dDurationMs": 1480,
    "segs": [ {
      "utf8": "Baraualdiak ernetasuna areagotuko du horrela."
    } ]
  }, {
    "tStartMs": 4047050,
    "dDurationMs": 1270,
    "segs": [ {
      "utf8": "Kafeina ere bai."
    } ]
  }, {
    "tStartMs": 4048320,
    "dDurationMs": 1230,
    "segs": [ {
      "utf8": "Denek ez dute"
    } ]
  }, {
    "tStartMs": 4049550,
    "dDurationMs": 1750,
    "segs": [ {
      "utf8": "kafeina jasaten besteen maila berean"
    } ]
  }, {
    "tStartMs": 4051300,
    "dDurationMs": 2100,
    "segs": [ {
      "utf8": "baina, mekanismo fisiologiko bat da."
    } ]
  }, {
    "tStartMs": 4053400,
    "dDurationMs": 850,
    "segs": [ {
      "utf8": "Errezeptoreak ezagutzen ditugu.  Errezeptoreei lotzen zaizkien"
    } ]
  }, {
    "tStartMs": 4054250,
    "dDurationMs": 1550,
    "segs": [ {
      "utf8": "ligandoak deitzen diren bezala ezagutzen ditugu"
    } ]
  }, {
    "tStartMs": 4055800,
    "dDurationMs": 1410,
    "segs": [ {
      "utf8": "."
    } ]
  }, {
    "tStartMs": 4057210,
    "dDurationMs": 1200,
    "segs": [ {
      "utf8": "Mekanismoak ezagutzen ditugu."
    } ]
  }, {
    "tStartMs": 4058410,
    "dDurationMs": 2540,
    "segs": [ {
      "utf8": "Kortisola eta epinefrina hartzen dute parte."
    } ]
  }, {
    "tStartMs": 4060950,
    "dDurationMs": 1230,
    "segs": [ {
      "utf8": "Horiek dira"
    } ]
  }, {
    "tStartMs": 4062180,
    "dDurationMs": 2250,
    "segs": [ {
      "utf8": "pertsonalki saiatzen eta aprobetxatzen ditudan gauza motak,"
    } ]
  }, {
    "tStartMs": 4064430,
    "dDurationMs": 2930,
    "segs": [ {
      "utf8": "\nnire garunaren"
    } ]
  }, {
    "tStartMs": 4067360,
    "dDurationMs": 1510,
    "segs": [ {
      "utf8": "eta nire jardueraren ikaskuntzarako eta optimizaziorako."
    } ]
  }, {
    "tStartMs": 4068870,
    "dDurationMs": 2740,
    "segs": [ {
      "utf8": "Jarduera fisikoa egunean goiz egiteak,\nadibidez,"
    } ]
  }, {
    "tStartMs": 4071610,
    "dDurationMs": 2060,
    "segs": [ {
      "utf8": "iraupen luzeagoa ematen digu,"
    } ]
  }, {
    "tStartMs": 4073670,
    "dDurationMs": 850,
    "segs": [ {
      "utf8": "esnatzeko seinalea,"
    } ]
  }, {
    "tStartMs": 4074520,
    "dDurationMs": 2680,
    "segs": [ {
      "utf8": "\negunean goiz esnatzea bizkortu nahi du."
    } ]
  }, {
    "tStartMs": 4077200,
    "dDurationMs": 2170,
    "segs": [ {
      "utf8": "Horregatik, egunean berandu lan egiteak\nbatzuetan"
    } ]
  }, {
    "tStartMs": 4079370,
    "dDurationMs": 2420,
    "segs": [ {
      "utf8": "\nloak hartzeko arazoak izan ditzake."
    } ]
  }, {
    "tStartMs": 4081790,
    "dDurationMs": 1210,
    "segs": [ {
      "utf8": "Fase atzerapena ere izango du."
    } ]
  }, {
    "tStartMs": 4083000,
    "dDurationMs": 2500,
    "segs": [ {
      "utf8": "\nHurrengo egunean beranduago esnatu nahi baduzu egiten duzu."
    } ]
  }, {
    "tStartMs": 4085500,
    "dDurationMs": 1050,
    "segs": [ {
      "utf8": "Ez da bakarrik nekatuta zaudelako."
    } ]
  }, {
    "tStartMs": 4086550,
    "dDurationMs": 1180,
    "segs": [ {
      "utf8": "Zure erlojua"
    } ]
  }, {
    "tStartMs": 4087730,
    "dDurationMs": 1500,
    "segs": [ {
      "utf8": "jarduerarekin eta tenperaturarekin aldatu duzulako da."
    } ]
  }, {
    "tStartMs": 4090350,
    "dDurationMs": 3900,
    "segs": [ {
      "utf8": "Askok\nplastikotasunerako tresna subjektiboei buruz galdetzen didate."
    } ]
  }, {
    "tStartMs": 4094250,
    "dDurationMs": 2280,
    "segs": [ {
      "utf8": "Zer gertatzen da bistaratzea?"
    } ]
  }, {
    "tStartMs": 4096530,
    "dDurationMs": 3220,
    "segs": [ {
      "utf8": "Imajina al dezakegu\njarduera jakin bat egitea,"
    } ]
  }, {
    "tStartMs": 4099750,
    "dDurationMs": 3780,
    "segs": [ {
      "utf8": "horrek lagunduko al digu\njarduera horretan hobetzen?"
    } ]
  }, {
    "tStartMs": 4103530,
    "dDurationMs": 910,
    "segs": [ {
      "utf8": ""
    } ]
  }, {
    "tStartMs": 4104440,
    "dDurationMs": 2543,
    "segs": [ {
      "utf8": "Ikuspegiak hori egin dezakeen froga batzuk daude, egia da."
    } ]
  }, {
    "tStartMs": 4108180,
    "dDurationMs": 2090,
    "segs": [ {
      "utf8": "Baina hona hemen bereizketa garrantzitsua."
    } ]
  }, {
    "tStartMs": 4110270,
    "dDurationMs": 1140,
    "segs": [ {
      "utf8": "Eta hona hemen zergatik pertsonalki,"
    } ]
  }, {
    "tStartMs": 4111410,
    "dDurationMs": 2530,
    "segs": [ {
      "utf8": "ez dut asko egiten, nahita bistaratze."
    } ]
  }, {
    "tStartMs": 4113940,
    "dDurationMs": 3920,
    "segs": [ {
      "utf8": "Lehenik eta behin, nire gaitasunik onena lortzen dut,"
    } ]
  }, {
    "tStartMs": 4117860,
    "dDurationMs": 2300,
    "segs": [ {
      "utf8": "edo gauzak ikusteko gaitasunik onena lortzen dut\n"
    } ]
  }, {
    "tStartMs": 4120160,
    "dDurationMs": 1410,
    "segs": [ {
      "utf8": "lo-egoera batean nagoenean."
    } ]
  }, {
    "tStartMs": 4121570,
    "dDurationMs": 833,
    "segs": [ {
      "utf8": "Ez dakit zergatik"
    } ]
  }, {
    "tStartMs": 4122403,
    "dDurationMs": 2507,
    "segs": [ {
      "utf8": "baina orduan\ngai naiz, nire burmuina"
    } ]
  }, {
    "tStartMs": 4124910,
    "dDurationMs": 2640,
    "segs": [ {
      "utf8": "\nbegiak itxita barne bisualizaziorantz bideratu."
    } ]
  }, {
    "tStartMs": 4127550,
    "dDurationMs": 1540,
    "segs": [ {
      "utf8": "Eta, oro har, lokartzen naiz"
    } ]
  }, {
    "tStartMs": 4129090,
    "dDurationMs": 1000,
    "segs": [ {
      "utf8": "eta ezin dut gogoratzen"
    } ]
  }, {
    "tStartMs": 4130090,
    "dDurationMs": 1630,
    "segs": [ {
      "utf8": "lehen pentsatzen nuen ezer."
    } ]
  }, {
    "tStartMs": 4131720,
    "dDurationMs": 3160,
    "segs": [ {
      "utf8": "Pertsona batzuk, eta hauek\nduela urte asko"
    } ]
  }, {
    "tStartMs": 4134880,
    "dDurationMs": 2030,
    "segs": [ {
      "utf8": "Roger Shephard-ek eta beste batzuek egin zituzten lanak dira."
    } ]
  }, {
    "tStartMs": 4136910,
    "dDurationMs": 1620,
    "segs": [ {
      "utf8": "Roger Stanforden egon zen, baina"
    } ]
  }, {
    "tStartMs": 4138530,
    "dDurationMs": 2190,
    "segs": [ {
      "utf8": "eta beste laborategi batzuek\nere hori egin dute, noski,"
    } ]
  }, {
    "tStartMs": 4140720,
    "dDurationMs": 2980,
    "segs": [ {
      "utf8": "objektuak\nfisikoki biratzeko euren buruan, kalkulu espazialen"
    } ]
  }, {
    "tStartMs": 4143700,
    "dDurationMs": 3520,
    "segs": [ {
      "utf8": "\nabiadura hobetzeko edo aztertzeko"
    } ]
  }, {
    "tStartMs": 4147220,
    "dDurationMs": 2543,
    "segs": [ {
      "utf8": "eta abar."
    } ]
  }, {
    "tStartMs": 4150890,
    "dDurationMs": 3140,
    "segs": [ {
      "utf8": "Pertsona batzuk oso\nonak dira bistaratzeko."
    } ]
  }, {
    "tStartMs": 4154030,
    "dDurationMs": 2000,
    "segs": [ {
      "utf8": "Begiak itxi ditzakete\neta objektuak besterik gabe ikusi"
    } ]
  }, {
    "tStartMs": 4156030,
    "dDurationMs": 1920,
    "segs": [ {
      "utf8": "eta nahita biratu ditzakete, etab."
    } ]
  }, {
    "tStartMs": 4157950,
    "dDurationMs": 3590,
    "segs": [ {
      "utf8": "Ni bezalako jende asko,\nhori egiten hasten garenean,"
    } ]
  }, {
    "tStartMs": 4161540,
    "dDurationMs": 1950,
    "segs": [ {
      "utf8": "gure gogoa errazegi noraezean dabil."
    } ]
  }, {
    "tStartMs": 4163490,
    "dDurationMs": 2480,
    "segs": [ {
      "utf8": "Baina esna egoeran zentzuz bideratua naizela pentsatzea gustatzen zait\n"
    } ]
  }, {
    "tStartMs": 4165970,
    "dDurationMs": 2660,
    "segs": [ {
      "utf8": "."
    } ]
  }, {
    "tStartMs": 4168630,
    "dDurationMs": 3030,
    "segs": [ {
      "utf8": "Beraz, bistaratzea interesgarria da,"
    } ]
  }, {
    "tStartMs": 4171660,
    "dDurationMs": 2520,
    "segs": [ {
      "utf8": "uste baitut jendea\noso erakartzen duela ideiak, zerbaitetan"
    } ]
  }, {
    "tStartMs": 4174180,
    "dDurationMs": 833,
    "segs": [ {
      "utf8": "pentsa dezakeela"
    } ]
  }, {
    "tStartMs": 4175013,
    "dDurationMs": 2097,
    "segs": [ {
      "utf8": "eta\nhorrela hobetu."
    } ]
  }, {
    "tStartMs": 4177110,
    "dDurationMs": 3990,
    "segs": [ {
      "utf8": "Eta ziurrenik egia da,\n"
    } ]
  }, {
    "tStartMs": 4181100,
    "dDurationMs": 2580,
    "segs": [ {
      "utf8": "gauzak ikusteko moduan oso lineala izan bazara."
    } ]
  }, {
    "tStartMs": 4183680,
    "dDurationMs": 833,
    "segs": [ {
      "utf8": "Beraz, hori errepikatu nahi dut."
    } ]
  }, {
    "tStartMs": 4184513,
    "dDurationMs": 2557,
    "segs": [ {
      "utf8": "Uste dut bistaratzeak\nbaduela nolabaiteko indar."
    } ]
  }, {
    "tStartMs": 4187070,
    "dDurationMs": 2050,
    "segs": [ {
      "utf8": "Oso lineala"
    } ]
  }, {
    "tStartMs": 4189120,
    "dDurationMs": 3350,
    "segs": [ {
      "utf8": "eta nahita eta bideratua izaten jarrai dezakezu\nbistaratzean."
    } ]
  }, {
    "tStartMs": 4192470,
    "dDurationMs": 2760,
    "segs": [ {
      "utf8": "Baina ni bezalako jende askok\nerronka daukana,"
    } ]
  }, {
    "tStartMs": 4195230,
    "dDurationMs": 4130,
    "segs": [ {
      "utf8": "\nbegiak itxita foku lineal hori mantentzea,"
    } ]
  }, {
    "tStartMs": 4199360,
    "dDurationMs": 1750,
    "segs": [ {
      "utf8": "eta bistaratzean,"
    } ]
  }, {
    "tStartMs": 4201110,
    "dDurationMs": 2380,
    "segs": [ {
      "utf8": "ez dute bistaratzeari etekin handirik ateratzen."
    } ]
  }, {
    "tStartMs": 4203490,
    "dDurationMs": 3070,
    "segs": [ {
      "utf8": "Eta\nerrendimenduaren datuek hori onartzen dutela uste dut."
    } ]
  }, {
    "tStartMs": 4206560,
    "dDurationMs": 2470,
    "segs": [ {
      "utf8": "Orain badaude adibideak non, adibidez,"
    } ]
  }, {
    "tStartMs": 4209030,
    "dDurationMs": 1500,
    "segs": [ {
      "utf8": "jendeak gorputz-adarra zaurituko duen"
    } ]
  }, {
    "tStartMs": 4210530,
    "dDurationMs": 3260,
    "segs": [ {
      "utf8": "eta gero\ngorputz-adarra oso-osorik,"
    } ]
  }, {
    "tStartMs": 4213790,
    "dDurationMs": 1840,
    "segs": [ {
      "utf8": "edo zauritu gabeko gorputz-adarra, hobeto esanda."
    } ]
  }, {
    "tStartMs": 4215630,
    "dDurationMs": 2140,
    "segs": [ {
      "utf8": "Eta kontrako gorputz-adarra bistaratuko dute."
    } ]
  }, {
    "tStartMs": 4217770,
    "dDurationMs": 1890,
    "segs": [ {
      "utf8": "Batzuetan\nispilu kutxak ere erabiltzen dira."
    } ]
  }, {
    "tStartMs": 4219660,
    "dDurationMs": 2370,
    "segs": [ {
      "utf8": "Beraz, demagun nire ezkerreko gorputz-adarra zaurituta dagoela.  Nire eskuineko gorputzarekin"
    } ]
  }, {
    "tStartMs": 4222030,
    "dDurationMs": 3120,
    "segs": [ {
      "utf8": "jarduera mantentzen ari naiz,\n"
    } ]
  }, {
    "tStartMs": 4225150,
    "dDurationMs": 1090,
    "segs": [ {
      "utf8": "baina ispilu-kutxa erabiltzen ari naiz,"
    } ]
  }, {
    "tStartMs": 4226240,
    "dDurationMs": 2290,
    "segs": [ {
      "utf8": "beraz, nire ezkerreko\ngorputz-adarra ondo funtzionatzen ari dela ematen du."
    } ]
  }, {
    "tStartMs": 4228530,
    "dDurationMs": 1270,
    "segs": [ {
      "utf8": "Bai, badaude goitik beherako"
    } ]
  }, {
    "tStartMs": 4229800,
    "dDurationMs": 3910,
    "segs": [ {
      "utf8": "edo feedback-mekanismo batzuk,\n"
    } ]
  }, {
    "tStartMs": 4233710,
    "dDurationMs": 3300,
    "segs": [ {
      "utf8": "zauritutako gorputz-adarra azkarrago birgaitu daitekeela\n, eta abar."
    } ]
  }, {
    "tStartMs": 4237010,
    "dDurationMs": 2873,
    "segs": [ {
      "utf8": "Baina, nahiko landutako eskemak dira."
    } ]
  }, {
    "tStartMs": 4240797,
    "dDurationMs": 1753,
    "segs": [ {
      "utf8": "Ez dut nahi ispilu-\nkutxak nire etxe inguruan.  Egoera espezializatuak"
    } ]
  }, {
    "tStartMs": 4242550,
    "dDurationMs": 3370,
    "segs": [ {
      "utf8": "direla uste dut\n.  Albistegietan ikusten ditugun adibideen"
    } ]
  }, {
    "tStartMs": 4245920,
    "dDurationMs": 3020,
    "segs": [ {
      "utf8": "antzekoak dira\n"
    } ]
  }, {
    "tStartMs": 4248940,
    "dDurationMs": 2360,
    "segs": [ {
      "utf8": ", oh,\nhalakoak kolpea duen"
    } ]
  }, {
    "tStartMs": 4251300,
    "dDurationMs": 2293,
    "segs": [ {
      "utf8": "eta gero berez\nhizkuntza berri bat hitz egiten duen."
    } ]
  }, {
    "tStartMs": 4255134,
    "dDurationMs": 2406,
    "segs": [ {
      "utf8": "Ez dakit zein den horren erantzuna."
    } ]
  }, {
    "tStartMs": 4257540,
    "dDurationMs": 2690,
    "segs": [ {
      "utf8": "Garunak\n"
    } ]
  }, {
    "tStartMs": 4260230,
    "dDurationMs": 2780,
    "segs": [ {
      "utf8": "normalean zapaltzen diren\neta askatu daitezkeen sare elkartuak dituela erakusten du."
    } ]
  }, {
    "tStartMs": 4263010,
    "dDurationMs": 1250,
    "segs": [ {
      "utf8": "Baina, zalantzarik gabe, ez duzu nahi kalera irten"
    } ]
  }, {
    "tStartMs": 4264260,
    "dDurationMs": 1600,
    "segs": [ {
      "utf8": "eta zure buruari\nkolpe bat eman nahita"
    } ]
  }, {
    "tStartMs": 4265860,
    "dDurationMs": 2130,
    "segs": [ {
      "utf8": "trebetasun bat asmatzen saiatzeko,"
    } ]
  }, {
    "tStartMs": 4267990,
    "dDurationMs": 3920,
    "segs": [ {
      "utf8": "ez dagoelako\nbide zehatzik hori egiteko modu konkretu batean, trazuaren ondorio kaltegarriak konpentsatu behar dituzula"
    } ]
  }, {
    "tStartMs": 4271910,
    "dDurationMs": 1290,
    "segs": [ {
      "utf8": "benetan jakin ahal izateko."
    } ]
  }, {
    "tStartMs": 4273200,
    "dDurationMs": 860,
    "segs": [ {
      "utf8": ""
    } ]
  }, {
    "tStartMs": 4274060,
    "dDurationMs": 2350,
    "segs": [ {
      "utf8": "."
    } ]
  }, {
    "tStartMs": 4276410,
    "dDurationMs": 1940,
    "segs": [ {
      "utf8": "Izan ere, ideia izugarria izango litzatekeela uste dut."
    } ]
  }, {
    "tStartMs": 4278350,
    "dDurationMs": 4020,
    "segs": [ {
      "utf8": "Beraz, uste dut deskribatzen saiatzen ari naizena\nnola tipikoa den,"
    } ]
  }, {
    "tStartMs": 4282370,
    "dDurationMs": 1990,
    "segs": [ {
      "utf8": "ez dakit tipikoa edo normala naizen.  Esan"
    } ]
  }, {
    "tStartMs": 4284360,
    "dDurationMs": 3720,
    "segs": [ {
      "utf8": "nahi dut, bestela esan didate,\neta, zalantzarik gabe, ez da normala."
    } ]
  }, {
    "tStartMs": 4288080,
    "dDurationMs": 2850,
    "segs": [ {
      "utf8": "Baina\nnire eguna egituratzeko moduari dagokionez,"
    } ]
  }, {
    "tStartMs": 4290930,
    "dDurationMs": 1760,
    "segs": [ {
      "utf8": "hori normala dela uste dut."
    } ]
  }, {
    "tStartMs": 4292690,
    "dDurationMs": 1010,
    "segs": [ {
      "utf8": "Hori nahiko normala da."
    } ]
  }, {
    "tStartMs": 4293700,
    "dDurationMs": 2130,
    "segs": [ {
      "utf8": "Berehala esnatu ohi naiz\n, ez dakit,"
    } ]
  }, {
    "tStartMs": 4295830,
    "dDurationMs": 2220,
    "segs": [ {
      "utf8": "nonbait goizeko 5:30etatik 7:00etara,"
    } ]
  }, {
    "tStartMs": 4298050,
    "dDurationMs": 1480,
    "segs": [ {
      "utf8": "\naurreko gauean egin dudanaren arabera."
    } ]
  }, {
    "tStartMs": 4299530,
    "dDurationMs": 2270,
    "segs": [ {
      "utf8": "\n10:30ak, 11:00ak aldera lotara joan ohi naiz.  Goizean"
    } ]
  }, {
    "tStartMs": 4301800,
    "dDurationMs": 1280,
    "segs": [ {
      "utf8": "sasoi bat izaten dut,"
    } ]
  }, {
    "tStartMs": 4303080,
    "dDurationMs": 2850,
    "segs": [ {
      "utf8": "non\nbenetan zentratuta lan gogorra egin dezakedan"
    } ]
  }, {
    "tStartMs": 4305930,
    "dDurationMs": 3040,
    "segs": [ {
      "utf8": "eta benetan aktibatzeko bidea aktibatu dezaket,"
    } ]
  }, {
    "tStartMs": 4308970,
    "dDurationMs": 2320,
    "segs": [ {
      "utf8": "joan ez den bidea ere aktibatuz,"
    } ]
  }, {
    "tStartMs": 4311290,
    "dDurationMs": 1430,
    "segs": [ {
      "utf8": "benetan kontzentratuta egon nadin."
    } ]
  }, {
    "tStartMs": 4312720,
    "dDurationMs": 1450,
    "segs": [ {
      "utf8": "Baina tresna batzuetan oinarritzen naiz.  Arratsaldean"
    } ]
  }, {
    "tStartMs": 4314170,
    "dDurationMs": 833,
    "segs": [ {
      "utf8": "badaukat aldi bat,"
    } ]
  }, {
    "tStartMs": 4315003,
    "dDurationMs": 2197,
    "segs": [ {
      "utf8": "non\nlo hartzen dudan eta"
    } ]
  }, {
    "tStartMs": 4317200,
    "dDurationMs": 1350,
    "segs": [ {
      "utf8": "jende gehienak uste dudan bezala kanpoan nago."
    } ]
  }, {
    "tStartMs": 4318550,
    "dDurationMs": 4120,
    "segs": [ {
      "utf8": "Eta hortik atera ohi naiz egoera\n"
    } ]
  }, {
    "tStartMs": 4322670,
    "dDurationMs": 2650,
    "segs": [ {
      "utf8": "apur bat loguratsu horrek\nsormen lanetarako"
    } ]
  }, {
    "tStartMs": 4325320,
    "dDurationMs": 2630,
    "segs": [ {
      "utf8": "eta\ngauzak modu berri batean pentsatzeko duen aukera aitortuz."
    } ]
  }, {
    "tStartMs": 4327950,
    "dDurationMs": 1800,
    "segs": [ {
      "utf8": "Egunean pare bat aldiz argitzen naiz."
    } ]
  }, {
    "tStartMs": 4329750,
    "dDurationMs": 1500,
    "segs": [ {
      "utf8": "Karbohidrato gutxien jaten dut egunean zehar,"
    } ]
  }, {
    "tStartMs": 4332331,
    "dDurationMs": 1079,
    "segs": [ {
      "utf8": "eta ez dut altua esaten, baina badakizue,"
    } ]
  }, {
    "tStartMs": 4333410,
    "dDurationMs": 4290,
    "segs": [ {
      "utf8": "karbohidrato altuagoak almidoiak jaten ditut\n, arratsaldean."
    } ]
  }, {
    "tStartMs": 4337700,
    "dDurationMs": 1510,
    "segs": [ {
      "utf8": "Beraz, nolabait lo egin dezaket."
    } ]
  }, {
    "tStartMs": 4339210,
    "dDurationMs": 2740,
    "segs": [ {
      "utf8": "Eta gero, benetan aurreikusten dut"
    } ]
  }, {
    "tStartMs": 4341950,
    "dDurationMs": 2780,
    "segs": [ {
      "utf8": "arratsalde amaierako gailur hori eta erne."
    } ]
  }, {
    "tStartMs": 4344730,
    "dDurationMs": 2580,
    "segs": [ {
      "utf8": "Barkatu, gaueko gailurra eta"
    } ]
  }, {
    "tStartMs": 4347310,
    "dDurationMs": 3170,
    "segs": [ {
      "utf8": "jende askok insomnioagatik"
    } ]
  }, {
    "tStartMs": 4350480,
    "dDurationMs": 3300,
    "segs": [ {
      "utf8": "edo erronkengatik nahasten dituen ernetasuna benetan\nnahiko normalak direnean"
    } ]
  }, {
    "tStartMs": 4353780,
    "dDurationMs": 1850,
    "segs": [ {
      "utf8": "bere ziklo zirkadianoan."
    } ]
  }, {
    "tStartMs": 4355630,
    "dDurationMs": 2300,
    "segs": [ {
      "utf8": "Eta orduan lo hartzen dut."
    } ]
  }, {
    "tStartMs": 4357930,
    "dDurationMs": 1820,
    "segs": [ {
      "utf8": "Eta dena ondo badoa,"
    } ]
  }, {
    "tStartMs": 4359750,
    "dDurationMs": 2310,
    "segs": [ {
      "utf8": "lauzpabost orduz lotan egoten naiz."
    } ]
  }, {
    "tStartMs": 4362060,
    "dDurationMs": 2270,
    "segs": [ {
      "utf8": "Normalean hiru edo\nlau izaten dira eta gero esnatzen naiz."
    } ]
  }, {
    "tStartMs": 4364330,
    "dDurationMs": 1610,
    "segs": [ {
      "utf8": "Uste dut gauaren erdian esnatzen naizen jende gehienak bezalakoa naizela"
    } ]
  }, {
    "tStartMs": 4365940,
    "dDurationMs": 1240,
    "segs": [ {
      "utf8": "."
    } ]
  }, {
    "tStartMs": 4367180,
    "dDurationMs": 2360,
    "segs": [ {
      "utf8": "Orain, uste dut ez denik\nasko eztabaidatu denik"
    } ]
  }, {
    "tStartMs": 4369540,
    "dDurationMs": 1440,
    "segs": [ {
      "utf8": "baina"
    } ]
  }, {
    "tStartMs": 4370980,
    "dDurationMs": 2280,
    "segs": [ {
      "utf8": "Stanfordeko lo-\nlaborategiko nire lankide batek esaten dit,"
    } ]
  }, {
    "tStartMs": 4373260,
    "dDurationMs": 2890,
    "segs": [ {
      "utf8": "ordu eta erdiz behin\n, denok esnatzen garela."
    } ]
  }, {
    "tStartMs": 4376150,
    "dDurationMs": 2180,
    "segs": [ {
      "utf8": "Batzuk ingurura begiratu ere\n, sinetsi ala ez"
    } ]
  }, {
    "tStartMs": 4378330,
    "dDurationMs": 1530,
    "segs": [ {
      "utf8": "eta berehala lotara zoazte."
    } ]
  }, {
    "tStartMs": 4379860,
    "dDurationMs": 1350,
    "segs": [ {
      "utf8": "Eta ez duzu ezagutzen.  Loaldian"
    } ]
  }, {
    "tStartMs": 4381210,
    "dDurationMs": 2990,
    "segs": [ {
      "utf8": "aldian-aldian esnatzea\nohikoa da."
    } ]
  }, {
    "tStartMs": 4384200,
    "dDurationMs": 1050,
    "segs": [ {
      "utf8": "Ez da anormala."
    } ]
  }, {
    "tStartMs": 4385250,
    "dDurationMs": 833,
    "segs": [ {
      "utf8": "Ez dakit"
    } ]
  }, {
    "tStartMs": 4386083,
    "dDurationMs": 2317,
    "segs": [ {
      "utf8": "zergatik ez den hau\nnabarmenago eztabaidatu."
    } ]
  }, {
    "tStartMs": 4388400,
    "dDurationMs": 2700,
    "segs": [ {
      "utf8": "Esnatu ohi naiz eta pertsianetatik\nargi distiratsu bat ateratzen bada"
    } ]
  }, {
    "tStartMs": 4391100,
    "dDurationMs": 3130,
    "segs": [ {
      "utf8": ", edo\ngoiko solairuan zaratarik badago,"
    } ]
  }, {
    "tStartMs": 4394230,
    "dDurationMs": 2060,
    "segs": [ {
      "utf8": "Costelloren zurrungak bereziki ozen bada,"
    } ]
  }, {
    "tStartMs": 4396290,
    "dDurationMs": 3220,
    "segs": [ {
      "utf8": "baliteke jaikitzea, baliteke\nkomunera joan."
    } ]
  }, {
    "tStartMs": 4399510,
    "dDurationMs": 2460,
    "segs": [ {
      "utf8": "Baliteke, badakizu,"
    } ]
  }, {
    "tStartMs": 4401970,
    "dDurationMs": 2200,
    "segs": [ {
      "utf8": "liburu bat hartu eta\nargi gutxiren azpian irakurriko nuke,"
    } ]
  }, {
    "tStartMs": 4404170,
    "dDurationMs": 2920,
    "segs": [ {
      "utf8": "eta gero, oro har,\nloak hartu eta esnatuko naiz,"
    } ]
  }, {
    "tStartMs": 4407090,
    "dDurationMs": 3600,
    "segs": [ {
      "utf8": "niretzako ohiko ordua,\ngoizeko 5:30 edo 7:00.  Gauaren"
    } ]
  }, {
    "tStartMs": 4410690,
    "dDurationMs": 1960,
    "segs": [ {
      "utf8": "erdian esnatze hori\n"
    } ]
  }, {
    "tStartMs": 4412650,
    "dDurationMs": 1060,
    "segs": [ {
      "utf8": ""
    } ]
  }, {
    "tStartMs": 4413710,
    "dDurationMs": 3370,
    "segs": [ {
      "utf8": "gaur podcast atalaren hasieran aipatu dudan bezala,\nez da zertan anormala."
    } ]
  }, {
    "tStartMs": 4417080,
    "dDurationMs": 2240,
    "segs": [ {
      "utf8": "Seguruenik islatzen duena zera"
    } ]
  }, {
    "tStartMs": 4419320,
    "dDurationMs": 3910,
    "segs": [ {
      "utf8": "da: denbora errealeko esanahia, lo\negin behar dudan ordua,"
    } ]
  }, {
    "tStartMs": 4423230,
    "dDurationMs": 2040,
    "segs": [ {
      "utf8": "ziurrenik,\nzortzietatik hurbilago dago."
    } ]
  }, {
    "tStartMs": 4425270,
    "dDurationMs": 3330,
    "segs": [ {
      "utf8": "Gauerdi hitzak literalki\ngauerdia esan nahi zuen."
    } ]
  }, {
    "tStartMs": 4428600,
    "dDurationMs": 3630,
    "segs": [ {
      "utf8": "Guk, askok esan nahi dugu denok\n"
    } ]
  }, {
    "tStartMs": 4432230,
    "dDurationMs": 1900,
    "segs": [ {
      "utf8": "lo egin eta esnatzera joan behar ginela\n,"
    } ]
  }, {
    "tStartMs": 4434130,
    "dDurationMs": 2270,
    "segs": [ {
      "utf8": "eguzkia sartzearekin eta irtetearekin batera, badakizu."
    } ]
  }, {
    "tStartMs": 4436400,
    "dDurationMs": 3000,
    "segs": [ {
      "utf8": "Eta hori dakigu Coloradoko Unibertsitateko\nikerketa eder honek"
    } ]
  }, {
    "tStartMs": 4439400,
    "dDurationMs": 3270,
    "segs": [ {
      "utf8": ",\nnon jendea basamortura atera baitzuten"
    } ]
  }, {
    "tStartMs": 4442670,
    "dDurationMs": 2600,
    "segs": [ {
      "utf8": "\nberen erloju zirkadianoak berrezartzera,"
    } ]
  }, {
    "tStartMs": 4447597,
    "dDurationMs": 1956,
    "segs": [ {
      "utf8": "melatonina eta kortisolaren bidez neurtuta."
    } ]
  }, {
    "tStartMs": 4450690,
    "dDurationMs": 2100,
    "segs": [ {
      "utf8": "\n"
    } ]
  }, {
    "tStartMs": 4452790,
    "dDurationMs": 1640,
    "segs": [ {
      "utf8": "Pantailekin elkarreraginetik eta\nberanduegi egotetik erabat baztertuta zeuden, etab."
    } ]
  }, {
    "tStartMs": 4454430,
    "dDurationMs": 2390,
    "segs": [ {
      "utf8": "Eta, funtsean, arratsaldero\neguzkia ikusi"
    } ]
  }, {
    "tStartMs": 4456820,
    "dDurationMs": 2960,
    "segs": [ {
      "utf8": "eta ilunabarra ikusteko aukera izan zuten\neta ia denek"
    } ]
  }, {
    "tStartMs": 4459780,
    "dDurationMs": 2380,
    "segs": [ {
      "utf8": "ez\nikasle horiek guztiak, baina denak,"
    } ]
  }, {
    "tStartMs": 4462160,
    "dDurationMs": 2230,
    "segs": [ {
      "utf8": "ordutegi batean sartu ziren, non\nnaturalki"
    } ]
  }, {
    "tStartMs": 4464390,
    "dDurationMs": 3230,
    "segs": [ {
      "utf8": "ilunabarrean lo egin\neta egunsentiaren inguruan esnatu nahi zuten."
    } ]
  }, {
    "tStartMs": 4467620,
    "dDurationMs": 2400,
    "segs": [ {
      "utf8": "eguzkia atera baino lehen, nahiz eta\n"
    } ]
  }, {
    "tStartMs": 4470020,
    "dDurationMs": 3100,
    "segs": [ {
      "utf8": "argi artifizial normal batera itzuli zirenean."
    } ]
  }, {
    "tStartMs": 4473120,
    "dDurationMs": 1630,
    "segs": [ {
      "utf8": "Beraz, uste dut hori dela eredu naturala"
    } ]
  }, {
    "tStartMs": 4474750,
    "dDurationMs": 2460,
    "segs": [ {
      "utf8": "eta hortik desbideratu besterik ez dugu egin\nargi artifizialekin."
    } ]
  }, {
    "tStartMs": 4477210,
    "dDurationMs": 3360,
    "segs": [ {
      "utf8": "Beraz, goizeko 03:00etan edo goizeko 4:00etan esnatzeak\nez du zertan esan nahi"
    } ]
  }, {
    "tStartMs": 4480570,
    "dDurationMs": 2716,
    "segs": [ {
      "utf8": "zerbait\nizorratuta dagoenik."
    } ]
  }, {
    "tStartMs": 4483286,
    "dDurationMs": 4084,
    "segs": [ {
      "utf8": "Edo antsietatea edo\nzerbait duzula, agian."
    } ]
  }, {
    "tStartMs": 4487370,
    "dDurationMs": 1130,
    "segs": [ {
      "utf8": "Litekeena da esan nahi duena"
    } ]
  }, {
    "tStartMs": 4488500,
    "dDurationMs": 1800,
    "segs": [ {
      "utf8": "\naskoz lehenago oheratu behar zinela."
    } ]
  }, {
    "tStartMs": 4490300,
    "dDurationMs": 1470,
    "segs": [ {
      "utf8": "Eta nerbio-sistema autonomoaren asimetria hori dela eta,"
    } ]
  }, {
    "tStartMs": 4491770,
    "dDurationMs": 2670,
    "segs": [ {
      "utf8": "\nnon askoz errazagoa zaigun"
    } ]
  }, {
    "tStartMs": 4494440,
    "dDurationMs": 3320,
    "segs": [ {
      "utf8": "bultzatzea eta\nlo egiteko denbora atzeratzea esnatzeko"
    } ]
  }, {
    "tStartMs": 4497760,
    "dDurationMs": 2323,
    "segs": [ {
      "utf8": "ordua bizkortzea baino."
    } ]
  }, {
    "tStartMs": 4501010,
    "dDurationMs": 2060,
    "segs": [ {
      "utf8": "Beste era batera esanda, errazagoa da"
    } ]
  }, {
    "tStartMs": 4503070,
    "dDurationMs": 1280,
    "segs": [ {
      "utf8": "jaian zutik egotea eta bertan"
    } ]
  }, {
    "tStartMs": 4504350,
    "dDurationMs": 1940,
    "segs": [ {
      "utf8": "egon nahi ez baduzu ere,\n"
    } ]
  }, {
    "tStartMs": 4506290,
    "dDurationMs": 2100,
    "segs": [ {
      "utf8": "nekea eta lo azkarra zarenean esnatzea baino."
    } ]
  }, {
    "tStartMs": 4509270,
    "dDurationMs": 3680,
    "segs": [ {
      "utf8": "Jende gehienak aurrera egiten ari dira\n,"
    } ]
  }, {
    "tStartMs": 4512950,
    "dDurationMs": 1080,
    "segs": [ {
      "utf8": "arratsaldeko eta gaueko orduetan eta"
    } ]
  }, {
    "tStartMs": 4514030,
    "dDurationMs": 2600,
    "segs": [ {
      "utf8": "ohera nahi baino askoz beranduago joaten dira\n."
    } ]
  }, {
    "tStartMs": 4516630,
    "dDurationMs": 2780,
    "segs": [ {
      "utf8": "Eta, beraz, pertsonalki ez dut\noheratu nahi 20:00etan."
    } ]
  }, {
    "tStartMs": 4519410,
    "dDurationMs": 4810,
    "segs": [ {
      "utf8": "Gauza on asko gertatzen dira\n20:00etatik 23:00etara."
    } ]
  }, {
    "tStartMs": 4524220,
    "dDurationMs": 1700,
    "segs": [ {
      "utf8": "Eta, beraz, gozatu nahi dut."
    } ]
  }, {
    "tStartMs": 4525920,
    "dDurationMs": 2470,
    "segs": [ {
      "utf8": "Eta arratseko orduak zeharkatzen ditut."
    } ]
  }, {
    "tStartMs": 4528390,
    "dDurationMs": 2820,
    "segs": [ {
      "utf8": "Baina, ondorioz,\nmelatonina gabe geratzen ari naiz."
    } ]
  }, {
    "tStartMs": 4531210,
    "dDurationMs": 2950,
    "segs": [ {
      "utf8": "Nire melatonina askatzea funtsean"
    } ]
  }, {
    "tStartMs": 4534160,
    "dDurationMs": 1080,
    "segs": [ {
      "utf8": "3:00 edo 4:00etan gutxitu da"
    } ]
  }, {
    "tStartMs": 4535240,
    "dDurationMs": 2060,
    "segs": [ {
      "utf8": "eta, beraz, zentzuzkoa da\nesnatzea."
    } ]
  }, {
    "tStartMs": 4537300,
    "dDurationMs": 1930,
    "segs": [ {
      "utf8": "Ez dut melatonina hartzen\n"
    } ]
  }, {
    "tStartMs": 4539230,
    "dDurationMs": 1940,
    "segs": [ {
      "utf8": "aurreko ataletan azaldutako arrazoiengatik."
    } ]
  }, {
    "tStartMs": 4541170,
    "dDurationMs": 3060,
    "segs": [ {
      "utf8": "\nMagnesio glizinatoa"
    } ]
  }, {
    "tStartMs": 4544230,
    "dDurationMs": 2948,
    "segs": [ {
      "utf8": "edo magnesio treonatoa bezalako gauzetan oinarritzen naiz,\nteanina bezalako gauzetan."
    } ]
  }, {
    "tStartMs": 4547178,
    "dDurationMs": 1722,
    "segs": [ {
      "utf8": "Ez dut esaten zuetako inork\nhartu behar dituenik."
    } ]
  }, {
    "tStartMs": 4548900,
    "dDurationMs": 1310,
    "segs": [ {
      "utf8": "Horixe hartu nuen"
    } ]
  }, {
    "tStartMs": 4550210,
    "dDurationMs": 1530,
    "segs": [ {
      "utf8": "loa errazteko."
    } ]
  }, {
    "tStartMs": 4551740,
    "dDurationMs": 2053,
    "segs": [ {
      "utf8": "Eta onura handia izan da niretzat."
    } ]
  }, {
    "tStartMs": 4554940,
    "dDurationMs": 1250,
    "segs": [ {
      "utf8": "Gauaren erdian esnatzen banaiz"
    } ]
  }, {
    "tStartMs": 4556190,
    "dDurationMs": 2870,
    "segs": [ {
      "utf8": "eta edozein arrazoirengatik kezkatuta nago\nedo nire gogoa begiztatuta badago,"
    } ]
  }, {
    "tStartMs": 4559060,
    "dDurationMs": 1300,
    "segs": [ {
      "utf8": "arau pare bat ditut."
    } ]
  }, {
    "tStartMs": 4560360,
    "dDurationMs": 3570,
    "segs": [ {
      "utf8": "Bata da, ez naizela\npentsatzen dudan ezertaz fidatzen.  Gau"
    } ]
  }, {
    "tStartMs": 4563930,
    "dDurationMs": 1960,
    "segs": [ {
      "utf8": "erdian esnatzen naizenean\n, edozein."
    } ]
  }, {
    "tStartMs": 4565890,
    "dDurationMs": 1410,
    "segs": [ {
      "utf8": "Amets zoragarri bat izan ezean"
    } ]
  }, {
    "tStartMs": 4567300,
    "dDurationMs": 1050,
    "segs": [ {
      "utf8": "eta idatzi nahi ez badut,"
    } ]
  }, {
    "tStartMs": 4568350,
    "dDurationMs": 1610,
    "segs": [ {
      "utf8": "noizean behin hori egingo dut."
    } ]
  }, {
    "tStartMs": 4569960,
    "dDurationMs": 1310,
    "segs": [ {
      "utf8": "Normalean itzuli eta irakurtzen dudanean,"
    } ]
  }, {
    "tStartMs": 4571270,
    "dDurationMs": 1913,
    "segs": [ {
      "utf8": "ez da batere zoragarria."
    } ]
  }, {
    "tStartMs": 4574690,
    "dDurationMs": 1540,
    "segs": [ {
      "utf8": "Ezin dut inoiz gogoratzen"
    } ]
  }, {
    "tStartMs": 4576230,
    "dDurationMs": 2100,
    "segs": [ {
      "utf8": ""
    } ]
  }, {
    "tStartMs": 4578330,
    "dDurationMs": 3710,
    "segs": [ {
      "utf8": "nire ametsetan itsatsi zitzaidan\nedo inplementatu nuen ezer fantastikorik asmatu nuenik."
    } ]
  }, {
    "tStartMs": 4582040,
    "dDurationMs": 2060,
    "segs": [ {
      "utf8": "Ez naiz benetan niri ziklo zirkadianoko ordu txiki horietan\ngertatzen den pentsamendu motaz fio"
    } ]
  }, {
    "tStartMs": 4584100,
    "dDurationMs": 3000,
    "segs": [ {
      "utf8": "\n."
    } ]
  }, {
    "tStartMs": 4587100,
    "dDurationMs": 3350,
    "segs": [ {
      "utf8": "Ez dago ezer ez\nniretzat izugarri sormenezkorik,"
    } ]
  }, {
    "tStartMs": 4590450,
    "dDurationMs": 3010,
    "segs": [ {
      "utf8": "ezta inplementazio lineala merezi duenik ere\n."
    } ]
  }, {
    "tStartMs": 4593460,
    "dDurationMs": 2070,
    "segs": [ {
      "utf8": "Baina, oso lagungarria izan den gauza bat"
    } ]
  }, {
    "tStartMs": 4595530,
    "dDurationMs": 1330,
    "segs": [ {
      "utf8": "da batzuetan"
    } ]
  }, {
    "tStartMs": 4596860,
    "dDurationMs": 2150,
    "segs": [ {
      "utf8": "lorik gabeko atseden sakoneko protokolo horietako bat egitea"
    } ]
  }, {
    "tStartMs": 4599010,
    "dDurationMs": 2000,
    "segs": [ {
      "utf8": "lotara itzultzeko modu gisa."
    } ]
  }, {
    "tStartMs": 4601010,
    "dDurationMs": 2760,
    "segs": [ {
      "utf8": "Beraz, hipnosi aplikazio bat edo lehen aipatu ditudan"
    } ]
  }, {
    "tStartMs": 4603770,
    "dDurationMs": 2570,
    "segs": [ {
      "utf8": "Michael Sealeyren gidoi batzuk\n,"
    } ]
  }, {
    "tStartMs": 4606340,
    "dDurationMs": 4100,
    "segs": [ {
      "utf8": "edo Reveri Health, edo\nyoga nidra protokolo bat."
    } ]
  }, {
    "tStartMs": 4610440,
    "dDurationMs": 4060,
    "segs": [ {
      "utf8": "Niretzat oso\nerabilgarriak izan dira itzaltzen laguntzeko, gauaren"
    } ]
  }, {
    "tStartMs": 4614500,
    "dDurationMs": 1910,
    "segs": [ {
      "utf8": "erdian pentsatzen\n"
    } ]
  }, {
    "tStartMs": 4616410,
    "dDurationMs": 1960,
    "segs": [ {
      "utf8": "eta loak hartzen laguntzeko."
    } ]
  }, {
    "tStartMs": 4618370,
    "dDurationMs": 2820,
    "segs": [ {
      "utf8": "Nire ordutegia berrikusterakoan,"
    } ]
  }, {
    "tStartMs": 4621190,
    "dDurationMs": 2880,
    "segs": [ {
      "utf8": "\n"
    } ]
  }, {
    "tStartMs": 4624070,
    "dDurationMs": 2600,
    "segs": [ {
      "utf8": "ikaskuntza optimizatzeko zenbait tresna mota ezartzeko testuinguru gisa."
    } ]
  }, {
    "tStartMs": 4626670,
    "dDurationMs": 1800,
    "segs": [ {
      "utf8": "Konturatu, goizean"
    } ]
  }, {
    "tStartMs": 4628470,
    "dDurationMs": 1940,
    "segs": [ {
      "utf8": "90 minutuko ikaskuntza"
    } ]
  }, {
    "tStartMs": 4630410,
    "dDurationMs": 2590,
    "segs": [ {
      "utf8": "eta lan saioa dagoela ematen du\neta arratsaldean 90 minutuko"
    } ]
  }, {
    "tStartMs": 4633000,
    "dDurationMs": 2720,
    "segs": [ {
      "utf8": "sormen motako lan bat dagoela\neta kito."
    } ]
  }, {
    "tStartMs": 4635720,
    "dDurationMs": 1730,
    "segs": [ {
      "utf8": "Ordu asko daude\ntartean noski,"
    } ]
  }, {
    "tStartMs": 4637450,
    "dDurationMs": 1740,
    "segs": [ {
      "utf8": "eta oso argi izan nahi dut."
    } ]
  }, {
    "tStartMs": 4639190,
    "dDurationMs": 3810,
    "segs": [ {
      "utf8": "Niretzat ordu horiek\nzeregin nahiko ez arruntak,"
    } ]
  }, {
    "tStartMs": 4643000,
    "dDurationMs": 2890,
    "segs": [ {
      "utf8": "baina ausazkoak diren gauzek hartzen dituzte."
    } ]
  }, {
    "tStartMs": 4645890,
    "dDurationMs": 3140,
    "segs": [ {
      "utf8": "Posta elektronikoa\nedo Zoom bileretara joatea"
    } ]
  }, {
    "tStartMs": 4649030,
    "dDurationMs": 2710,
    "segs": [ {
      "utf8": "edo lankideekin eta ikasleekin biltzea"
    } ]
  }, {
    "tStartMs": 4651740,
    "dDurationMs": 1543,
    "segs": [ {
      "utf8": "eta horrelako gauzak dira."
    } ]
  }, {
    "tStartMs": 4654120,
    "dDurationMs": 3040,
    "segs": [ {
      "utf8": "Batzuetan irakurriko dut\nnire aberasteagatik."
    } ]
  }, {
    "tStartMs": 4657160,
    "dDurationMs": 1850,
    "segs": [ {
      "utf8": "90 minutuko bi saio horiek aipatu ditut,"
    } ]
  }, {
    "tStartMs": 4659010,
    "dDurationMs": 1940,
    "segs": [ {
      "utf8": "90 minutuko bi saio horiek direlako"
    } ]
  }, {
    "tStartMs": 4660950,
    "dDurationMs": 2660,
    "segs": [ {
      "utf8": "zabaltzen saiatzen ari naizen,"
    } ]
  }, {
    "tStartMs": 4663610,
    "dDurationMs": 2420,
    "segs": [ {
      "utf8": "lehendik ditudan gaitasun mentalak."
    } ]
  }, {
    "tStartMs": 4666030,
