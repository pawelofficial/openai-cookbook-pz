    "segs": [ {
      "utf8": "Berriro ere, hori getmadefor.com da,"
    } ]
  }, {
    "tStartMs": 286300,
    "dDurationMs": 1410,
    "segs": [ {
      "utf8": "jarri Huberman ordaintzerakoan,"
    } ]
  }, {
    "tStartMs": 287710,
    "dDurationMs": 2300,
    "segs": [ {
      "utf8": "eta\n% 20ko deskontua lortzeko aukera izango duzu, baita"
    } ]
  }, {
    "tStartMs": 290010,
    "dDurationMs": 2970,
    "segs": [ {
      "utf8": "\ngurekin hileroko Zoom deietarako sarbidea ere."
    } ]
  }, {
    "tStartMs": 292980,
    "dDurationMs": 2560,
    "segs": [ {
      "utf8": "Hitz egin dezagun neuroplastikotasunaz."
    } ]
  }, {
    "tStartMs": 295540,
    "dDurationMs": 2810,
    "segs": [ {
      "utf8": "Zehazkiago, hitz egin dezagun nola"
    } ]
  }, {
    "tStartMs": 298350,
    "dDurationMs": 2520,
    "segs": [ {
      "utf8": "optimizatu dezakegun gure garuna."
    } ]
  }, {
    "tStartMs": 300870,
    "dDurationMs": 2710,
    "segs": [ {
      "utf8": "Neuroplastikotasuna gure nerbio-sistemaren ezaugarri ikaragarri hau da,"
    } ]
  }, {
    "tStartMs": 303580,
    "dDurationMs": 1720,
    "segs": [ {
      "utf8": ""
    } ]
  }, {
    "tStartMs": 305300,
    "dDurationMs": 5000,
    "segs": [ {
      "utf8": "bera aldatzeko aukera ematen duena, baita\nguk kontzienteki erabakitzen dugun moduan ere."
    } ]
  }, {
    "tStartMs": 310350,
    "dDurationMs": 1750,
    "segs": [ {
      "utf8": "Hori jabetza sinestezina da."
    } ]
  }, {
    "tStartMs": 312100,
    "dDurationMs": 2800,
    "segs": [ {
      "utf8": "Gure gibelak ezin du\nbere burua aldatzea erabaki."
    } ]
  }, {
    "tStartMs": 314900,
    "dDurationMs": 2830,
    "segs": [ {
      "utf8": "Gure bareak ezin du erabaki\nbere burua aldatzea,"
    } ]
  }, {
    "tStartMs": 317730,
    "dDurationMs": 1300,
    "segs": [ {
      "utf8": "pentsamendu kontzientearen bidez"
    } ]
  }, {
    "tStartMs": 320109,
    "dDurationMs": 2171,
    "segs": [ {
      "utf8": "edo beste pertsona baten iritziaren bidez."
    } ]
  }, {
    "tStartMs": 322280,
    "dDurationMs": 2940,
    "segs": [ {
      "utf8": "Ehun horietako zelulek\naldaketak egin ditzakete ziur,"
    } ]
  }, {
    "tStartMs": 325220,
    "dDurationMs": 1690,
    "segs": [ {
      "utf8": "baina gure nerbio-sistema da bere aldaketak zuzentzeko"
    } ]
  }, {
    "tStartMs": 326910,
    "dDurationMs": 2490,
    "segs": [ {
      "utf8": "gaitasun ikaragarri hori gordetzen duena"
    } ]
  }, {
    "tStartMs": 329400,
    "dDurationMs": 2770,
    "segs": [ {
      "utf8": ","
    } ]
  }, {
    "tStartMs": 332170,
    "dDurationMs": 5000,
    "segs": [ {
      "utf8": "\nhobeto balioko digula uste dugun edo esaten diguten moduan."
    } ]
  }, {
    "tStartMs": 337760,
    "dDurationMs": 2880,
    "segs": [ {
      "utf8": "Orain, gaurko\natal berezia da, izan ere,"
    } ]
  }, {
    "tStartMs": 340640,
    "dDurationMs": 2210,
    "segs": [ {
      "utf8": "bitartean zientziari buruz hitz egingo dugu"
    } ]
  }, {
    "tStartMs": 342850,
    "dDurationMs": 2773,
    "segs": [ {
      "utf8": "eta beti bezala\nmekanismoan sakonduko dugulako."
    } ]
  }, {
    "tStartMs": 346630,
    "dDurationMs": 1670,
    "segs": [ {
      "utf8": "Gaurko atala benetan"
    } ]
  }, {
    "tStartMs": 348300,
    "dDurationMs": 2450,
    "segs": [ {
      "utf8": "\n"
    } ]
  }, {
    "tStartMs": 350750,
    "dDurationMs": 2720,
    "segs": [ {
      "utf8": "neuroplastikotasuna aprobetxatzeari buruzko zure galdera ohikoenak erantzutera zuzenduta dago."
    } ]
  }, {
    "tStartMs": 353470,
    "dDurationMs": 2450,
    "segs": [ {
      "utf8": "Aurreko pasarteak fokuari buruzkoak ziren,"
    } ]
  }, {
    "tStartMs": 355920,
    "dDurationMs": 2990,
    "segs": [ {
      "utf8": "eta fokua nola lortu\nplastikotasunaren mesedetan."
    } ]
  }, {
    "tStartMs": 358910,
    "dDurationMs": 1980,
    "segs": [ {
      "utf8": "Baita azken atala, hau da,"
    } ]
  }, {
    "tStartMs": 360890,
    "dDurationMs": 833,
    "segs": [ {
      "utf8": "zeintzuk diren"
    } ]
  }, {
    "tStartMs": 361723,
    "dDurationMs": 2517,
    "segs": [ {
      "utf8": "\nmugimenduarekin zerikusia duten plastikotasunaren atarietako batzuk,"
    } ]
  }, {
    "tStartMs": 364240,
    "dDurationMs": 2360,
    "segs": [ {
      "utf8": "jokabideak nola aktibatu dezakeen plastikotasuna,"
    } ]
  }, {
    "tStartMs": 366600,
    "dDurationMs": 3500,
    "segs": [ {
      "utf8": "baita nola aktibatzeko\nplastikotasuna portaerarentzat bera."
    } ]
  }, {
    "tStartMs": 370100,
    "dDurationMs": 2860,
    "segs": [ {
      "utf8": "Nola hobetu\nzenbait mugimendu ikasten."
    } ]
  }, {
    "tStartMs": 372960,
    "dDurationMs": 2230,
    "segs": [ {
      "utf8": "Gaurko podcasta"
    } ]
  }, {
    "tStartMs": 375190,
    "dDurationMs": 2790,
    "segs": [ {
      "utf8": "zure\ngaldera ohikoenak erantzutera zuzenduta dago,"
    } ]
  }, {
    "tStartMs": 377980,
    "dDurationMs": 1460,
    "segs": [ {
      "utf8": "eta gai handiagoari,"
    } ]
  }, {
    "tStartMs": 379440,
    "dDurationMs": 2830,
    "segs": [ {
      "utf8": "nola egiten zaio\nbere garuna optimizatzeari"
    } ]
  }, {
    "tStartMs": 382270,
    "dDurationMs": 2280,
    "segs": [ {
      "utf8": "edo, are gehiago, garuna optimizatzeari buruz?"
    } ]
  }, {
    "tStartMs": 384550,
    "dDurationMs": 1590,
    "segs": [ {
      "utf8": "Zer da garuna optimizatzea deitzen dugun gauza hori"
    } ]
  }, {
    "tStartMs": 386140,
    "dDurationMs": 1750,
    "segs": [ {
      "utf8": "?"
    } ]
  }, {
    "tStartMs": 387890,
    "dDurationMs": 2720,
    "segs": [ {
      "utf8": "Hori eginez gero,\nnire"
    } ]
  }, {
    "tStartMs": 390610,
    "dDurationMs": 2040,
    "segs": [ {
      "utf8": "ohiko errutina eta tresna batzuk ere partekatuko ditut."
    } ]
  }, {
    "tStartMs": 392650,
    "dDurationMs": 943,
    "segs": [ {
      "utf8": "Ez ditut partekatzen hauek"
    } ]
  }, {
    "tStartMs": 393593,
    "dDurationMs": 2307,
    "segs": [ {
      "utf8": "\neskuragarri dauden bakarrak direla uste dudalako."
    } ]
  }, {
    "tStartMs": 395900,
    "dDurationMs": 1110,
    "segs": [ {
      "utf8": "Zalantzarik gabe, ez dira."
    } ]
  }, {
    "tStartMs": 397010,
    "dDurationMs": 833,
    "segs": [ {
      "utf8": "Ez ditut partekatzen,"
    } ]
  }, {
    "tStartMs": 397843,
    "dDurationMs": 1517,
    "segs": [ {
      "utf8": "uste dudalako\nguztiek egin beharko lituzketela nik"
    } ]
  }, {
    "tStartMs": 399360,
    "dDurationMs": 2680,
    "segs": [ {
      "utf8": "egiten ditudalako, zalantzarik gabe."
    } ]
  }, {
    "tStartMs": 402040,
    "dDurationMs": 2140,
    "segs": [ {
      "utf8": "Partekatzen ditut zuetako askok"
    } ]
  }, {
    "tStartMs": 404180,
    "dDurationMs": 4560,
    "segs": [ {
      "utf8": "\nzer eta noiz egiten dudanaren adibide oso zehatzak eskatu dituzuelako,"
    } ]
  }, {
    "tStartMs": 408740,
    "dDurationMs": 1310,
    "segs": [ {
      "utf8": "eta, beraz, zuekin partekatuko ditut"
    } ]
  }, {
    "tStartMs": 410050,
    "dDurationMs": 1960,
    "segs": [ {
      "utf8": "eta erabaki dezakezue\nprotokolo horiek"
    } ]
  }, {
    "tStartMs": 412010,
    "dDurationMs": 1540,
    "segs": [ {
      "utf8": "zuretzat diren ala ez."
    } ]
  }, {
    "tStartMs": 413550,
    "dDurationMs": 3260,
    "segs": [ {
      "utf8": "Denak desberdinak dira, baina\nbadaude ezaugarri komun batzuk,"
    } ]
  }, {
    "tStartMs": 416810,
    "dDurationMs": 2210,
    "segs": [ {
      "utf8": "denok elkartzen garen,"
    } ]
  }, {
    "tStartMs": 419020,
    "dDurationMs": 2230,
    "segs": [ {
      "utf8": "\nnerbio-sistema eta gorputzaren mailan,"
    } ]
  }, {
    "tStartMs": 421250,
    "dDurationMs": 1490,
    "segs": [ {
      "utf8": ""
    } ]
  }, {
    "tStartMs": 422740,
    "dDurationMs": 2690,
    "segs": [ {
      "utf8": "praktika jakinetara zuzentzen gaituztenak,\n"
    } ]
  }, {
    "tStartMs": 425430,
    "dDurationMs": 3580,
    "segs": [ {
      "utf8": "bereziki\nindartsuak izan daitezkeen errutina partikularretara, neuroplastikotasunerako."
    } ]
  }, {
    "tStartMs": 429010,
    "dDurationMs": 3180,
    "segs": [ {
      "utf8": "Beraz, gaur eztabaida ireki nahi dut, funtsezkoa den"
    } ]
  }, {
    "tStartMs": 432190,
    "dDurationMs": 3500,
    "segs": [ {
      "utf8": "zerbait azpimarratuz\n."
    } ]
  }, {
    "tStartMs": 435690,
    "dDurationMs": 3660,
    "segs": [ {
      "utf8": "Hau da plastikotasuna, ez da helburua."
    } ]
  }, {
    "tStartMs": 439350,
    "dDurationMs": 2390,
    "segs": [ {
      "utf8": "Plastikotasuna ez da inoiz helburua."
    } ]
  }, {
    "tStartMs": 441740,
    "dDurationMs": 2640,
    "segs": [ {
      "utf8": "Plastikotasuna"
    } ]
  }, {
    "tStartMs": 444380,
    "dDurationMs": 3290,
    "segs": [ {
      "utf8": "gure\nnerbio-sistema aldatzeko egoera bat edo ahalmen bat besterik ez da."
    } ]
  }, {
    "tStartMs": 447670,
    "dDurationMs": 2670,
    "segs": [ {
      "utf8": "Eta, beraz, ezerk ez nau gehiago frustratzen,"
    } ]
  }, {
    "tStartMs": 450340,
    "dDurationMs": 3280,
    "segs": [ {
      "utf8": "agian orduan entzuten dudanean,\noh, badakizu, pilula honek,"
    } ]
  }, {
    "tStartMs": 453620,
    "dDurationMs": 4740,
    "segs": [ {
      "utf8": "edabe honek, praktika honek\nplastikotasuna ematen dizu."
    } ]
  }, {
    "tStartMs": 458360,
    "dDurationMs": 2200,
    "segs": [ {
      "utf8": "Plastikotasuna aldaketa besterik ez da."
    } ]
  }, {
    "tStartMs": 460560,
    "dDurationMs": 2220,
    "segs": [ {
      "utf8": "Benetako galdera da zer\naldatzen saiatzen ari zara?"
    } ]
  }, {
    "tStartMs": 462780,
    "dDurationMs": 3950,
    "segs": [ {
      "utf8": "Eta zehazki, zein helburu\nlortzen saiatzen ari zara?"
    } ]
  }, {
    "tStartMs": 466730,
    "dDurationMs": 3800,
    "segs": [ {
      "utf8": "Azken helburu zehatzak\noso zehatzak izan daitezke."
    } ]
  }, {
    "tStartMs": 470530,
    "dDurationMs": 3120,
    "segs": [ {
      "utf8": "Esaterako, hizkuntza jakin bat hitz egiten ikasi nahi baduzu\n"
    } ]
  }, {
    "tStartMs": 473650,
    "dDurationMs": 2230,
    "segs": [ {
      "utf8": "edo trebetasun motor berri bat ikasi nahi baduzu"
    } ]
  }, {
    "tStartMs": 475880,
    "dDurationMs": 2220,
    "segs": [ {
      "utf8": "edo kalkuluan oso ona izan nahi baduzu,"
    } ]
  }, {
    "tStartMs": 478100,
    "dDurationMs": 3090,
    "segs": [ {
      "utf8": "edo\n"
    } ]
  }, {
    "tStartMs": 481190,
    "dDurationMs": 3350,
    "segs": [ {
      "utf8": "gizaki edo esperientzia jakin bati lotutako emozio txarrak ahaztu nahi dituzu."
    } ]
  }, {
    "tStartMs": 484540,
    "dDurationMs": 1220,
    "segs": [ {
      "utf8": "edo orokorragoa izan daiteke."
    } ]
  }, {
    "tStartMs": 485760,
    "dDurationMs": 2550,
    "segs": [ {
      "utf8": "Sormen handiagoa izan nahiko zenukeen bezala."
    } ]
  }, {
    "tStartMs": 488310,
    "dDurationMs": 3060,
    "segs": [ {
      "utf8": "Eta sormenari buruz hitz egingo dugu\ngaur."
    } ]
  }, {
    "tStartMs": 491370,
    "dDurationMs": 2130,
    "segs": [ {
      "utf8": "Edo arreta gehiago lortu nahiko zenuke"
    } ]
  }, {
    "tStartMs": 493500,
    "dDurationMs": 1790,
    "segs": [ {
      "utf8": "edo gutxiago estresatu nahi zenuke."
    } ]
  }, {
    "tStartMs": 495290,
    "dDurationMs": 4480,
    "segs": [ {
      "utf8": "Beraz, oso garrantzitsua da,\nplastikotasuna hori ulertzea,"
    } ]
  }, {
    "tStartMs": 499770,
    "dDurationMs": 3950,
    "segs": [ {
      "utf8": "eta plastikotasuna lortzea\nda lehen urratsa,"
    } ]
  }, {
    "tStartMs": 503720,
    "dDurationMs": 2680,
    "segs": [ {
      "utf8": "zure garuna optimizatzea deitzen dugun horretan."
    } ]
  }, {
    "tStartMs": 506400,
    "dDurationMs": 2200,
    "segs": [ {
      "utf8": "Ez duzu nahi zure garuna\nplastikoa izatea denbora guztian."
    } ]
  }, {
    "tStartMs": 508600,
    "dDurationMs": 2340,
    "segs": [ {
      "utf8": "Izan ere, galdera nagusietako bat, neurozientziaren"
    } ]
  }, {
    "tStartMs": 510940,
    "dDurationMs": 2420,
    "segs": [ {
      "utf8": "argitu gabeko misterio nagusietako bat\n"
    } ]
  }, {
    "tStartMs": 513360,
    "dDurationMs": 2960,
    "segs": [ {
      "utf8": "da gutariko bakoitza nola\nesnatzen garen egunero,"
    } ]
  }, {
    "tStartMs": 516320,
    "dDurationMs": 2280,
    "segs": [ {
      "utf8": "eta dakien nor garen."
    } ]
  }, {
    "tStartMs": 518600,
    "dDurationMs": 1420,
    "segs": [ {
      "utf8": "Zergatik izan behar da hori?"
    } ]
  }, {
    "tStartMs": 520020,
    "dDurationMs": 1740,
    "segs": [ {
      "utf8": "Beno, garuna plastikozkoa da.  Bizitza osoan zehar"
    } ]
  }, {
    "tStartMs": 521760,
    "dDurationMs": 2330,
    "segs": [ {
      "utf8": "aldatzeko ahalmena du\n"
    } ]
  }, {
    "tStartMs": 524090,
    "dDurationMs": 2370,
    "segs": [ {
      "utf8": "baina ez da hain plastikoa, gauero"
    } ]
  }, {
    "tStartMs": 526460,
    "dDurationMs": 2260,
    "segs": [ {
      "utf8": "lotara goazenean, edo esnatzean,"
    } ]
  }, {
    "tStartMs": 528720,
    "dDurationMs": 2150,
    "segs": [ {
      "utf8": "konexioak\nhainbeste birkonfiguratzen direla,"
    } ]
  }, {
    "tStartMs": 530870,
    "dDurationMs": 2953,
    "segs": [ {
      "utf8": "zeintzuk garen, edo\nnola ibili, edo nola ahazten baitigu.  jan."
    } ]
  }, {
    "tStartMs": 535090,
    "dDurationMs": 2130,
    "segs": [ {
      "utf8": "Gauza ona da ez daukaguna,"
    } ]
  }, {
    "tStartMs": 537220,
    "dDurationMs": 2970,
    "segs": [ {
      "utf8": "halako plastikotasun sendoa\nedo etengabeko plastikotasuna,"
    } ]
  }, {
    "tStartMs": 540190,
    "dDurationMs": 2080,
    "segs": [ {
      "utf8": "\negunero gure burua berregituratu behar izatea."
    } ]
  }, {
    "tStartMs": 542270,
    "dDurationMs": 2230,
    "segs": [ {
      "utf8": "\nGure bizitzari jarraipena ematen dionaren parte da."
    } ]
  }, {
    "tStartMs": 544500,
    "dDurationMs": 3840,
    "segs": [ {
      "utf8": "Beraz, gogoratu, plastikotasuna\nez da eta ez da inoiz helburua."
    } ]
  }, {
    "tStartMs": 548340,
    "dDurationMs": 4220,
    "segs": [ {
      "utf8": "Helburua\nplastikotasuna nola atzitu asmatzea da,"
    } ]
  }, {
    "tStartMs": 552560,
    "dDurationMs": 4310,
    "segs": [ {
      "utf8": "eta gero plastikotasun hori zuzentzea\n,"
    } ]
  }, {
    "tStartMs": 556870,
    "dDurationMs": 2670,
    "segs": [ {
      "utf8": "lortu nahi dituzun helburu edo aldaketa zehatzetara."
    } ]
  }, {
    "tStartMs": 559540,
    "dDurationMs": 1500,
    "segs": [ {
      "utf8": "Eta aipatu behar dut,"
    } ]
  }, {
    "tStartMs": 561040,
    "dDurationMs": 2870,
    "segs": [ {
      "utf8": "ez dago araurik bizitzan"
    } ]
  }, {
    "tStartMs": 563910,
    "dDurationMs": 2660,
    "segs": [ {
      "utf8": "\nneuro-plastikotasuna deritzon gauza ikaragarri hau aprobetxatu behar duzula."
    } ]
  }, {
    "tStartMs": 566570,
    "dDurationMs": 1590,
    "segs": [ {
      "utf8": "Inork ez zuen esan hori egin behar zenik."
    } ]
  }, {
    "tStartMs": 568160,
    "dDurationMs": 3260,
    "segs": [ {
      "utf8": "Atal honetako podcast hau\nbereziki"
    } ]
  }, {
    "tStartMs": 571420,
    "dDurationMs": 3650,
    "segs": [ {
      "utf8": "\n"
    } ]
  }, {
    "tStartMs": 575070,
    "dDurationMs": 1990,
    "segs": [ {
      "utf8": "bizitzako alderdi jakin batekin pozik edo pozik dauden pertsonentzat da."
    } ]
  }, {
    "tStartMs": 577060,
    "dDurationMs": 2780,
    "segs": [ {
      "utf8": "Eta aldatu nahi dute,\nmodu positiboan."
    } ]
  }, {
    "tStartMs": 579840,
    "dDurationMs": 1150,
    "segs": [ {
      "utf8": "Eta entzuten ari zareten askok esango lukete,"
    } ]
  }, {
    "tStartMs": 580990,
    "dDurationMs": 1113,
    "segs": [ {
      "utf8": "denek ez al lukete hori egin nahi?"
    } ]
  }, {
    "tStartMs": 582103,
    "dDurationMs": 1847,
    "segs": [ {
      "utf8": "Beno, egia esan, badaude jende kopuru jakin bat"
    } ]
  }, {
    "tStartMs": 583950,
    "dDurationMs": 1520,
    "segs": [ {
      "utf8": "nahiko\nona dagoen tokian"
    } ]
  }, {
    "tStartMs": 585470,
    "dDurationMs": 1690,
    "segs": [ {
      "utf8": "eta ez dute\naldatu nahi eta hori izugarria da."
    } ]
  }, {
    "tStartMs": 587160,
    "dDurationMs": 3840,
    "segs": [ {
      "utf8": "Eta txapela botatzen diet eta\nhori zoragarria iruditzen zait."
    } ]
  }, {
    "tStartMs": 591000,
    "dDurationMs": 1040,
    "segs": [ {
      "utf8": "Inoiz"
    } ]
  }, {
    "tStartMs": 592040,
    "dDurationMs": 2280,
    "segs": [ {
      "utf8": "\nplastikotasun-mekanismo horiek aprobetxatu nahi dituztela erabakitzen badute, bizitza osoan zehar"
    } ]
  }, {
    "tStartMs": 594320,
    "dDurationMs": 2803,
    "segs": [ {
      "utf8": "edozein unetan egin dezakete\n."
    } ]
  }, {
    "tStartMs": 598450,
    "dDurationMs": 3340,
    "segs": [ {
      "utf8": "Has gaitezen\n"
    } ]
  }, {
    "tStartMs": 601790,
    "dDurationMs": 1770,
    "segs": [ {
      "utf8": "nerbio-sistemaren barnean dauden sistema ezberdinei buruz hitz egiten,"
    } ]
  }, {
    "tStartMs": 603560,
    "dDurationMs": 2310,
    "segs": [ {
      "utf8": "plastikotasunerako eskuragarri daudenak."
    } ]
  }, {
    "tStartMs": 605870,
    "dDurationMs": 2140,
    "segs": [ {
      "utf8": "Eta horrela,\n"
    } ]
  }, {
    "tStartMs": 608010,
    "dDurationMs": 2570,
    "segs": [ {
      "utf8": "egunero,"
    } ]
  }, {
    "tStartMs": 610580,
    "dDurationMs": 2763,
    "segs": [ {
      "utf8": "astero eta urtero egiten dudanaren testuinguruan kokatuko ditut."
    } ]
  }, {
    "tStartMs": 614410,
    "dDurationMs": 4290,
    "segs": [ {
      "utf8": "Lehenik eta behin,\nhainbat plastikotasun mota daude.  Epe"
    } ]
  }, {
    "tStartMs": 618700,
    "dDurationMs": 2530,
    "segs": [ {
      "utf8": "\nluzeko potentziazioa,"
    } ]
  }, {
    "tStartMs": 621230,
    "dDurationMs": 2060,
    "segs": [ {
      "utf8": "epe luzeko depresioa bezalako izenak dituzte, depresio emozionalarekin\nzerikusirik ez duena"
    } ]
  }, {
    "tStartMs": 623290,
    "dDurationMs": 1510,
    "segs": [ {
      "utf8": ", bide batez."
    } ]
  }, {
    "tStartMs": 624800,
    "dDurationMs": 3170,
    "segs": [ {
      "utf8": "Eta\nspike-timing-en menpeko plastikotasuna bezalako gauzak."
    } ]
  }, {
    "tStartMs": 627970,
    "dDurationMs": 4220,
    "segs": [ {
      "utf8": "Izen horiek\nfenomeno zelularra deskribatzeko erabiltzen dira."
    } ]
  }, {
    "tStartMs": 632190,
    "dDurationMs": 2170,
    "segs": [ {
      "utf8": "\nSinapsiak,"
    } ]
  }, {
    "tStartMs": 634360,
    "dDurationMs": 1750,
    "segs": [ {
      "utf8": "neuronen arteko konexioak aldatzen diren benetako moduak."
    } ]
  }, {
    "tStartMs": 636110,
    "dDurationMs": 960,
    "segs": [ {
      "utf8": "Gauza horiek aipatuko ditut"
    } ]
  }, {
    "tStartMs": 637070,
    "dDurationMs": 1712,
    "segs": [ {
      "utf8": "eta apur bat gehiago emango diot"
    } ]
  }, {
    "tStartMs": 638782,
    "dDurationMs": 2278,
    "segs": [ {
      "utf8": "zer diren aipatu bezala."
    } ]
  }, {
    "tStartMs": 641060,
    "dDurationMs": 2470,
    "segs": [ {
      "utf8": "Baina, ziurrenik, hori ez da plastikotasunari buruz\npentsatzeko modurik onena"
    } ]
  }, {
    "tStartMs": 643530,
    "dDurationMs": 3080,
    "segs": [ {
      "utf8": "\nzure garuna optimizatzeari dagokionez."
    } ]
  }, {
    "tStartMs": 646610,
    "dDurationMs": 3200,
    "segs": [ {
      "utf8": "Pentsatzeko modurik onena\nepe laburreko,"
    } ]
  }, {
    "tStartMs": 649810,
    "dDurationMs": 3540,
    "segs": [ {
      "utf8": "epe ertaineko eta epe luzeko plastikotasuna da."
    } ]
  }, {
    "tStartMs": 653350,
    "dDurationMs": 2480,
    "segs": [ {
      "utf8": "Epe laburreko plastikotasuna, momentuan edo egunean"
    } ]
  }, {
    "tStartMs": 655830,
    "dDurationMs": 2620,
    "segs": [ {
      "utf8": "lortu nahi duzun edozein aldaketa da,"
    } ]
  }, {
    "tStartMs": 658450,
    "dDurationMs": 2060,
    "segs": [ {
      "utf8": ""
    } ]
  }, {
    "tStartMs": 660510,
    "dDurationMs": 2820,
    "segs": [ {
      "utf8": "baina ez diozun zertan\nbetirako eutsi nahi."
    } ]
  }, {
    "tStartMs": 663330,
    "dDurationMs": 1980,
    "segs": [ {
      "utf8": "Esan dezakezu, ba, zer motatako\ngauzak dira horiek?"
    } ]
  }, {
    "tStartMs": 665310,
    "dDurationMs": 3090,
    "segs": [ {
      "utf8": "Beno, adibidez, epe laburreko\nplastikotasuna izan daiteke,"
    } ]
  }, {
    "tStartMs": 668400,
    "dDurationMs": 3500,
    "segs": [ {
      "utf8": "\nhegaldi bat hartu nahi baino lehenago esnatzen zara."
    } ]
  }, {
    "tStartMs": 671900,
    "dDurationMs": 2570,
    "segs": [ {
      "utf8": "Ez zaude bereziki erne sentitzen,"
    } ]
  }, {
    "tStartMs": 674470,
    "dDurationMs": 2480,
    "segs": [ {
      "utf8": "eta protokolo bat erabili nahi duzu,"
    } ]
  }, {
    "tStartMs": 676950,
    "dDurationMs": 2740,
    "segs": [ {
      "utf8": "edo\nkafea izan daitekeen protokolo bat erabiltzea erabakitzen duzu"
    } ]
  }, {
    "tStartMs": 679690,
    "dDurationMs": 1700,
    "segs": [ {
      "utf8": "edo arnasketa modu jakin bat izan daiteke"
    } ]
  }, {
    "tStartMs": 681390,
    "dDurationMs": 1280,
    "segs": [ {
      "utf8": "edo beste tresnaren bat izan daiteke,"
    } ]
  }, {
    "tStartMs": 682670,
    "dDurationMs": 1620,
    "segs": [ {
      "utf8": "erneago egoteko."
    } ]
  }, {
    "tStartMs": 684290,
    "dDurationMs": 1800,
    "segs": [ {
      "utf8": "normalean horren alerta ez zauden eguneko ordua."
    } ]
  }, {
    "tStartMs": 686090,
    "dDurationMs": 3540,
    "segs": [ {
      "utf8": "Baina, zure itxaropena da\netxera itzultzean,"
    } ]
  }, {
    "tStartMs": 689630,
    "dDurationMs": 1720,
    "segs": [ {
      "utf8": "horrekin baztertuko duzula,"
    } ]
  }, {
    "tStartMs": 691350,
    "dDurationMs": 1960,
    "segs": [ {
      "utf8": "hori 5:30ean egin beharra,"
    } ]
  }, {
    "tStartMs": 693310,
    "dDurationMs": 1730,
    "segs": [ {
      "utf8": "5:30ean lo egongo zarelako."
    } ]
  }, {
    "tStartMs": 695040,
    "dDurationMs": 3280,
    "segs": [ {
      "utf8": "Beraz, epe laburreko plastikotasuna dago,\nportaerazko plastikotasuna."
    } ]
  }, {
    "tStartMs": 698320,
    "dDurationMs": 1890,
    "segs": [ {
      "utf8": "Gero, epe ertaineko plastikotasuna dago,"
    } ]
  }, {
    "tStartMs": 700210,
    "dDurationMs": 2020,
    "segs": [ {
      "utf8": "aldaketak direnak,\negin nahi dituzunak."
    } ]
  }, {
    "tStartMs": 702230,
    "dDurationMs": 4830,
    "segs": [ {
      "utf8": "Errespetuz deitzen diot honi,\neta umore pixka bat,"
    } ]
  }, {
    "tStartMs": 707060,
    "dDurationMs": 1240,
    "segs": [ {
      "utf8": "edo behintzat nire umore modukoa."
    } ]
  }, {
    "tStartMs": 708300,
    "dDurationMs": 5000,
    "segs": [ {
      "utf8": "Honi graduko,\npre-med fenomenoa deitzen diot."
    } ]
  }, {
    "tStartMs": 713790,
    "dDurationMs": 2080,
    "segs": [ {
      "utf8": "\nMedikuntza aurrekoekin lan egin duzuenontzat."
    } ]
  }, {
    "tStartMs": 715870,
    "dDurationMs": 1140,
    "segs": [ {
      "utf8": "Eta errespetu izugarria diet"
    } ]
  }, {
    "tStartMs": 717010,
    "dDurationMs": 2290,
    "segs": [ {
      "utf8": "medikuntzako ikasleei eta medikuntza aurrekoei,"
    } ]
  }, {
    "tStartMs": 719300,
    "dDurationMs": 2890,
    "segs": [ {
      "utf8": "badago"
    } ]
  }, {
    "tStartMs": 722190,
    "dDurationMs": 2440,
    "segs": [ {
      "utf8": "zertan ados ez nagoen estereotipo mota bat,"
    } ]
  }, {
    "tStartMs": 724630,
    "dDurationMs": 1830,
    "segs": [ {
      "utf8": "baina estereotipoa da azterketarako jakin behar dutena jakin nahi dutela"
    } ]
  }, {
    "tStartMs": 726460,
    "dDurationMs": 1640,
    "segs": [ {
      "utf8": "\n,"
    } ]
  }, {
    "tStartMs": 728100,
    "dDurationMs": 1020,
    "segs": [ {
      "utf8": "baina  ez dut benetan jakin nahi."
    } ]
  }, {
    "tStartMs": 729120,
    "dDurationMs": 1210,
    "segs": [ {
      "utf8": "A besterik ez dute nahi."
    } ]
  }, {
    "tStartMs": 730330,
    "dDurationMs": 1400,
    "segs": [ {
      "utf8": "Eta ez dut uste hori beti egia denik."
    } ]
  }, {
    "tStartMs": 731730,
    "dDurationMs": 1330,
    "segs": [ {
      "utf8": ""
    } ]
  }, {
    "tStartMs": 733060,
    "dDurationMs": 1400,
    "segs": [ {
      "utf8": "Urte hauetan zehar aurre-medikuntza ezberdinekin lan egin dut"
    } ]
  }, {
    "tStartMs": 734460,
    "dDurationMs": 3940,
    "segs": [ {
      "utf8": "eta horietako asko daude\n"
    } ]
  }, {
    "tStartMs": 738400,
    "dDurationMs": 2530,
    "segs": [ {
      "utf8": "ezagutzari buruz erabat sutsuak direnak."
    } ]
  }, {
    "tStartMs": 740930,
    "dDurationMs": 1670,
    "segs": [ {
      "utf8": "Eta A ere nahi zuten."
    } ]
  }, {
    "tStartMs": 742600,
    "dDurationMs": 2030,
    "segs": [ {
      "utf8": "Baina medikuntza aurreko fenomenoa\n"
    } ]
  }, {
    "tStartMs": 744630,
    "dDurationMs": 2710,
    "segs": [ {
      "utf8": "irakasleen eta\nTAren artean eztabaidatzen den bezala, hau da, badakizu,"
    } ]
  }, {
    "tStartMs": 747340,
    "dDurationMs": 980,
    "segs": [ {
      "utf8": "ikasle hauek dituzula,"
    } ]
  }, {
    "tStartMs": 748320,
    "dDurationMs": 1420,
    "segs": [ {
      "utf8": "\njakin nahi dutena besterik ez dute jakin nahi"
    } ]
  }, {
    "tStartMs": 749740,
    "dDurationMs": 1350,
    "segs": [ {
      "utf8": "A lor dezaten,  ezta?  Epe"
    } ]
  }, {
    "tStartMs": 751090,
    "dDurationMs": 1430,
    "segs": [ {
      "utf8": "ertaineko plastikotasuna da.  Egia esan,"
    } ]
  }, {
    "tStartMs": 752520,
    "dDurationMs": 1410,
    "segs": [ {
      "utf8": "ez dute nahi"
    } ]
  }, {
    "tStartMs": 753930,
    "dDurationMs": 1560,
    "segs": [ {
      "utf8": "memorian denbora gehiegi txertatuta egotea,"
    } ]
  }, {
    "tStartMs": 755490,
    "dDurationMs": 1790,
    "segs": [ {
      "utf8": "edo, bestela,\ninformazioa axola zaie."
    } ]
  }, {
    "tStartMs": 757280,
    "dDurationMs": 2150,
    "segs": [ {
      "utf8": "Beraz, epe ertaineko informazioa da."
    } ]
  }, {
    "tStartMs": 759430,
    "dDurationMs": 1740,
    "segs": [ {
      "utf8": "Eta batzuetan hori erabilgarria da adibidez"
    } ]
  }, {
    "tStartMs": 761170,
    "dDurationMs": 2520,
    "segs": [ {
      "utf8": "Costa Ricara oporretara joaten bazara,"
    } ]
  }, {
    "tStartMs": 763690,
    "dDurationMs": 2020,
    "segs": [ {
      "utf8": "eta ez baduzu ezagutzen\nCosta Ricatik nola ibili."
    } ]
  }, {
    "tStartMs": 765710,
    "dDurationMs": 2330,
    "segs": [ {
      "utf8": "\nHerri desberdinak eta hango ibilbideak ikasi nahi dituzu."
    } ]
  }, {
    "tStartMs": 768040,
    "dDurationMs": 2030,
    "segs": [ {
      "utf8": "Baina ez duzu\natzera egiteko asmorik."
    } ]
  }, {
    "tStartMs": 770070,
    "dDurationMs": 880,
    "segs": [ {
      "utf8": "Epe ertainekoa besterik ez da."
    } ]
  }, {
    "tStartMs": 770950,
    "dDurationMs": 2270,
    "segs": [ {
      "utf8": "Programatu besterik ez duzu egin nahi\nduzun denboragatik"
    } ]
  }, {
    "tStartMs": 773220,
    "dDurationMs": 1610,
    "segs": [ {
      "utf8": "eta gero baztertu nahi duzu."
    } ]
  }, {
    "tStartMs": 774830,
    "dDurationMs": 833,
    "segs": [ {
      "utf8": "Gehienetan garuna optimizatzeaz"
    } ]
  }, {
    "tStartMs": 775663,
    "dDurationMs": 2107,
    "segs": [ {
      "utf8": "pentsatzen edo hitz egiten dugunean\n,"
    } ]
  }, {
    "tStartMs": 777770,
    "dDurationMs": 2400,
    "segs": [ {
      "utf8": "epe luzerako plastikotasunaz ari gara."
    } ]
  }, {
    "tStartMs": 780170,
    "dDurationMs": 1720,
    "segs": [ {
      "utf8": ""
    } ]
  }, {
    "tStartMs": 781890,
    "dDurationMs": 833,
    "segs": [ {
      "utf8": "Jendeak egin nahi dituen aldaketei buruz ari gara,"
    } ]
  }, {
    "tStartMs": 782723,
    "dDurationMs": 3927,
    "segs": [ {
      "utf8": "haien garunak modu\nerreflexiboan modu ezberdinean funtziona dezan."
    } ]
  }, {
    "tStartMs": 786650,
    "dDurationMs": 1280,
    "segs": [ {
      "utf8": "Hauxe egiten du umeak"
    } ]
  }, {
    "tStartMs": 787930,
    "dDurationMs": 1840,
    "segs": [ {
      "utf8": "ibiltzen ez jakitetik ibiltzen jakitera pasatzen denean"
    } ]
  }, {
    "tStartMs": 789770,
    "dDurationMs": 1010,
    "segs": [ {
      "utf8": "."
    } ]
  }, {
    "tStartMs": 790780,
    "dDurationMs": 980,
    "segs": [ {
      "utf8": "Ez du zertan pentsatu behar"
    } ]
  }, {
    "tStartMs": 791760,
    "dDurationMs": 1330,
    "segs": [ {
      "utf8": "ibiltzen ikasi ondoren."
    } ]
  }, {
    "tStartMs": 793090,
    "dDurationMs": 1313,
    "segs": [ {
      "utf8": "Erreflexibo bihurtzen da."
    } ]
  }, {
    "tStartMs": 795240,
    "dDurationMs": 4300,
    "segs": [ {
      "utf8": "Epe luzerako plastikotasuna,\nia beti da helburu handia."
    } ]
  }, {
    "tStartMs": 799540,
    "dDurationMs": 1770,
    "segs": [ {
      "utf8": "\nHizkuntza hori hitz egiten jakin nahi dut."
    } ]
  }, {
    "tStartMs": 801310,
    "dDurationMs": 1780,
    "segs": [ {
      "utf8": "Trebetasun hori egiteko gai izan nahi dut."
    } ]
  }, {
    "tStartMs": 803090,
    "dDurationMs": 1670,
    "segs": [ {
      "utf8": "Horrela sentitzeko gai izan nahi dut,"
    } ]
  }, {
    "tStartMs": 804760,
    "dDurationMs": 2200,
    "segs": [ {
      "utf8": "lan handirik egin gabe."
    } ]
  }, {
    "tStartMs": 806960,
    "dDurationMs": 833,
    "segs": [ {
      "utf8": "Eta"
    } ]
  }, {
    "tStartMs": 807793,
    "dDurationMs": 1847,
    "segs": [ {
      "utf8": "\nhori lortzeko norberak egin ditzakeen tresnak eta protokoloak daude."
    } ]
  }, {
    "tStartMs": 809640,
    "dDurationMs": 1490,
    "segs": [ {
      "utf8": "Eta horietaz hitz egingo dugu."
    } ]
  }, {
    "tStartMs": 811130,
    "dDurationMs": 1930,
    "segs": [ {
      "utf8": "Horietako batzuei buruz hitz egin dugu\naurreko ataletan,"
    } ]
  }, {
    "tStartMs": 813060,
    "dDurationMs": 2373,
    "segs": [ {
      "utf8": "baina protokolo horiek berriro aztertuko ditut gaur."
    } ]
  }, {
    "tStartMs": 817070,
    "dDurationMs": 1390,
    "segs": [ {
      "utf8": "Hau guztia"
    } ]
  }, {
    "tStartMs": 818460,
    "dDurationMs": 4170,
    "segs": [ {
      "utf8": "eguneroko\nbizitzaren, asteko bizitzaren"
    } ]
  }, {
    "tStartMs": 822630,
    "dDurationMs": 2070,
    "segs": [ {
      "utf8": "eta urteko bizitzaren testuinguruan kokatuko dut."
    } ]
  }, {
    "tStartMs": 824700,
    "dDurationMs": 1280,
    "segs": [ {
      "utf8": "Eta hori da,"
    } ]
  }, {
    "tStartMs": 825980,
    "dDurationMs": 3223,
    "segs": [ {
      "utf8": "neuroplastikotasuna eta zure garuna optimizatuz,"
    } ]
  }, {
    "tStartMs": 830070,
    "dDurationMs": 3430,
    "segs": [ {
      "utf8": "oinarri sakonago batean ibiltzen direlako,"
    } ]
  }, {
    "tStartMs": 833500,
    "dDurationMs": 3310,
    "segs": [ {
      "utf8": "plastikotasuna gobernatzen duen gauza honen gainean."
    } ]
  }, {
    "tStartMs": 836810,
    "dDurationMs": 3310,
    "segs": [ {
      "utf8": "Izan ere, gure bizitza guztia gobernatzen du\n, arousal autonomikoa deritzona,"
    } ]
  }, {
    "tStartMs": 840120,
    "dDurationMs": 2600,
    "segs": [ {
      "utf8": "hau da,\n24 orduko zikloaren zati batean lo gaudela"
    } ]
  }, {
    "tStartMs": 842720,
    "dDurationMs": 1680,
    "segs": [ {
      "utf8": "eta ia beti esna gaudela."
    } ]
  }, {
    "tStartMs": 844400,
    "dDurationMs": 2340,
    "segs": [ {
      "utf8": "Geure burua bultzatzen badugu eta\nesna egoten bagara, ondo gaude."
    } ]
  }, {
    "tStartMs": 846740,
    "dDurationMs": 1470,
    "segs": [ {
      "utf8": "Hori egin dezakegu gau bat edo bi,"
    } ]
  }, {
    "tStartMs": 848210,
    "dDurationMs": 2230,
    "segs": [ {
      "utf8": "baina ia beti\nzati batean lotan gaude"
    } ]
  }, {
    "tStartMs": 850440,
    "dDurationMs": 1850,
    "segs": [ {
      "utf8": "eta zati batean esna gaude."
    } ]
  }, {
    "tStartMs": 853220,
    "dDurationMs": 1620,
    "segs": [ {
      "utf8": "Lehen esan dut baina berriro esango dut.  Ikaskuntzan"
    } ]
  }, {
    "tStartMs": 854840,
    "dDurationMs": 2240,
    "segs": [ {
      "utf8": "plastikotasunaren eragilea\n"
    } ]
  }, {
    "tStartMs": 857080,
    "dDurationMs": 2740,
    "segs": [ {
      "utf8": "foku handian eta ernetasun-egoeretan gertatzen da,"
    } ]
  }, {
    "tStartMs": 859820,
    "dDurationMs": 1810,
    "segs": [ {
      "utf8": "ez lo zauden bitartean."
    } ]
  }, {
    "tStartMs": 861630,
    "dDurationMs": 2020,
    "segs": [ {
      "utf8": "Eta fokua eta ernetasuna biak dira gakoak,"
    } ]
  }, {
    "tStartMs": 863650,
    "dDurationMs": 3033,
    "segs": [ {
      "utf8": "\negoera horiekin lotutako neurokimikoengatik."
    } ]
  }, {
    "tStartMs": 867610,
    "dDurationMs": 2860,
    "segs": [ {
      "utf8": "Baina, benetako birkonexioa\neta"
    } ]
  }, {
    "tStartMs": 870470,
    "dDurationMs": 3200,
    "segs": [ {
      "utf8": "garuneko konexioen birkonfigurazioa\nlorik gabeko atseden sakonean gertatzen da,"
    } ]
  }, {
    "tStartMs": 873670,
    "dDurationMs": 2090,
    "segs": [ {
      "utf8": "beti bezala gehiago hitz egingo dugu."
    } ]
  }, {
    "tStartMs": 875760,
    "dDurationMs": 2150,
    "segs": [ {
      "utf8": "Eta, lo sakona."
    } ]
  }, {
    "tStartMs": 877910,
    "dDurationMs": 3260,
    "segs": [ {
      "utf8": "Beraz, aldaketa abiarazten duzu eta\nloan aldaketa lortzen duzu."
    } ]
  }, {
    "tStartMs": 881170,
    "dDurationMs": 3100,
    "segs": [ {
      "utf8": "Beraz,\ngaur"
    } ]
  }, {
    "tStartMs": 884270,
    "dDurationMs": 1670,
    "segs": [ {
      "utf8": "garunaren optimizazioari buruz hitz egingo ditugun gauza batzuk"
    } ]
  }, {
    "tStartMs": 885940,
    "dDurationMs": 2030,
    "segs": [ {
      "utf8": "loaren inguruan oinarritzen dira,"
    } ]
  }, {
    "tStartMs": 887970,
    "dDurationMs": 2670,
    "segs": [ {
      "utf8": "arousal sistema autonomoaren inguruan baizik."
    } ]
  }, {
    "tStartMs": 890640,
    "dDurationMs": 2680,
    "segs": [ {
      "utf8": "\nGure garunean eta gorputzean neurona-sistema hau"
    } ]
  }, {
    "tStartMs": 893320,
    "dDurationMs": 4580,
    "segs": [ {
      "utf8": "ikaragarria dena,\nesnatzen gaituena eta erne jartzen gaituena."
    } ]
  }, {
    "tStartMs": 897900,
    "dDurationMs": 2470,
    "segs": [ {
      "utf8": "Eta sistema horretara sartzen ez garenean,"
    } ]
  }, {
    "tStartMs": 900370,
    "dDurationMs": 1860,
    "segs": [ {
      "utf8": "ba, ezin dugu plastikotasuna sartu."
    } ]
  }, {
    "tStartMs": 902230,
    "dDurationMs": 2020,
    "segs": [ {
      "utf8": "Ezin dugu gure garuna optimizatu."
    } ]
  }, {
    "tStartMs": 904250,
    "dDurationMs": 3960,
    "segs": [ {
      "utf8": "Era berean, ezin badugu ondo lo egin\n, eta ezin badugu ondo atseden hartu,"
    } ]
  }, {
    "tStartMs": 908210,
    "dDurationMs": 3610,
    "segs": [ {
      "utf8": "ez dugu plastikotasunean sartuko\neta gure garuna berrezartuko dugu,"
    } ]
  }, {
    "tStartMs": 911820,
    "dDurationMs": 1880,
    "segs": [ {
      "utf8": "orduan\n"
    } ]
  }, {
    "tStartMs": 913700,
    "dDurationMs": 1870,
    "segs": [ {
      "utf8": "gertatzen baita konexioen arteko benetako konfigurazioa."
    } ]
  }, {
    "tStartMs": 915570,
    "dDurationMs": 2670,
    "segs": [ {
      "utf8": "Beraz, hau testuinguruan jartzeko,"
    } ]
  }, {
    "tStartMs": 918240,
    "dDurationMs": 4750,
    "segs": [ {
      "utf8": "egunero esnatzen naiz, eta\nguztiz zintzoa izango naiz."
    } ]
  }, {
    "tStartMs": 922990,
    "dDurationMs": 3160,
    "segs": [ {
      "utf8": "Normalean ez dut ohetik\nirteteko gogorik."
    } ]
  }, {
    "tStartMs": 926150,
    "dDurationMs": 2730,
    "segs": [ {
      "utf8": "Normalean ez naiz guztiz atseden hartzen."
    } ]
  }, {
    "tStartMs": 928880,
    "dDurationMs": 2200,
    "segs": [ {
      "utf8": "Eta hori ez da\nnahikoa lo egiten ez dudalako."
    } ]
  }, {
    "tStartMs": 931080,
    "dDurationMs": 2040,
    "segs": [ {
      "utf8": "Seguruenik, ez dudalako izugarria"
    } ]
  }, {
    "tStartMs": 933120,
    "dDurationMs": 2230,
    "segs": [ {
      "utf8": "nire loaldia hain ondo denbora egiteagatik izango da."
    } ]
  }, {
    "tStartMs": 935350,
    "dDurationMs": 1540,
    "segs": [ {
      "utf8": "Orain hilabete hau ez da lo egiteari buruz,"
    } ]
  }, {
    "tStartMs": 936890,
    "dDurationMs": 1000,
    "segs": [ {
      "utf8": "aurreko hilabetea izan zen"
    } ]
  }, {
    "tStartMs": 937890,
    "dDurationMs": 2133,
    "segs": [ {
      "utf8": "baina benetan puntu batzuk azpimarratu nahi ditut."
    } ]
  }, {
    "tStartMs": 941160,
    "dDurationMs": 2880,
    "segs": [ {
      "utf8": "Esnatzen naiz, orokorrean nekatuago,"
    } ]
  }, {
    "tStartMs": 944040,
    "dDurationMs": 2020,
    "segs": [ {
      "utf8": "eta nahi nukeena baino haserretuago,"
    } ]
  }, {
    "tStartMs": 946060,
    "dDurationMs": 2320,
    "segs": [ {
      "utf8": "beranduegi joan ohi naizelako lotara.  Egiten"
    } ]
  }, {
    "tStartMs": 948380,
    "dDurationMs": 1507,
    "segs": [ {
      "utf8": "dudan gauza bat besterik ez da."
    } ]
  }, {
    "tStartMs": 949887,
    "dDurationMs": 3073,
    "segs": [ {
      "utf8": "Eta goiz jaiki ohi naiz\nalarma jartzen dudalako,"
    } ]
  }, {
    "tStartMs": 952960,
    "dDurationMs": 1370,
    "segs": [ {
      "utf8": "egiteko gauzak ditudalako,"
    } ]
  }, {
    "tStartMs": 954330,
    "dDurationMs": 1840,
    "segs": [ {
      "utf8": "edo berez goiz esnatzen naizelako"
    } ]
  }, {
    "tStartMs": 956170,
    "dDurationMs": 2620,
    "segs": [ {
      "utf8": "argia sartzen delako eta abar."
    } ]
  }, {
    "tStartMs": 958790,
    "dDurationMs": 2170,
    "segs": [ {
      "utf8": "Beno, horrek esaten didana da"
    } ]
  }, {
    "tStartMs": 960960,
    "dDurationMs": 4180,
    "segs": [ {
      "utf8": "ziurrenik bere\nerritmo zirkadiano naturala duen norbait naizela."
    } ]
  }, {
    "tStartMs": 965140,
    "dDurationMs": 1300,
    "segs": [ {
      "utf8": "Kronotipoen berri izan dezakezu."
    } ]
  }, {
    "tStartMs": 966440,
    "dDurationMs": 1620,
    "segs": [ {
      "utf8": "Hauek genetikoki programatutako gauzak dira."
    } ]
  }, {
    "tStartMs": 968060,
    "dDurationMs": 2610,
    "segs": [ {
      "utf8": "Baina kronotipoa, 24 ordu baino laburragoa da."
    } ]
  }, {
    "tStartMs": 970670,
    "dDurationMs": 1450,
    "segs": [ {
      "utf8": "Horrek esan nahi du"
    } ]
  }, {
    "tStartMs": 972120,
    "dDurationMs": 3700,
    "segs": [ {
      "utf8": "niretzat esna eta erne-zikloa\nziurrenik 24 ordu baino laburragoa dela."
    } ]
  }, {
    "tStartMs": 975820,
    "dDurationMs": 1590,
    "segs": [ {
      "utf8": "Horrek esan nahi du arratsaldean argi pixka bat hartzeak"
    } ]
  }, {
    "tStartMs": 977410,
    "dDurationMs": 2830,
    "segs": [ {
      "utf8": "aldaketak egiten lagunduko didala"
    } ]
  }, {
    "tStartMs": 980240,
    "dDurationMs": 1660,
    "segs": [ {
      "utf8": "eta zikloa pixka bat luzeagoa izango dela."
    } ]
  }, {
    "tStartMs": 981900,
    "dDurationMs": 1120,
    "segs": [ {
      "utf8": "Fasea atzeratuko dit"
    } ]
  }, {
    "tStartMs": 983020,
    "dDurationMs": 2030,
    "segs": [ {
      "utf8": "horrek zentzurik ez badu\n, ikusi aurreko atal bat."
    } ]
  }, {
    "tStartMs": 985050,
    "dDurationMs": 1530,
    "segs": [ {
      "utf8": "Baina benetan esan nahi duena arratsaldean\nargi pixka bat lortzea da"
    } ]
  }, {
    "tStartMs": 986580,
    "dDurationMs": 1270,
    "segs": [ {
      "utf8": ", beranduago jaikitzeko aukera emango dit"
    } ]
  }, {
    "tStartMs": 987850,
    "dDurationMs": 1990,
    "segs": [ {
      "utf8": "."
    } ]
  }, {
    "tStartMs": 989840,
    "dDurationMs": 1940,
    "segs": [ {
      "utf8": "Baina esan nahi duena zera da,"
    } ]
  }, {
    "tStartMs": 991780,
    "dDurationMs": 5000,
    "segs": [ {
      "utf8": "ez nagoela ziurrenik\n"
    } ]
  }, {
    "tStartMs": 997360,
    "dDurationMs": 2810,
    "segs": [ {
      "utf8": "8:30etan edo 9:00etan oheratu"
    } ]
  }, {
    "tStartMs": 1000170,
    "dDurationMs": 1860,
    "segs": [ {
      "utf8": "eta goizeko 4:00etan esnatzeko nire beharrizanak betetzen."
    } ]
  }, {
    "tStartMs": 1002030,
    "dDurationMs": 2200,
    "segs": [ {
      "utf8": "10:30, 11:00ak aldera joan ohi naiz lotara,"
    } ]
  }, {
    "tStartMs": 1004230,
    "dDurationMs": 1610,
    "segs": [ {
      "utf8": "azkenaldian 11:30ak edo 12:00ak aldera"
    } ]
  }, {
    "tStartMs": 1005840,
    "dDurationMs": 867,
    "segs": [ {
      "utf8": "eta gero 6:00etan esnatzen naiz."
    } ]
  }, {
    "tStartMs": 1006707,
    "dDurationMs": 2083,
    "segs": [ {
      "utf8": "Eta beraz, noski, nazkatuta sentituko naiz."
    } ]
  }, {
    "tStartMs": 1008790,
    "dDurationMs": 4160,
    "segs": [ {
      "utf8": "Beraz, neuroplastikoak ahalbidetuko\ndit esna optimizatzeko,"
    } ]
  }, {
    "tStartMs": 1012950,
    "dDurationMs": 3160,
    "segs": [ {
      "utf8": "baina zerbait egin behar dut\nhorretara iristeko."
    } ]
  }, {
    "tStartMs": 1016110,
    "dDurationMs": 2020,
    "segs": [ {
      "utf8": "Eta zuetako batzuk\ndagoeneko aurreikusten ari zarete"
    } ]
  }, {
    "tStartMs": 1018130,
    "dDurationMs": 1300,
    "segs": [ {
      "utf8": "esango dudana, hau da,"
    } ]
  }, {
    "tStartMs": 1019430,
    "dDurationMs": 2290,
    "segs": [ {
      "utf8": "ez, eguneko lehen 30 minutuetan\neguzki-argia gure begietan sartzeko esango digu"
    } ]
  }, {
    "tStartMs": 1021720,
    "dDurationMs": 1720,
    "segs": [ {
      "utf8": "."
    } ]
  }, {
    "tStartMs": 1023440,
    "dDurationMs": 1260,
    "segs": [ {
      "utf8": "Hori egiteko esango dizut,"
    } ]
  }, {
    "tStartMs": 1024700,
    "dDurationMs": 1770,
    "segs": [ {
      "utf8": "baina lehenago eztabaidatu ez ditudan bi gauza ere esango dizkizut"
    } ]
  }, {
    "tStartMs": 1026470,
    "dDurationMs": 2340,
    "segs": [ {
      "utf8": ","
    } ]
  }, {
    "tStartMs": 1028810,
    "dDurationMs": 2430,
    "segs": [ {
      "utf8": ""
    } ]
  }, {
    "tStartMs": 1031240,
    "dDurationMs": 2687,
    "segs": [ {
      "utf8": "melanopsina zelulen arteko plastikotasunari dagozkionak."
    } ]
  }, {
    "tStartMs": 1033927,
    "dDurationMs": 1953,
    "segs": [ {
      "utf8": "Eguzki-argia detektatzen dute,\nargi distiratsua"
    } ]
  }, {
    "tStartMs": 1035880,
    "dDurationMs": 2330,
    "segs": [ {
      "utf8": "gure begiko zelulak eta erloju zirkadianoa detektatzen dute.  Inoiz"
    } ]
  }, {
    "tStartMs": 1038210,
    "dDurationMs": 1740,
    "segs": [ {
      "utf8": "ez dut hau\npodcast honetan esan,"
    } ]
  }, {
    "tStartMs": 1039950,
    "dDurationMs": 1810,
    "segs": [ {
      "utf8": "baina ondorioztatzen da"
    } ]
  }, {
    "tStartMs": 1041760,
    "dDurationMs": 3140,
    "segs": [ {
      "utf8": "melanopsina\nzelula horien eta erloju zirkadianoaren arteko loturak"
    } ]
  }, {
    "tStartMs": 1044900,
    "dDurationMs": 2750,
    "segs": [ {
      "utf8": "plastikoak direla bizitza osoan zehar.  Bertan konexioen"
    } ]
  }, {
    "tStartMs": 1047650,
    "dDurationMs": 2650,
    "segs": [ {
      "utf8": "konfigurazio masiboa dago\n."
    } ]
  }, {
    "tStartMs": 1050300,
    "dDurationMs": 1780,
    "segs": [ {
      "utf8": "Eta zelula glial bat diren astrozitoak izeneko zelula mota bat"
    } ]
  }, {
    "tStartMs": 1052080,
    "dDurationMs": 1570,
    "segs": [ {
      "utf8": ""
    } ]
  }, {
    "tStartMs": 1053650,
    "dDurationMs": 3480,
    "segs": [ {
      "utf8": "aktiboki kentzen eta sendotzen ari dira\n"
    } ]
  }, {
    "tStartMs": 1057130,
    "dDurationMs": 3350,
    "segs": [ {
      "utf8": "begiaren eta erloju horren arteko konexioak, egunero."
    } ]
  }, {
    "tStartMs": 1060480,
    "dDurationMs": 1850,
    "segs": [ {
      "utf8": "Orain sinestezina da\n"
    } ]
  }, {
    "tStartMs": 1062330,
    "dDurationMs": 2000,
    "segs": [ {
      "utf8": "zure garuneko beste alderdi batzuek, adibidez,"
    } ]
  }, {
    "tStartMs": 1064330,
    "dDurationMs": 2630,
    "segs": [ {
      "utf8": "nor zaren jakitea adierazten zaituztelako,"
    } ]
  }, {
    "tStartMs": 1066960,
    "dDurationMs": 1050,
    "segs": [ {
      "utf8": "goizean esnatzen zarenean"
    } ]
  }, {
    "tStartMs": 1068010,
    "dDurationMs": 2440,
    "segs": [ {
      "utf8": "edo zure izena zein den,\n"
    } ]
  }, {
    "tStartMs": 1070450,
    "dDurationMs": 1710,
    "segs": [ {
      "utf8": "zure izena ikasi duzulako adina zarela suposatuz.  ."
    } ]
  }, {
    "tStartMs": 1072160,
    "dDurationMs": 1530,
    "segs": [ {
      "utf8": "Haurrek lehen gauzak ikasten dituztenean"
    } ]
  }, {
    "tStartMs": 1073690,
    "dDurationMs": 1843,
    "segs": [ {
      "utf8": "eta inoiz gutxitan ahazten dugun zerbait."
    } ]
  }, {
    "tStartMs": 1077030,
    "dDurationMs": 2870,
    "segs": [ {
      "utf8": "Konexio horiek etengabe aldatzen ari dira"
    } ]
  }, {
    "tStartMs": 1079900,
    "dDurationMs": 1610,
    "segs": [ {
      "utf8": "24 orduko ziklo bakoitzean."
    } ]
  }, {
    "tStartMs": 1081510,
    "dDurationMs": 2760,
    "segs": [ {
      "utf8": "Beraz,\nepe laburreko plastikotasunerako aukera dago."
    } ]
  }, {
    "tStartMs": 1084270,
    "dDurationMs": 1770,
    "segs": [ {
      "utf8": "Horregatik, eguzki-argia egunean ikusten dut\nlehenengo gauza,"
    } ]
  }, {
    "tStartMs": 1086040,
    "dDurationMs": 1330,
    "segs": [ {
      "utf8": "esnatzen laguntzen dit."
    } ]
  }, {
    "tStartMs": 1087370,
    "dDurationMs": 3250,
    "segs": [ {
      "utf8": "Nik egiten dudan beste gauza bat\nda, lehen"
    } ]
  }, {
    "tStartMs": 1090620,
    "dDurationMs": 1940,
    "segs": [ {
      "utf8": ""
    } ]
  }, {
    "tStartMs": 1092560,
    "dDurationMs": 1560,
    "segs": [ {
      "utf8": "hitz egin dudan erloju zirkadianoaren eta gure adrenalen artean dagoen zirkuitu bat dagoela, goizean goiz kortisolaren"
    } ]
  }, {
    "tStartMs": 1094120,
    "dDurationMs": 1630,
    "segs": [ {
      "utf8": "askapena eragiten duena"
    } ]
  }, {
    "tStartMs": 1095750,
    "dDurationMs": 2890,
    "segs": [ {
      "utf8": "\n, esnatzen gaituena."
    } ]
  }, {
    "tStartMs": 1098640,
    "dDurationMs": 1810,
    "segs": [ {
      "utf8": "Batez ere argia ikusten dugunean."
    } ]
  }, {
    "tStartMs": 1100450,
    "dDurationMs": 1310,
    "segs": [ {
      "utf8": "Beraz, goizean haserre bazaude,"
    } ]
  }, {
    "tStartMs": 1101760,
    "dDurationMs": 2080,
    "segs": [ {
      "utf8": "horregatik argia ikustea lagungarria da."
    } ]
  }, {
    "tStartMs": 1103840,
    "dDurationMs": 1900,
    "segs": [ {
      "utf8": "Baina, gauza interesgarria da goizean"
    } ]
  }, {
    "tStartMs": 1105740,
    "dDurationMs": 3080,
    "segs": [ {
      "utf8": "argia maiz ikusten hasten bazara\n,"
    } ]
  }, {
    "tStartMs": 1108820,
    "dDurationMs": 3190,
    "segs": [ {
      "utf8": "orduan\nmelanopsinaren zelulen"
    } ]
  }, {
    "tStartMs": 1112010,
    "dDurationMs": 3280,
    "segs": [ {
      "utf8": "eta erloju zirkadianoaren arteko lotura horiek indartu"
    } ]
  }, {
    "tStartMs": 1115290,
    "dDurationMs": 1560,
    "segs": [ {
      "utf8": "edo indartu egiten direla esango genuke, argiaren aurreikuspenerako"
    } ]
  }, {
    "tStartMs": 1116850,
    "dDurationMs": 2950,
    "segs": [ {
      "utf8": "indartsuago bihurtzen direla\n."
    } ]
  }, {
    "tStartMs": 1119800,
    "dDurationMs": 2230,
    "segs": [ {
      "utf8": "Eta naturalki lehenago hasten zara esnatzen,"
    } ]
  }, {
    "tStartMs": 1122030,
    "dDurationMs": 1460,
    "segs": [ {
      "utf8": "erneago sentitzen."
    } ]
  }, {
    "tStartMs": 1123490,
    "dDurationMs": 1760,
    "segs": [ {
      "utf8": "Beraz, honek esaten duena"
    } ]
  }, {
    "tStartMs": 1125250,
    "dDurationMs": 2560,
    "segs": [ {
      "utf8": "eta egiten dudana da\nohiko argi hori jasotzen dudala badakitelako"
    } ]
  }, {
    "tStartMs": 1127810,
    "dDurationMs": 1000,
    "segs": [ {
      "utf8": "goiz batzuetan"
    } ]
  }, {
    "tStartMs": 1128810,
    "dDurationMs": 1560,
    "segs": [ {
      "utf8": "ez naizela oso erne sentituko,"
    } ]
  }, {
    "tStartMs": 1130370,
    "dDurationMs": 1580,
    "segs": [ {
      "utf8": "nekatuta sentituko naizela, batez ere."
    } ]
  }, {
    "tStartMs": 1131950,
    "dDurationMs": 1770,
    "segs": [ {
      "utf8": "Eta agian ezingo dut eguzki-argia sartu,"
    } ]
  }, {
    "tStartMs": 1133720,
    "dDurationMs": 1780,
    "segs": [ {
      "utf8": "oso\nestalita dagoelako edo bidaiatzen ari naizelako,"
    } ]
  }, {
    "tStartMs": 1135500,
    "dDurationMs": 860,
    "segs": [ {
      "utf8": "edo beste ezaugarriren bat."
    } ]
  }, {
    "tStartMs": 1136360,
    "dDurationMs": 1270,
    "segs": [ {
      "utf8": "Baina sistema plastikoa da,"
    } ]
  }, {
    "tStartMs": 1137630,
    "dDurationMs": 2320,
    "segs": [ {
      "utf8": "beraz, norabide egokian aldatu da."
    } ]
  }, {
    "tStartMs": 1139950,
    "dDurationMs": 1470,
    "segs": [ {
      "utf8": "Orain atzera egingo du,"
    } ]
  }, {
    "tStartMs": 1141420,
    "dDurationMs": 1390,
    "segs": [ {
      "utf8": ""
    } ]
  }, {
    "tStartMs": 1142810,
    "dDurationMs": 1100,
    "segs": [ {
      "utf8": "bi, hiru egunen buruan epe laburreko plastikotasuna delako."
    } ]
  }, {
    "tStartMs": 1143910,
    "dDurationMs": 833,
    "segs": [ {
      "utf8": "Beraz,"
    } ]
  }, {
    "tStartMs": 1144743,
    "dDurationMs": 2347,
    "segs": [ {
      "utf8": "eguzki-argiaren\nesposizioa aldizka lortzen saiatu nahi duzu."
    } ]
  }, {
    "tStartMs": 1147090,
    "dDurationMs": 4010,
    "segs": [ {
      "utf8": "Egiten dudan beste gauza\nkafeina hartzea atzeratzen dudala da, esna nagoen"
    } ]
  }, {
    "tStartMs": 1151100,
    "dDurationMs": 1920,
    "segs": [ {
      "utf8": "lehen bi orduetan."
    } ]
  }, {
    "tStartMs": 1153020,
    "dDurationMs": 2840,
    "segs": [ {
      "utf8": "Orain, hau oso mingarria izan daiteke jendearentzat."
    } ]
  }, {
    "tStartMs": 1155860,
    "dDurationMs": 3540,
    "segs": [ {
      "utf8": "Baina, lehenago hitz egin genuen\n, adenosina sistemaz"
    } ]
  }, {
    "tStartMs": 1159400,
    "dDurationMs": 3320,
    "segs": [ {
      "utf8": "eta nola\nadenosina metatzeak logura eragiten digu."
    } ]
  }, {
    "tStartMs": 1162720,
    "dDurationMs": 3270,
    "segs": [ {
      "utf8": "Eta kafeinak adenosina kentzen du"
    } ]
  }, {
    "tStartMs": 1165990,
    "dDurationMs": 1580,
    "segs": [ {
      "utf8": "eta erne sentiarazten gaitu."
    } ]
  }, {
    "tStartMs": 1167570,
    "dDurationMs": 1040,
    "segs": [ {
      "utf8": "Baina badakigu"
    } ]
  }, {
    "tStartMs": 1168610,
    "dDurationMs": 4040,
    "segs": [ {
      "utf8": "kafeina irensten baduzu, esnatzean\nberehala,"
    } ]
  }, {
    "tStartMs": 1172650,
    "dDurationMs": 3120,
    "segs": [ {
      "utf8": "adrenalentzako seinalea,\n"
    } ]
  }, {
    "tStartMs": 1175770,
    "dDurationMs": 2320,
    "segs": [ {
      "utf8": "kortisolaren askapen osasungarria dena,"
    } ]
  }, {
    "tStartMs": 1178090,
    "dDurationMs": 1716,
    "segs": [ {
      "utf8": "eta"
    } ]
  }, {
    "tStartMs": 1179806,
    "dDurationMs": 3377,
    "segs": [ {
      "utf8": "loalditik irtetean"
    } ]
  }, {
    "tStartMs": 1184140,
    "dDurationMs": 2090,
    "segs": [ {
      "utf8": "eta lo sakonean gertatzen den adenosina kentzea.\nadenosina kentzea,"
    } ]
  }, {
    "tStartMs": 1186230,
    "dDurationMs": 3450,
    "segs": [ {
      "utf8": "kafeina goizegi irensten baduzu\n,"
    } ]
  }, {
    "tStartMs": 1189680,
    "dDurationMs": 3410,
    "segs": [ {
      "utf8": "adenosina\nerrezeptoreen eta abar lehiatzen duen mekanismo bat dago,"
    } ]
  }, {
    "tStartMs": 1193090,
    "dDurationMs": 2710,
    "segs": [ {
      "utf8": "goiz erdian istripu bat izan dezazun."
    } ]
  }, {
    "tStartMs": 1195800,
    "dDurationMs": 2760,
    "segs": [ {
      "utf8": "Kafeina bada, funtzionatzen duen modua"
    } ]
  }, {
    "tStartMs": 1198560,
    "dDurationMs": 2830,
    "segs": [ {
      "utf8": "kafeina\nadenosina-hartzailea okupatzen badu,"
    } ]
  }, {
    "tStartMs": 1201390,
    "dDurationMs": 3330,
    "segs": [ {
      "utf8": "orduan mekanismo endogeno naturalek,"
    } ]
  }, {
    "tStartMs": 1204720,
    "dDurationMs": 2060,
    "segs": [ {
      "utf8": "adenosina kentzeko,"
    } ]
  }, {
    "tStartMs": 1206780,
    "dDurationMs": 2080,
    "segs": [ {
      "utf8": "ez dute bere ekintzarik izango."
    } ]
  }, {
    "tStartMs": 1208860,
    "dDurationMs": 4760,
    "segs": [ {
      "utf8": "Beraz, garuna adrenal-ardatzarekiko,\nplastikotasunaren menpe dago ere."
    } ]
  }, {
    "tStartMs": 1213620,
    "dDurationMs": 1710,
    "segs": [ {
      "utf8": "Beraz, kafeina"
    } ]
  }, {
    "tStartMs": 1215330,
    "dDurationMs": 2280,
    "segs": [ {
      "utf8": "esnatu eta bi ordura arte atzeratuz gero,"
    } ]
  }, {
    "tStartMs": 1217610,
    "dDurationMs": 3760,
    "segs": [ {
      "utf8": "gai naiz harrapatzeko, eta\nindartzeko, indartzeko,"
    } ]
  }, {
    "tStartMs": 1221370,
    "dDurationMs": 2780,
    "segs": [ {
      "utf8": "\nerloju zirkadianoaren"
    } ]
  }, {
    "tStartMs": 1224150,
    "dDurationMs": 2150,
    "segs": [ {
      "utf8": "eta adrenaletan askatzen den kortisolaren artean dagoen neurona-zirkuitua,"
    } ]
  }, {
    "tStartMs": 1226300,
    "dDurationMs": 3530,
    "segs": [ {
      "utf8": "baita\nadenosina-hartzaile horiek utzi ere.  ,"
    } ]
  }, {
    "tStartMs": 1229830,
    "dDurationMs": 3170,
    "segs": [ {
      "utf8": "okupatu gabe,\ngero kafeina erabil dezadan"
    } ]
  }, {
    "tStartMs": 1233000,
    "dDurationMs": 1640,
    "segs": [ {
      "utf8": "igoera natural bat lortzeko,"
    } ]
  }, {
    "tStartMs": 1234640,
    "dDurationMs": 2690,
    "segs": [ {
      "utf8": "erne eta\nbi ordu beranduago zentratu,"
    } ]
  }, {
    "tStartMs": 1237330,
    "dDurationMs": 4090,
    "segs": [ {
      "utf8": "erabiltzearen aurka,\nlozorrotik esnatzeko."
    } ]
  }, {
    "tStartMs": 1241420,
    "dDurationMs": 2480,
    "segs": [ {
      "utf8": "Beraz, ziur nagoen arren, hor\nkanpoan begiak"
    } ]
  }, {
    "tStartMs": 1243900,
    "dDurationMs": 1600,
    "segs": [ {
      "utf8": "eta aharrausi batzuk daudela, oi"
    } ]
  }, {
    "tStartMs": 1245500,
    "dDurationMs": 2110,
    "segs": [ {
      "utf8": "ez, goizeko eguzki-argia da\nberriro."
    } ]
  }, {
    "tStartMs": 1247610,
    "dDurationMs": 3250,
    "segs": [ {
      "utf8": "Tresna indartsua da,\nzirkuitu horiek berregokitzeko."
    } ]
  }, {
    "tStartMs": 1250860,
    "dDurationMs": 1530,
    "segs": [ {
      "utf8": "Beraz, epe laburreko plastikotasuna."
    } ]
  }, {
    "tStartMs": 1252390,
    "dDurationMs": 833,
    "segs": [ {
      "utf8": "Eta"
    } ]
  }, {
    "tStartMs": 1253223,
    "dDurationMs": 1917,
    "segs": [ {
      "utf8": "kafeina eguneko\nlehen bi orduetan atzeratzeko arrazoia,"
    } ]
  }, {
    "tStartMs": 1255140,
    "dDurationMs": 2660,
    "segs": [ {
      "utf8": "nahiz eta\nlehen bi"
    } ]
  }, {
    "tStartMs": 1257800,
    "dDurationMs": 3620,
    "segs": [ {
      "utf8": "egunetan egitea mingarria izan, orduan modu\nnaturalean esnatzen hasten zara,"
    } ]
  }, {
    "tStartMs": 1261420,
    "dDurationMs": 3520,
    "segs": [ {
      "utf8": "goizetan\nkafeinarik gabe errazago,"
    } ]
  }, {
    "tStartMs": 1264940,
    "dDurationMs": 1940,
    "segs": [ {
      "utf8": "adenosina kendu egiten delako"
    } ]
  }, {
    "tStartMs": 1266880,
    "dDurationMs": 2550,
    "segs": [ {
      "utf8": "eta  ez dituzu hauek lehian."
    } ]
  }, {
    "tStartMs": 1269430,
    "dDurationMs": 2300,
    "segs": [ {
      "utf8": "Antagonista lehiakorra deitzen zaio,"
    } ]
  }, {
    "tStartMs": 1271730,
    "dDurationMs": 3360,
    "segs": [ {
      "utf8": "adenosina hartzaileari."
    } ]
  }, {
    "tStartMs": 1275090,
    "dDurationMs": 2430,
    "segs": [ {
      "utf8": "Beraz, esnatu naiz, eguzki-argia sartzen zait begietan."
    } ]
  }, {
    "tStartMs": 1277520,
    "dDurationMs": 4180,
    "segs": [ {
      "utf8": "Azkenaldian oso\ngoiz esnatzen naizenez argi distiratsua erabiltzen dut,"
    } ]
  }, {
    "tStartMs": 1281700,
    "dDurationMs": 1870,
    "segs": [ {
      "utf8": "ernetasuna suspertzeko."
    } ]
  }, {
    "tStartMs": 1283570,
    "dDurationMs": 1770,
    "segs": [ {
      "utf8": "Egia esan, ez dago\nhelburu horretarako diseinatuta."
    } ]
  }, {
    "tStartMs": 1285340,
    "dDurationMs": 2430,
    "segs": [ {
      "utf8": "\n900 lux inguruko argi-taula besterik ez da."
    } ]
  }, {
    "tStartMs": 1287770,
    "dDurationMs": 2450,
    "segs": [ {
      "utf8": "Eta gero kafeina atzeratzen dut."
    } ]
  }, {
    "tStartMs": 1290220,
    "dDurationMs": 1193,
    "segs": [ {
      "utf8": "Batzuek galdetu duzue,"
    } ]
  }, {
    "tStartMs": 1292630,
    "dDurationMs": 1970,
    "segs": [ {
      "utf8": "eta berriro ez dut esaten\ninork hau egin behar duenik."
    } ]
  }, {
    "tStartMs": 1294600,
    "dDurationMs": 1420,
    "segs": [ {
      "utf8": "Badakizu, zer edaten duzu zehazki?"
    } ]
  }, {
    "tStartMs": 1296020,
    "dDurationMs": 1970,
    "segs": [ {
      "utf8": "Kafe beltzean sinesten dut."
    } ]
  }, {
    "tStartMs": 1297990,
    "dDurationMs": 1540,
    "segs": [ {
      "utf8": "Kafe beltza gustatzen zait."
    } ]
  }, {
    "tStartMs": 1299530,
    "dDurationMs": 1320,
    "segs": [ {
      "utf8": "Jendeak galdetu dit,"
    } ]
  }, {
    "tStartMs": 1300850,
    "dDurationMs": 1320,
    "segs": [ {
      "utf8": "eta ez dut hemen marka-izenik jarri nahi"
    } ]
  }, {
    "tStartMs": 1302170,
    "dDurationMs": 2330,
    "segs": [ {
      "utf8": "kafe mota honi buruz edo beste gauz hauekin nahastu"
    } ]
  }, {
    "tStartMs": 1304500,
    "dDurationMs": 2180,
    "segs": [ {
      "utf8": "duzun kafe mota horri buruz\n"
    } ]
  }, {
    "tStartMs": 1306680,
    "dDurationMs": 1450,
    "segs": [ {
      "utf8": "."
    } ]
  }, {
    "tStartMs": 1308130,
    "dDurationMs": 1860,
    "segs": [ {
      "utf8": "Horrek fokua areagotuko al du?"
    } ]
  }, {
    "tStartMs": 1309990,
    "dDurationMs": 2230,
    "segs": [ {
      "utf8": "Badakizu, gaur asko hitz egingo dut"
    } ]
  }, {
    "tStartMs": 1312220,
    "dDurationMs": 3070,
    "segs": [ {
      "utf8": "dietaren eta\nbarauaren erabilerari eta elikagaien"
    } ]
  }, {
    "tStartMs": 1315290,
    "dDurationMs": 1320,
    "segs": [ {
      "utf8": "eta janari mota jakin batzuei buruz."
    } ]
  }, {
    "tStartMs": 1316610,
    "dDurationMs": 2990,
    "segs": [ {
      "utf8": "Baina egia esan,\nkafe beltza niretzat beti funtzionatu izan den aukera sinple bat besterik ez da"
    } ]
  }, {
    "tStartMs": 1319600,
    "dDurationMs": 1330,
    "segs": [ {
      "utf8": ".  Goizean lehen orduan"
    } ]
  }, {
    "tStartMs": 1320930,
    "dDurationMs": 2310,
    "segs": [ {
      "utf8": "hidratatzen dudala ere ziurtatzen dut\n."
    } ]
  }, {
    "tStartMs": 1323240,
    "dDurationMs": 3890,
    "segs": [ {
      "utf8": "Datu ugari daude orain\nerakusten duten deshidratazioa apur bat"
    } ]
  }, {
    "tStartMs": 1327130,
    "dDurationMs": 1380,
    "segs": [ {
      "utf8": "areagotzeak,"
    } ]
  }, {
    "tStartMs": 1328510,
    "dDurationMs": 1860,
    "segs": [ {
      "utf8": "hau da, ura falta zarenean,"
    } ]
  }, {
    "tStartMs": 1330370,
    "dDurationMs": 1930,
    "segs": [ {
      "utf8": "jendeari buruko mina sor diezaiokeela."
    } ]
  }, {
    "tStartMs": 1332300,
    "dDurationMs": 2480,
    "segs": [ {
      "utf8": "Fotofobia gehigarri bat eman"
    } ]
  }, {
    "tStartMs": 1334780,
    "dDurationMs": 2080,
    "segs": [ {
      "utf8": "diezaieke migrainerako joera duzuenontzat."
    } ]
  }, {
    "tStartMs": 1336860,
    "dDurationMs": 1580,
    "segs": [ {
      "utf8": "Argi distiratsuak migrainak eragin ditzake."
    } ]
  }, {
    "tStartMs": 1338440,
    "dDurationMs": 1240,
    "segs": [ {
      "utf8": "Hori ez da harritzekoa"
    } ]
  }, {
    "tStartMs": 1339680,
    "dDurationMs": 1180,
    "segs": [ {
      "utf8": "buruko minak eta migrainak jasaten dituzuenontzat."
    } ]
  }, {
    "tStartMs": 1340860,
    "dDurationMs": 3580,
    "segs": [ {
      "utf8": "Baina deshidratazioak,"
    } ]
  }, {
    "tStartMs": 1344440,
    "dDurationMs": 1910,
    "segs": [ {
      "utf8": "\nmigrainaren eta buruko minaren ahultasuna areagotu dezake."
    } ]
  }, {
    "tStartMs": 1346350,
    "dDurationMs": 2610,
    "segs": [ {
      "utf8": "Beraz, ura edaten dut,\nkafe beltza edaten dut edo mate edaten dut,"
    } ]
  }, {
    "tStartMs": 1348960,
    "dDurationMs": 2490,
    "segs": [ {
      "utf8": "hau da,\nArgentinako leinua dudalako, kafeina"
    } ]
  }, {
    "tStartMs": 1351450,
    "dDurationMs": 2690,
    "segs": [ {
      "utf8": "handiko edari bat besterik ez baita\n"
    } ]
  }, {
    "tStartMs": 1354140,
    "dDurationMs": 833,
    "segs": [ {
      "utf8": "goizean,"
    } ]
  }, {
    "tStartMs": 1354973,
    "dDurationMs": 2340,
    "segs": [ {
      "utf8": "baina esnatu eta bi ordura arte atzeratzen dut\n."
    } ]
  }, {
    "tStartMs": 1358180,
    "dDurationMs": 3010,
    "segs": [ {
      "utf8": "Eta horregatik,"
    } ]
  }, {
    "tStartMs": 1361190,
    "dDurationMs": 3560,
    "segs": [ {
      "utf8": "nire begiaren eta nire\nerloju zirkadianoaren eta nire adrenalen arteko zirkuituak"
    } ]
  }, {
    "tStartMs": 1364750,
    "dDurationMs": 2130,
    "segs": [ {
      "utf8": "modu jakin batean funtzionatzea nahi dut,"
    } ]
  }, {
    "tStartMs": 1366880,
    "dDurationMs": 2360,
    "segs": [ {
      "utf8": "gero\nkafeina gehigarri bat izan dadin."
    } ]
  }, {
    "tStartMs": 1369240,
    "dDurationMs": 1770,
    "segs": [ {
      "utf8": "Ernetasun gehiago gehitzen du."
    } ]
  }, {
    "tStartMs": 1371010,
    "dDurationMs": 3300,
    "segs": [ {
      "utf8": "Orain hau\nzure garuna nola optimizatu buruzko eztabaida da."
    } ]
  }, {
    "tStartMs": 1374310,
    "dDurationMs": 2270,
    "segs": [ {
      "utf8": "Azkar esnatzen diren"
    } ]
  }, {
    "tStartMs": 1376580,
    "dDurationMs": 2570,
    "segs": [ {
      "utf8": "eta\nohetik irteteko gogoa daukaten jende askok."
    } ]
  }, {
    "tStartMs": 1379150,
    "dDurationMs": 1800,
    "segs": [ {
      "utf8": "Inbidia dut pertsona hau."
    } ]
  }, {
    "tStartMs": 1380950,
    "dDurationMs": 2450,
    "segs": [ {
      "utf8": "Ondo egingo dute,"
    } ]
  }, {
    "tStartMs": 1383400,
    "dDurationMs": 3280,
    "segs": [ {
      "utf8": "ikasketa batean sartuz\nedo zainduz,"
    } ]
  }, {
    "tStartMs": 1386680,
    "dDurationMs": 1490,
    "segs": [ {
      "utf8": "\nzaindu behar duten guztia."
    } ]
  }, {
    "tStartMs": 1388170,
    "dDurationMs": 2260,
    "segs": [ {
      "utf8": "Batzuetan,\n"
    } ]
  }, {
    "tStartMs": 1390430,
    "dDurationMs": 2610,
    "segs": [ {
      "utf8": "posta elektronikoa eta bestelakoak bezalako zeregin arruntagoak dira."
    } ]
  }, {
    "tStartMs": 1393040,
    "dDurationMs": 2590,
    "segs": [ {
      "utf8": "Hona hemen"
    } ]
  }, {
    "tStartMs": 1395630,
    "dDurationMs": 3720,
    "segs": [ {
      "utf8": "garunak nola\nfuntzionatzen duen, arreta,"
    } ]
  }, {
    "tStartMs": 1399350,
    "dDurationMs": 1560,
    "segs": [ {
      "utf8": "ikaskuntza eta sormenari buruzko arau bat gutxi gorabehera."
    } ]
  }, {
    "tStartMs": 1400910,
    "dDurationMs": 3710,
    "segs": [ {
      "utf8": "Eta\naskoz gehiago eztabaidatuko dut hurrengo ataletan."
    } ]
  }, {
    "tStartMs": 1404620,
    "dDurationMs": 2680,
    "segs": [ {
      "utf8": "Orokorrean alerta handiko egoerak,"
    } ]
  }, {
    "tStartMs": 1407300,
    "dDurationMs": 1810,
    "segs": [ {
      "utf8": "oso erne gaudenean,"
    } ]
  }, {
    "tStartMs": 1409110,
    "dDurationMs": 3300,
    "segs": [ {
      "utf8": "bikainak dira estrategia ezartzeko."
    } ]
  }, {
    "tStartMs": 1412410,
    "dDurationMs": 2400,
    "segs": [ {
      "utf8": "Dagoeneko zerbait egiten dakigunean."
    } ]
  }, {
    "tStartMs": 1414810,
    "dDurationMs": 2320,
    "segs": [ {
      "utf8": "Eta"
    } ]
  }, {
    "tStartMs": 1417130,
    "dDurationMs": 4133,
    "segs": [ {
      "utf8": "\nelementu zuzenak kutxa egokietan konektatzea besterik ez da."
    } ]
  }, {
    "tStartMs": 1422300,
    "dDurationMs": 2650,
    "segs": [ {
      "utf8": "Lehenago hitz egin dut\niraupenari, bideari eta emaitzari buruz,"
    } ]
  }, {
    "tStartMs": 1424950,
    "dDurationMs": 1030,
    "segs": [ {
      "utf8": ""
    } ]
  }, {
    "tStartMs": 1425980,
    "dDurationMs": 2150,
    "segs": [ {
      "utf8": "nahita\nkontzienteko garunak"
    } ]
  }, {
    "tStartMs": 1428130,
    "dDurationMs": 2950,
    "segs": [ {
      "utf8": "\nzeregin jakin batzuk egiteko,"
    } ]
  }, {
    "tStartMs": 1431080,
    "dDurationMs": 1850,
    "segs": [ {
      "utf8": "baita zeregin kognitiboak ere, asmatu nahi dituen hiru gauzez.  Ondo atseden hartzen dugunean oso onak garen"
    } ]
  }, {
    "tStartMs": 1432930,
    "dDurationMs": 3210,
    "segs": [ {
      "utf8": "gauza mota da\n"
    } ]
  }, {
    "tStartMs": 1436140,
    "dDurationMs": 2620,
    "segs": [ {
      "utf8": ", eta kontzentratuta gaude."
    } ]
  }, {
    "tStartMs": 1438760,
    "dDurationMs": 1530,
    "segs": [ {
      "utf8": "Gure kitzikapen autonomikoan"
    } ]
  }, {
    "tStartMs": 1440290,
    "dDurationMs": 3170,
    "segs": [ {
      "utf8": "edo gure ernetasuna\nmaila altuan dago.  Zure eguneko"
    } ]
  }, {
    "tStartMs": 1443460,
    "dDurationMs": 3420,
    "segs": [ {
      "utf8": "\nerne-fase horretan sartzen ari den norbait bazara"
    } ]
  }, {
    "tStartMs": 1446880,
    "dDurationMs": 2740,
    "segs": [ {
      "utf8": ", oso goiz, esnatu eta\nberehala,"
    } ]
  }, {
    "tStartMs": 1449620,
    "dDurationMs": 3640,
    "segs": [ {
      "utf8": "momentu bikaina da ikerketak dioen\ngauzetara zuzenean sartzeko,"
    } ]
  }, {
    "tStartMs": 1453260,
    "dDurationMs": 1530,
    "segs": [ {
      "utf8": ""
    } ]
  }, {
    "tStartMs": 1454790,
    "dDurationMs": 1910,
    "segs": [ {
      "utf8": "dagoeneko ezagutzen dituzun, estrategia duzula"
    } ]
  }, {
    "tStartMs": 1456700,
    "dDurationMs": 2440,
    "segs": [ {
      "utf8": "eta besterik gabe.  estrategia ezarri nahi."
    } ]
  }, {
    "tStartMs": 1459140,
    "dDurationMs": 1990,
    "segs": [ {
      "utf8": "Hau da, funtsean,"
    } ]
  }, {
    "tStartMs": 1461130,
    "dDurationMs": 2320,
    "segs": [ {
      "utf8": "oi,\nbadakizu,"
    } ]
  }, {
    "tStartMs": 1463450,
    "dDurationMs": 1510,
    "segs": [ {
      "utf8": "egun osoan zehar zeregin gogorrenak edo kritikoenak egin behar dituzula ideiatik aldendu naiz."
    } ]
  }, {
    "tStartMs": 1464960,
    "dDurationMs": 2340,
    "segs": [ {
      "utf8": "Batzuetan\nzeregin gogorrenak eta kritikoenak"
    } ]
  }, {
    "tStartMs": 1467300,
    "dDurationMs": 2220,
    "segs": [ {
      "utf8": "sormena eskatzen duten lanak dira."
    } ]
  }, {
    "tStartMs": 1469520,
    "dDurationMs": 3520,
    "segs": [ {
      "utf8": "Eta laster sormenaz"
    } ]
  }, {
    "tStartMs": 1473040,
    "dDurationMs": 2030,
    "segs": [ {
      "utf8": "eta hari lotutako zereginez hitz egingo dugunez,"
    } ]
  }, {
    "tStartMs": 1475070,
    "dDurationMs": 3230,
    "segs": [ {
      "utf8": "sarritan etortzen zaizkigu hobekien"
    } ]
  }, {
    "tStartMs": 1478300,
    "dDurationMs": 1950,
    "segs": [ {
      "utf8": "edo garuna da hori lortzeko onena,"
    } ]
  }, {
    "tStartMs": 1480250,
    "dDurationMs": 4170,
    "segs": [ {
      "utf8": "lasaitasun egoeran\nedo logura samarrean gaudenean."
    } ]
  }, {
    "tStartMs": 1484420,
    "dDurationMs": 1370,
    "segs": [ {
      "utf8": "Zein da interesgarria den zerbait"
    } ]
  }, {
    "tStartMs": 1485790,
    "dDurationMs": 1310,
    "segs": [ {
      "utf8": "sartuko dugun horretan."
    } ]
  }, {
    "tStartMs": 1487100,
    "dDurationMs": 1400,
    "segs": [ {
      "utf8": "Baina ni, adibidez,"
    } ]
  }, {
    "tStartMs": 1488500,
    "dDurationMs": 2730,
    "segs": [ {
      "utf8": "jaikitzen naiz, ez nago oso\nerne, lehenik eta behin."
    } ]
  }, {
    "tStartMs": 1491230,
    "dDurationMs": 1937,
    "segs": [ {
      "utf8": "Beraz, nire garuna"
    } ]
  }, {
    "tStartMs": 1493167,
    "dDurationMs": 1783,
    "segs": [ {
      "utf8": "eta nire pentsamenduak antolatzen saiatzen naiz."
    } ]
  }, {
    "tStartMs": 1494950,
    "dDurationMs": 2050,
    "segs": [ {
      "utf8": "Ez da niretzat"
    } ]
  }, {
    "tStartMs": 1497000,
    "dDurationMs": 2040,
    "segs": [ {
      "utf8": "mezu elektronikoei oso modu linealean erantzuteko"
    } ]
  }, {
    "tStartMs": 1499040,
    "dDurationMs": 2010,
    "segs": [ {
      "utf8": "edo kalkuluak egiteko garaia."
    } ]
  }, {
    "tStartMs": 1501050,
    "dDurationMs": 1237,
    "segs": [ {
      "utf8": "Hori bi ordu geroago dator."
    } ]
  }, {
    "tStartMs": 1502287,
    "dDurationMs": 2483,
    "segs": [ {
      "utf8": "Eta uste dut jende askok\nerlazionatuko duela,"
    } ]
  }, {
    "tStartMs": 1504770,
    "dDurationMs": 3900,
    "segs": [ {
      "utf8": "goiz erdian jende askok\n"
    } ]
  }, {
    "tStartMs": 1508670,
    "dDurationMs": 1810,
    "segs": [ {
      "utf8": "erne eta arretarako gailurra lortzeko joera du."
    } ]
  }, {
    "tStartMs": 1510480,
    "dDurationMs": 4290,
    "segs": [ {
      "utf8": "Orain, askotan galdera jasotzen dut eta esango"
    } ]
  }, {
    "tStartMs": 1514770,
    "dDurationMs": 1990,
    "segs": [ {
      "utf8": "dudan hau\nzuzenean lotuta dago"
    } ]
  }, {
    "tStartMs": 1516760,
    "dDurationMs": 3080,
    "segs": [ {
      "utf8": "\nhoni buruz lortu ditudan ehunka galderekin."
    } ]
  }, {
    "tStartMs": 1519840,
    "dDurationMs": 3280,
    "segs": [ {
      "utf8": "Hondoko musika erabili behar al dut\nikasteko?  Ondoan eraiki"
    } ]
  }, {
    "tStartMs": 1523120,
    "dDurationMs": 3240,
    "segs": [ {
      "utf8": "behar al dut, badakizu\n?"
    } ]
  }, {
    "tStartMs": 1526360,
    "dDurationMs": 1500,
    "segs": [ {
      "utf8": "Gauza ona ala txarra da hori?"
    } ]
  }, {
    "tStartMs": 1527860,
    "dDurationMs": 2430,
    "segs": [ {
      "utf8": "Hobe al da\nerabat isiltasunean egotea, eta abar?"
    } ]
  }, {
    "tStartMs": 1530290,
    "dDurationMs": 940,
    "segs": [ {
      "utf8": "Orain hau aldatu egingo da."
    } ]
  }, {
    "tStartMs": 1531230,
    "dDurationMs": 2280,
    "segs": [ {
      "utf8": "Batzuek beren buruaren zarata jasaten dute"
    } ]
  }, {
    "tStartMs": 1533510,
    "dDurationMs": 1520,
    "segs": [ {
      "utf8": "besteek baino askoz hobeto."
    } ]
  }, {
    "tStartMs": 1535030,
    "dDurationMs": 2570,
    "segs": [ {
      "utf8": "Beste pertsona batzuek\nhondoko zarata izateak"
    } ]
  }, {
    "tStartMs": 1537600,
    "dDurationMs": 1483,
    "segs": [ {
      "utf8": "hori bertan behera uzten laguntzen duela uste dute."
    } ]
  }, {
    "tStartMs": 1539920,
    "dDurationMs": 1400,
    "segs": [ {
      "utf8": "Baina bada arau sinple bat"
    } ]
  }, {
    "tStartMs": 1541320,
    "dDurationMs": 1420,
    "segs": [ {
      "utf8": "erabil daitekeena,"
    } ]
  }, {
    "tStartMs": 1542740,
    "dDurationMs": 2110,
    "segs": [ {
      "utf8": "izan ere, nire esperientzia behintzat,"
    } ]
  }, {
    "tStartMs": 1544850,
    "dDurationMs": 2770,
    "segs": [ {
      "utf8": "batzuetan atzeko\nmusika, hondoko zarata"
    } ]
  }, {
    "tStartMs": 1547620,
    "dDurationMs": 2510,
    "segs": [ {
      "utf8": "oso lagungarria izaten da fokua uzteko."
    } ]
  }, {
    "tStartMs": 1550130,
    "dDurationMs": 2610,
    "segs": [ {
      "utf8": "Eta beste batzuetan, oso distraigarria da."
    } ]
  }, {
    "tStartMs": 1552740,
    "dDurationMs": 2120,
    "segs": [ {
      "utf8": "Beraz, zerk zuzentzen du hori?"
    } ]
  }, {
    "tStartMs": 1554860,
    "dDurationMs": 2400,
    "segs": [ {
      "utf8": "Bada, geure buruari galdetu behar diogu,"
    } ]
  }, {
    "tStartMs": 1557260,
    "dDurationMs": 4290,
    "segs": [ {
      "utf8": "zer dago jatorrian, foku\nezaren jatorrian?"
    } ]
  }, {
    "tStartMs": 1561550,
    "dDurationMs": 2890,
    "segs": [ {
      "utf8": "Gure foku eza\ngure kitzikapen autonomikoa"
    } ]
  }, {
    "tStartMs": 1564440,
    "dDurationMs": 2140,
    "segs": [ {
      "utf8": "edo gure ernetasuna oso-oso altua delako bada."
    } ]
  }, {
    "tStartMs": 1566580,
    "dDurationMs": 1250,
    "segs": [ {
      "utf8": "Kafe apur bat gehiegi hartu genuen,"
    } ]
  }, {
    "tStartMs": 1567830,
    "dDurationMs": 2580,
    "segs": [ {
      "utf8": "edo halakorik badago,\ngehiegi lo egin genuen"
    } ]
  }, {
    "tStartMs": 1570410,
    "dDurationMs": 2250,
    "segs": [ {
      "utf8": "edo oso estresatuta\nedo benetan aktibatuta gaude,"
    } ]
  }, {
    "tStartMs": 1572660,
    "dDurationMs": 2620,
    "segs": [ {
      "utf8": "eta ezin omen gara zentratu."
    } ]
  }, {
    "tStartMs": 1575280,
    "dDurationMs": 1780,
    "segs": [ {
      "utf8": "Kasu horretan,"
    } ]
  }, {
    "tStartMs": 1577060,
    "dDurationMs": 1890,
    "segs": [ {
      "utf8": "hondoko zarata ezabatzea,"
    } ]
  }, {
    "tStartMs": 1578950,
    "dDurationMs": 2240,
    "segs": [ {
      "utf8": "eta benetan isiltasuna lortzen saiatzea,"
    } ]
  }, {
    "tStartMs": 1581190,
    "dDurationMs": 3870,
    "segs": [ {
      "utf8": "\nkitzikapen autonomo hori isildu ahal izateko,"
    } ]
  }, {
    "tStartMs": 1585060,
    "dDurationMs": 1090,
    "segs": [ {
      "utf8": "hoberena izango da"
    } ]
  }, {
    "tStartMs": 1586150,
    "dDurationMs": 3320,
    "segs": [ {
      "utf8": "ikasteko eta"
    } ]
  }, {
    "tStartMs": 1589470,
    "dDurationMs": 1890,
    "segs": [ {
      "utf8": "dagoeneko egiten dakigun gauzak ezartzeko."
    } ]
  }, {
    "tStartMs": 1591360,
    "dDurationMs": 2590,
    "segs": [ {
      "utf8": "Edozein motatako foku-zeregin linealetarako."
    } ]
  }, {
    "tStartMs": 1593950,
    "dDurationMs": 2573,
    "segs": [ {
      "utf8": "Horrek, funtsean, ikastea\nfoku lineal bat"
    } ]
  }, {
    "tStartMs": 1596523,
    "dDurationMs": 1637,
    "segs": [ {
      "utf8": "da, ez zarela zertan,"
    } ]
  }, {
    "tStartMs": 1598160,
    "dDurationMs": 1360,
    "segs": [ {
      "utf8": "denbora guztian ondo aritzea."
    } ]
  }, {
    "tStartMs": 1599520,
    "dDurationMs": 1680,
    "segs": [ {
      "utf8": "Azken aldian akatsak egiteari buruz hitz egin genuen."
    } ]
  }, {
    "tStartMs": 1601200,
    "dDurationMs": 3470,
    "segs": [ {
      "utf8": "Beraz, arau orokor gisa,\noso gakoa bazara,"
    } ]
  }, {
    "tStartMs": 1604670,
    "dDurationMs": 2820,
    "segs": [ {
      "utf8": "orduan isiltasuna eta isiltasuna\nlagungarriak izango dira."
    } ]
  }, {
    "tStartMs": 1607490,
    "dDurationMs": 1763,
    "segs": [ {
      "utf8": "Izan ere, oso giltzapean bazaude,"
    } ]
  }, {
    "tStartMs": 1610120,
    "dDurationMs": 1710,
    "segs": [ {
      "utf8": "zirkuitu jakin bat,"
    } ]
  }, {
    "tStartMs": 1611830,
    "dDurationMs": 1920,
    "segs": [ {
      "utf8": "ganglio basalekin lotutakoa,"
    } ]
  }, {
    "tStartMs": 1613750,
    "dDurationMs": 2010,
    "segs": [ {
      "utf8": "errazago hasten da abiarazten."
    } ]
  }, {
    "tStartMs": 1615760,
    "dDurationMs": 1940,
    "segs": [ {
      "utf8": "Eta zirkuitu honi buruz,\nsakon hitz egingo dut."
    } ]
  }, {
    "tStartMs": 1617700,
    "dDurationMs": 2730,
    "segs": [ {
      "utf8": "Baina, joan, no-go zirkuitua deitzen da."
    } ]
  }, {
    "tStartMs": 1620430,
    "dDurationMs": 2400,
    "segs": [ {
      "utf8": "\nGure burmuina lotzen duten zirkuituak ditugu,"
    } ]
  }, {
    "tStartMs": 1622830,
    "dDurationMs": 2440,
    "segs": [ {
      "utf8": "gure garuneko gure egiturarekin, hain\n"
    } ]
  }, {
    "tStartMs": 1625270,
    "dDurationMs": 2050,
    "segs": [ {
      "utf8": "zuzen ere\negitura bilduma bat den ganglio basalak deitzen direnak."
    } ]
  }, {
    "tStartMs": 1627320,
    "dDurationMs": 2600,
    "segs": [ {
      "utf8": "Eta buruburua,"
    } ]
  }, {
    "tStartMs": 1629920,
    "dDurationMs": 2410,
    "segs": [ {
      "utf8": "pentsamendu eta pentsamendu arrazionalean\neta plangintzan eta ekintzan parte hartzen duena,"
    } ]
  }, {
    "tStartMs": 1632330,
    "dDurationMs": 2790,
    "segs": [ {
      "utf8": "zer egin behar dudan planifikatzen saiatzen ari da beti,"
    } ]
  }, {
    "tStartMs": 1635120,
    "dDurationMs": 1237,
    "segs": [ {
      "utf8": "eta gero ekintza hori gauzatzen."
    } ]
  }, {
    "tStartMs": 1636357,
    "dDurationMs": 2553,
    "segs": [ {
      "utf8": "Eta ganglio basalak\noso inplikatuta daude"
    } ]
  }, {
    "tStartMs": 1638910,
    "dDurationMs": 833,
    "segs": [ {
      "utf8": "eztabaida horretan."
    } ]
  }, {
    "tStartMs": 1639743,
    "dDurationMs": 2527,
    "segs": [ {
      "utf8": "Elkarrekiko\nkomunikazio begizta bat dago,"
    } ]
  }, {
    "tStartMs": 1642270,
    "dDurationMs": 2170,
    "segs": [ {
      "utf8": "albahaka ganglioen eta kortexaren artean."
    } ]
  }, {
    "tStartMs": 1644440,
    "dDurationMs": 1900,
    "segs": [ {
      "utf8": "Oinarrizko ganglioek kortexarekin lotura multzo bat dute"
    } ]
  }, {
    "tStartMs": 1646340,
    "dDurationMs": 1880,
    "segs": [ {
      "utf8": ","
    } ]
  }, {
    "tStartMs": 1648220,
    "dDurationMs": 2110,
    "segs": [ {
      "utf8": "eta kortexak goa errazten duen basal ganglioetara itzultzen da"
    } ]
  }, {
    "tStartMs": 1650330,
    "dDurationMs": 2530,
    "segs": [ {
      "utf8": "."
    } ]
  }, {
    "tStartMs": 1652860,
    "dDurationMs": 2380,
    "segs": [ {
      "utf8": "Ekintza errazten du."
    } ]
  }, {
    "tStartMs": 1655240,
    "dDurationMs": 2543,
    "segs": [ {
      "utf8": "Eta molekulak,\ndopamina neuromodulatzaileak,"
    } ]
  }, {
    "tStartMs": 1658870,
    "dDurationMs": 2000,
    "segs": [ {
      "utf8": "goaren aktibazioa eragiten du."
    } ]
  }, {
    "tStartMs": 1660870,
    "dDurationMs": 2490,
    "segs": [ {
      "utf8": "\nGauza gehiago egiteko gogoa piztu ohi digu."
    } ]
  }, {
    "tStartMs": 1663360,
    "dDurationMs": 3090,
    "segs": [ {
      "utf8": "Ekintzarako alborapena egin ohi digu."
    } ]
  }, {
    "tStartMs": 1666450,
    "dDurationMs": 1910,
    "segs": [ {
      "utf8": "Bide batez, dopamina"
    } ]
  }, {
    "tStartMs": 1668360,
    "dDurationMs": 1700,
    "segs": [ {
      "utf8": "D1 errezeptore izeneko zerbaitekin lotzen da.  Dopamina hartzaile"
    } ]
  }, {
    "tStartMs": 1670060,
    "dDurationMs": 2100,
    "segs": [ {
      "utf8": "mota jakin bat besterik ez\n."
    } ]
  }, {
    "tStartMs": 1672160,
    "dDurationMs": 2280,
    "segs": [ {
      "utf8": "Jakin nahi duzuenontzat."
    } ]
  }, {
    "tStartMs": 1674440,
    "dDurationMs": 1820,
    "segs": [ {
      "utf8": "No-go bidea,"
    } ]
  }, {
    "tStartMs": 1676260,
    "dDurationMs": 2380,
    "segs": [ {
      "utf8": "ganglio basaletako bidea"
    } ]
  }, {
    "tStartMs": 1678640,
    "dDurationMs": 2413,
    "segs": [ {
      "utf8": "eta ekintza kentzen duen kortexak,"
    } ]
  }, {
    "tStartMs": 1682130,
    "dDurationMs": 1930,
    "segs": [ {
      "utf8": "dopamina"
    } ]
  }, {
    "tStartMs": 1684060,
    "dDurationMs": 2830,
    "segs": [ {
      "utf8": "\nD2 hartzaile izeneko beste hartzaile honekin lotzea dakar."
    } ]
  }, {
    "tStartMs": 1686890,
    "dDurationMs": 1430,
    "segs": [ {
      "utf8": "Orain D1, D2 errezeptoreak,"
    } ]
  }, {
    "tStartMs": 1688320,
    "dDurationMs": 1300,
    "segs": [ {
      "utf8": "ezin duzu kontzienteki erabaki,"
    } ]
  }, {
    "tStartMs": 1689620,
    "dDurationMs": 2950,
    "segs": [ {
      "utf8": "oh, nire D1\nerrezeptoreak eta nire D2 errezeptoreak"
    } ]
  }, {
    "tStartMs": 1692570,
    "dDurationMs": 990,
    "segs": [ {
      "utf8": "aktiboak izatea bakarrik nahi dut."
    } ]
  }, {
    "tStartMs": 1693560,
    "dDurationMs": 1890,
    "segs": [ {
      "utf8": "Pentsatu behar duzu"
    } ]
  }, {
    "tStartMs": 1695450,
    "dDurationMs": 4480,
    "segs": [ {
      "utf8": "zein motatako gogamen- eta gorputz-egoerak"
    } ]
  }, {
    "tStartMs": 1699930,
    "dDurationMs": 4013,
    "segs": [ {
      "utf8": "errazten duten goa, eta zeintzuk\nerrazten duten no-go."
    } ]
  }, {
    "tStartMs": 1705060,
    "dDurationMs": 1530,
    "segs": [ {
      "utf8": "Orain hori oso garrantzitsua da,"
    } ]
  }, {
    "tStartMs": 1706590,
    "dDurationMs": 1930,
    "segs": [ {
      "utf8": "zeren eta lan zentratua egiteak,"
    } ]
  }, {
    "tStartMs": 1708520,
    "dDurationMs": 3490,
    "segs": [ {
      "utf8": "plastikotasuna eskuratzeak eta ikasteak gauza"
    } ]
  }, {
    "tStartMs": 1712010,
    "dDurationMs": 3740,
    "segs": [ {
      "utf8": "batzuk egitea dakarrelako\neta beste batzuk ez egitea."
    } ]
  }, {
    "tStartMs": 1715750,
    "dDurationMs": 990,
    "segs": [ {
      "utf8": "Beraz, hona nola funtzionatzen duen."
    } ]
  }, {
    "tStartMs": 1716740,
    "dDurationMs": 2483,
    "segs": [ {
      "utf8": "Eta hona hemen nola aplikatzen dudan\negunero."
    } ]
  }, {
    "tStartMs": 1720360,
    "dDurationMs": 1930,
    "segs": [ {
      "utf8": "Erne egon ohi naizelako,"
    } ]
  }, {
    "tStartMs": 1722290,
    "dDurationMs": 3270,
    "segs": [ {
      "utf8": "lehenengo, goiz erdian edo."
    } ]
  }, {
    "tStartMs": 1725560,
    "dDurationMs": 2200,
    "segs": [ {
      "utf8": "Eta gero, oro har,\nnire kafeina izango dut goiz erdialdean."
    } ]
  }, {
    "tStartMs": 1727760,
    "dDurationMs": 4820,
    "segs": [ {
      "utf8": "Nire aditasun gailurra,\negunaren hasieran,"
    } ]
  }, {
    "tStartMs": 1732580,
    "dDurationMs": 3080,
    "segs": [ {
      "utf8": "\n9:30etik 11:00etara bitartean gertatzen ari zait."
    } ]
  }, {
    "tStartMs": 1735660,
    "dDurationMs": 833,
    "segs": [ {
      "utf8": "Hori ni besterik ez naiz."
    } ]
  }, {
    "tStartMs": 1736493,
    "dDurationMs": 917,
    "segs": [ {
      "utf8": "Beste pertsona batzuek hori bizi dezakete"
    } ]
  }, {
    "tStartMs": 1737410,
    "dDurationMs": 1800,
    "segs": [ {
      "utf8": "ohetik altxatu eta berehala."
    } ]
  }, {
    "tStartMs": 1739210,
    "dDurationMs": 2280,
    "segs": [ {
      "utf8": "Baliteke esna eta joateko prest egotea."
    } ]
  }, {
    "tStartMs": 1741490,
    "dDurationMs": 1350,
