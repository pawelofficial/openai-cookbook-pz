  }, {
    "tStartMs": 2045280,
    "dDurationMs": 1020,
    "segs": [ {
      "utf8": "."
    } ]
  }, {
    "tStartMs": 2046300,
    "dDurationMs": 2050,
    "segs": [ {
      "utf8": "Eta hori da gure entzumen-sistema"
    } ]
  }, {
    "tStartMs": 2048350,
    "dDurationMs": 3140,
    "segs": [ {
      "utf8": "eta gure sistema bisuala\nlotuta daudelako eta benetan"
    } ]
  }, {
    "tStartMs": 2051490,
    "dDurationMs": 1690,
    "segs": [ {
      "utf8": "nabarmentasun sare deritzonaren parte direlako."
    } ]
  }, {
    "tStartMs": 2053180,
    "dDurationMs": 833,
    "segs": [ {
      "utf8": "Hau da, beti ari garela"
    } ]
  }, {
    "tStartMs": 2054013,
    "dDurationMs": 1907,
    "segs": [ {
      "utf8": "gure ingurunea gauzen bila."
    } ]
  }, {
    "tStartMs": 2055920,
    "dDurationMs": 3340,
    "segs": [ {
      "utf8": "Eta\ngure ingurunean eskaneatu beharreko gauza asko ditugunean,"
    } ]
  }, {
    "tStartMs": 2059260,
    "dDurationMs": 2480,
    "segs": [ {
      "utf8": "oro har, gure adimen-maila igotzen da."
    } ]
  }, {
    "tStartMs": 2061740,
    "dDurationMs": 3030,
    "segs": [ {
      "utf8": "Horregatik,\noso gogorrak diren"
    } ]
  }, {
    "tStartMs": 2064770,
    "dDurationMs": 3160,
    "segs": [ {
      "utf8": "edo oso objektu gutxi edo\noso gutxi dituzten inguruneek"
    } ]
  }, {
    "tStartMs": 2067930,
    "dDurationMs": 1820,
    "segs": [ {
      "utf8": "lasai sentiarazten gaituzte,"
    } ]
  }, {
    "tStartMs": 2069750,
    "dDurationMs": 2220,
    "segs": [ {
      "utf8": "gure\nsare nabarmena nolabait itzaltzen delako."
    } ]
  }, {
    "tStartMs": 2071970,
    "dDurationMs": 1160,
    "segs": [ {
      "utf8": "Jende askori ez zaio hori gustatzen."
    } ]
  }, {
    "tStartMs": 2073130,
    "dDurationMs": 2300,
    "segs": [ {
      "utf8": "Meditazio-erretiro batera joango dira,"
    } ]
  }, {
    "tStartMs": 2075430,
    "dDurationMs": 1490,
    "segs": [ {
      "utf8": "edo nahasmen gutxi dagoen ingurune batera joango dira,"
    } ]
  }, {
    "tStartMs": 2076920,
    "dDurationMs": 2340,
    "segs": [ {
      "utf8": "\nbatez ere hiriko jendea."
    } ]
  }, {
    "tStartMs": 2079260,
    "dDurationMs": 860,
    "segs": [ {
      "utf8": "Eta bat-batean, oso,"
    } ]
  }, {
    "tStartMs": 2080120,
    "dDurationMs": 2230,
    "segs": [ {
      "utf8": "oso urduri sentitzen hasten dira."
    } ]
  }, {
    "tStartMs": 2082350,
    "dDurationMs": 1820,
    "segs": [ {
      "utf8": "Eta hori da beren barneko"
    } ]
  }, {
    "tStartMs": 2084170,
    "dDurationMs": 1620,
    "segs": [ {
      "utf8": "kitzikapen autonomiko-maila oso altua delako"
    } ]
  }, {
    "tStartMs": 2085790,
    "dDurationMs": 1790,
    "segs": [ {
      "utf8": "eta ez dutelako\ngauza guzti hauei"
    } ]
  }, {
    "tStartMs": 2087580,
    "dDurationMs": 1050,
    "segs": [ {
      "utf8": "arreta emateko okupatzen."
    } ]
  }, {
    "tStartMs": 2088630,
    "dDurationMs": 2400,
    "segs": [ {
      "utf8": "Eta, beraz, haien nabarmen-sarea\nbarrurantz bihurtzen hasten da."
    } ]
  }, {
    "tStartMs": 2091030,
    "dDurationMs": 3080,
    "segs": [ {
      "utf8": "Exterozepziotik\ninterozepziora pasatzen dira."
    } ]
  }, {
    "tStartMs": 2094110,
    "dDurationMs": 1650,
    "segs": [ {
      "utf8": "Ez dute beren burua kanpora begira,"
    } ]
  }, {
    "tStartMs": 2095760,
    "dDurationMs": 1100,
    "segs": [ {
      "utf8": "euren barrura begiratzen dute"
    } ]
  }, {
    "tStartMs": 2096860,
    "dDurationMs": 1950,
    "segs": [ {
      "utf8": "eta zarata handia dago hor barruan."
    } ]
  }, {
    "tStartMs": 2098810,
    "dDurationMs": 4150,
    "segs": [ {
      "utf8": "Beraz, orokorrean,\n"
    } ]
  }, {
    "tStartMs": 2102960,
    "dDurationMs": 3620,
    "segs": [ {
      "utf8": "\nerne-maila eta antsietate-maila altua izateko joera baduzu,"
    } ]
  }, {
    "tStartMs": 2106580,
    "dDurationMs": 2180,
    "segs": [ {
      "utf8": "eta ez naiz\nantsietate-maila klinikoez ari,"
    } ]
  }, {
    "tStartMs": 2108760,
    "dDurationMs": 2650,
    "segs": [ {
      "utf8": "baina nahiko energia handia izan ohi baduzu,"
    } ]
  }, {
    "tStartMs": 2111410,
    "dDurationMs": 3190,
    "segs": [ {
      "utf8": "bada,  Zalantzarik gabe,\netekin handiagoa aterako diozu"
    } ]
  }, {
    "tStartMs": 2114600,
    "dDurationMs": 990,
    "segs": [ {
      "utf8": "ikasketa-bota batean, joaten"
    } ]
  }, {
    "tStartMs": 2115590,
    "dDurationMs": 4317,
    "segs": [ {
      "utf8": "ikastetik eta\nbaita joan-etorririk gabeko bidea aktibatuz ere."
    } ]
  }, {
    "tStartMs": 2119907,
    "dDurationMs": 1843,
    "segs": [ {
      "utf8": "Eta horrek energia asko eskatzen du."
    } ]
  }, {
    "tStartMs": 2121750,
    "dDurationMs": 2600,
    "segs": [ {
      "utf8": "Eta\nzure ingurunean distrakzio asko dituzunean, ikaskuntzatik distraitzeko"
    } ]
  }, {
    "tStartMs": 2124350,
    "dDurationMs": 2290,
    "segs": [ {
      "utf8": "probabilitate handia dago\n"
    } ]
  }, {
    "tStartMs": 2126640,
    "dDurationMs": 1420,
    "segs": [ {
      "utf8": "."
    } ]
  }, {
    "tStartMs": 2128060,
    "dDurationMs": 2330,
    "segs": [ {
      "utf8": "Orain, pertsona batzuk\nberez lasaiago daude.  Nire"
    } ]
  }, {
    "tStartMs": 2130390,
    "dDurationMs": 3220,
    "segs": [ {
      "utf8": "Bulldog Costello bezalakoak dira\n, oso lasaia dena."
    } ]
  }, {
    "tStartMs": 2133610,
    "dDurationMs": 1000,
    "segs": [ {
      "utf8": "Nahiko leunak dira."
    } ]
  }, {
    "tStartMs": 2134610,
    "dDurationMs": 2890,
    "segs": [ {
      "utf8": "Argiak, lasaiak\neta kontzentratuak dira uneoro."
    } ]
  }, {
    "tStartMs": 2137500,
    "dDurationMs": 3550,
    "segs": [ {
      "utf8": "Eta pertsona horiek, egia esan,\ngutxiago flapable izango dira."
    } ]
  }, {
    "tStartMs": 2141050,
    "dDurationMs": 2960,
    "segs": [ {
      "utf8": "Ez dira\nhondoko zaratak astinduko"
    } ]
  }, {
    "tStartMs": 2144010,
    "dDurationMs": 1670,
    "segs": [ {
      "utf8": "edo ez dira inguruan egongo, badakizue,"
    } ]
  }, {
    "tStartMs": 2145680,
    "dDurationMs": 2280,
    "segs": [ {
      "utf8": "ikasteagatik\nedo ikasten dutenetik"
    } ]
  }, {
    "tStartMs": 2147960,
    "dDurationMs": 3070,
    "segs": [ {
      "utf8": "\nsukaldean dagoen norbaitek lapiko bat jotzen duelako."
    } ]
  }, {
    "tStartMs": 2151030,
    "dDurationMs": 2580,
    "segs": [ {
      "utf8": "Beraz, gutako bakoitzak,\noro har, eskailera"
    } ]
  }, {
    "tStartMs": 2153610,
    "dDurationMs": 3940,
    "segs": [ {
      "utf8": "autonomiko honetan gora eta behera ibiltzeko joera du\n, nolabait esateko,"
    } ]
  }, {
    "tStartMs": 2157550,
    "dDurationMs": 1000,
    "segs": [ {
      "utf8": "eguneko ordu ezberdinetan."
    } ]
  }, {
    "tStartMs": 2158550,
    "dDurationMs": 3410,
    "segs": [ {
      "utf8": "Jende gehienentzat, esnatu eta hiru ordura."
    } ]
  }, {
    "tStartMs": 2161960,
    "dDurationMs": 2510,
    "segs": [ {
      "utf8": "Hiru ordu horiek, ez\nhiru ordu,"
    } ]
  }, {
    "tStartMs": 2164470,
    "dDurationMs": 2340,
    "segs": [ {
      "utf8": "baina hiru orduko\nedukiontzi hori"
    } ]
  }, {
    "tStartMs": 2166810,
    "dDurationMs": 1740,
    "segs": [ {
      "utf8": "\negunean zehar erne egoten diren aldia izan ohi da."
    } ]
  }, {
    "tStartMs": 2168550,
    "dDurationMs": 1240,
    "segs": [ {
      "utf8": "Geroago esango dizut izan ezik,"
    } ]
  }, {
    "tStartMs": 2169790,
    "dDurationMs": 2300,
    "segs": [ {
      "utf8": "lo egin baino lehen,"
    } ]
  }, {
    "tStartMs": 2172090,
    "dDurationMs": 3320,
    "segs": [ {
      "utf8": "oso-\noso erne zauden garai paregabe bati buruz, naturalki."
    } ]
  }, {
    "tStartMs": 2175410,
    "dDurationMs": 3280,
    "segs": [ {
      "utf8": "Beraz, goiz hori hiru\nordu ezinbestekoa da."
    } ]
  }, {
    "tStartMs": 2178690,
    "dDurationMs": 3010,
    "segs": [ {
      "utf8": "Orain, zuetako askok ariketari buruz"
    } ]
  }, {
    "tStartMs": 2181700,
    "dDurationMs": 1560,
    "segs": [ {
      "utf8": "eta noiz egin ariketak galdetuko dizute."
    } ]
  }, {
    "tStartMs": 2183260,
    "dDurationMs": 1180,
    "segs": [ {
      "utf8": "Uste dut"
    } ]
  }, {
    "tStartMs": 2184440,
    "dDurationMs": 1430,
    "segs": [ {
      "utf8": "aurreko podcast-eko pasarte batean aipatu dudala."
    } ]
  }, {
    "tStartMs": 2185870,
    "dDurationMs": 1760,
    "segs": [ {
      "utf8": "Baina, ikerketak erakusten du"
    } ]
  }, {
    "tStartMs": 2187630,
    "dDurationMs": 2980,
    "segs": [ {
      "utf8": "errendimendurako behintzat,\narratsaldeko ariketak hobeak izan daitezkeela"
    } ]
  }, {
    "tStartMs": 2190610,
    "dDurationMs": 1580,
    "segs": [ {
      "utf8": "lesioak saihesteko, etab."
    } ]
  }, {
    "tStartMs": 2192190,
    "dDurationMs": 2153,
    "segs": [ {
      "utf8": "Baina gorputz-tenperatura igotzeari dagokionez,"
    } ]
  }, {
    "tStartMs": 2195720,
    "dDurationMs": 1490,
    "segs": [ {
      "utf8": "eta gorputz-tenperatura bat etortzea"
    } ]
  }, {
    "tStartMs": 2197210,
    "dDurationMs": 1430,
    "segs": [ {
      "utf8": "adimen mentala izateko, etab."
    } ]
  }, {
    "tStartMs": 2198640,
    "dDurationMs": 3390,
    "segs": [ {
      "utf8": "Nahiko argia da,\ngoizean goiz ariketa egiteak,"
    } ]
  }, {
    "tStartMs": 2202030,
    "dDurationMs": 2800,
    "segs": [ {
      "utf8": "ez bakarrik\ngoiz esnatzera bultzatzen gaituela,"
    } ]
  }, {
    "tStartMs": 2204830,
    "dDurationMs": 2430,
    "segs": [ {
      "utf8": "baizik eta\n"
    } ]
  }, {
    "tStartMs": 2207260,
    "dDurationMs": 2980,
    "segs": [ {
      "utf8": "epinefrina eta\nbeste neuromodulatzaile batzuk askatzea ere eragiten duela,"
    } ]
  }, {
    "tStartMs": 2210240,
    "dDurationMs": 2430,
    "segs": [ {
      "utf8": "zeinak kitzikapen"
    } ]
  }, {
    "tStartMs": 2212670,
    "dDurationMs": 2830,
    "segs": [ {
      "utf8": "\nmaila areagotu"
    } ]
  }, {
    "tStartMs": 2215500,
    "dDurationMs": 2180,
    "segs": [ {
      "utf8": "eta areagotu ditugun egoera batean.  zorroztasun mentala"
    } ]
  }, {
    "tStartMs": 2217680,
    "dDurationMs": 2493,
    "segs": [ {
      "utf8": "goizaldean eta\nbaita arratsaldean ere."
    } ]
  }, {
    "tStartMs": 2220173,
    "dDurationMs": 1687,
    "segs": [ {
      "utf8": "Orain, hau oso ona izan daiteke,"
    } ]
  }, {
    "tStartMs": 2221860,
    "dDurationMs": 2880,
    "segs": [ {
      "utf8": "zeren eta\nzure arretaren ikaskuntzaren zatirik handiena"
    } ]
  }, {
    "tStartMs": 2224740,
    "dDurationMs": 1170,
    "segs": [ {
      "utf8": "egunaren hasierara mugatu nahi baduzu,"
    } ]
  }, {
    "tStartMs": 2225910,
    "dDurationMs": 4080,
    "segs": [ {
      "utf8": "egunean goiz ariketak\ntestuinguru neurokimiko bat ezartzen du"
    } ]
  }, {
    "tStartMs": 2229990,
    "dDurationMs": 1740,
    "segs": [ {
      "utf8": "edo joan zaitezke."
    } ]
  }, {
    "tStartMs": 2231730,
    "dDurationMs": 2950,
    "segs": [ {
      "utf8": "\nGo bidearen aktibazioa eragin ohi du."
    } ]
  }, {
    "tStartMs": 2234680,
    "dDurationMs": 1760,
    "segs": [ {
      "utf8": "Eta, beraz, ni bezalakoak, egunean goiz"
    } ]
  }, {
    "tStartMs": 2236440,
    "dDurationMs": 1880,
    "segs": [ {
      "utf8": "\nparte hartzea eta"
    } ]
  }, {
    "tStartMs": 2238320,
    "dDurationMs": 1490,
    "segs": [ {
      "utf8": "ekintzan hastea zaila duzuenontzat, goizean"
    } ]
  }, {
    "tStartMs": 2239810,
    "dDurationMs": 2690,
    "segs": [ {
      "utf8": "goiz ariketa esnatu eta\nordubeteko epean,"
    } ]
  }, {
    "tStartMs": 2242500,
    "dDurationMs": 2990,
    "segs": [ {
      "utf8": "eta, zalantzarik gabe,\nesnatu eta hiru ordu geroago,"
    } ]
  }, {
    "tStartMs": 2245490,
    "dDurationMs": 2570,
    "segs": [ {
      "utf8": "komatxo gehiago emango dizute.  energia"
    } ]
  }, {
    "tStartMs": 2248060,
    "dDurationMs": 850,
    "segs": [ {
      "utf8": "egunean zehar."
    } ]
  }, {
    "tStartMs": 2248910,
    "dDurationMs": 2360,
    "segs": [ {
      "utf8": "\nEkintzarako joera gehiago sentituko zaitu."
    } ]
  }, {
    "tStartMs": 2251270,
    "dDurationMs": 1600,
    "segs": [ {
      "utf8": "Ez zara letargikoa sentituko."
    } ]
  }, {
    "tStartMs": 2252870,
    "dDurationMs": 1950,
    "segs": [ {
      "utf8": "Beraz,"
    } ]
  }, {
    "tStartMs": 2254820,
    "dDurationMs": 2730,
    "segs": [ {
      "utf8": "orain arte konfiguratu dudana berrikusteko,\ngoizeko argia egiten dut,"
    } ]
  }, {
    "tStartMs": 2257550,
    "dDurationMs": 3020,
    "segs": [ {
      "utf8": "kafeina\nesnatu eta bi ordu atzeratzen dut,"
    } ]
  }, {
    "tStartMs": 2260570,
    "dDurationMs": 3040,
    "segs": [ {
      "utf8": "eta, oro har, ariketa egiten saiatzen naiz"
    } ]
  }, {
    "tStartMs": 2263610,
    "dDurationMs": 1950,
    "segs": [ {
      "utf8": "lehenengo orduan edo, hoberena,"
    } ]
  }, {
    "tStartMs": 2265560,
    "dDurationMs": 1890,
    "segs": [ {
      "utf8": "lehenengo hiru orduetan.  esnatzearena."
    } ]
  }, {
    "tStartMs": 2267450,
    "dDurationMs": 2570,
    "segs": [ {
      "utf8": "Eta gero, ikaskuntza bideratu batera joango naiz\n."
    } ]
  }, {
    "tStartMs": 2270020,
    "dDurationMs": 3430,
    "segs": [ {
      "utf8": "Orain, zuetako batzuek idatzi zidaten\neta esan zidaten, goizean goiz ariketa egiten badut"
    } ]
  }, {
    "tStartMs": 2273450,
    "dDurationMs": 1960,
    "segs": [ {
      "utf8": ", gero kolpea sentitzen dut\n."
    } ]
  }, {
    "tStartMs": 2275410,
    "dDurationMs": 2040,
    "segs": [ {
      "utf8": "Ariketa hori oso-oso bizia bada."
    } ]
  }, {
    "tStartMs": 2277450,
    "dDurationMs": 1650,
    "segs": [ {
      "utf8": "Beraz, zure glukogeno guztia agortzen ari zara."
    } ]
  }, {
    "tStartMs": 2279100,
    "dDurationMs": 1990,
    "segs": [ {
      "utf8": "Beraz, deadlift astunak egiten ari zara\n, eta abar."
    } ]
  }, {
    "tStartMs": 2281090,
    "dDurationMs": 3650,
    "segs": [ {
      "utf8": "Litekeena da jan ondoren,\ntalka bat sentitzen hasiko zara."
    } ]
  }, {
    "tStartMs": 2284740,
    "dDurationMs": 2100,
    "segs": [ {
      "utf8": "Beraz, elikaduraren denborarekin lotuta dago."
    } ]
  }, {
    "tStartMs": 2286840,
    "dDurationMs": 2390,
    "segs": [ {
      "utf8": "Eta arau orokor gisa,"
    } ]
  }, {
    "tStartMs": 2289230,
    "dDurationMs": 3340,
    "segs": [ {
      "utf8": "barau-egoeretan eta\nkarbohidrato gutxiko egoeretan,"
    } ]
  }, {
    "tStartMs": 2292570,
    "dDurationMs": 2510,
    "segs": [ {
      "utf8": "ez naiz erloju osoan edo aste osoan keto-dietaz ari,"
    } ]
  }, {
    "tStartMs": 2295080,
    "dDurationMs": 1270,
    "segs": [ {
      "utf8": ""
    } ]
  }, {
    "tStartMs": 2296350,
    "dDurationMs": 2290,
    "segs": [ {
      "utf8": "barau-egoerak baizik,"
    } ]
  }, {
    "tStartMs": 2298640,
    "dDurationMs": 3710,
    "segs": [ {
      "utf8": "eta karbohidrato gutxiko egoerak\nadi egoteko balio dute."
    } ]
  }, {
    "tStartMs": 2302350,
    "dDurationMs": 3410,
    "segs": [ {
      "utf8": "Eta hori da karbohidratoak\ntriptofanoan aberatsak direlako,"
    } ]
  }, {
    "tStartMs": 2305760,
    "dDurationMs": 3150,
    "segs": [ {
      "utf8": "eta logura izateko joera dute\n."
    } ]
  }, {
    "tStartMs": 2308910,
    "dDurationMs": 2830,
    "segs": [ {
      "utf8": "Jakina,\nedozein motatako janari kantitate handiak irenstea, tripa"
    } ]
  }, {
    "tStartMs": 2311740,
    "dDurationMs": 3560,
    "segs": [ {
      "utf8": "betetzen duen edozein substantzia,\nodola"
    } ]
  }, {
    "tStartMs": 2315300,
    "dDurationMs": 833,
    "segs": [ {
      "utf8": "hesteetara bideratuko du."
    } ]
  }, {
    "tStartMs": 2316133,
    "dDurationMs": 1817,
    "segs": [ {
      "utf8": "Beraz, janari asko jaten baduzu"
    } ]
  }, {
    "tStartMs": 2317950,
    "dDurationMs": 1860,
    "segs": [ {
      "utf8": "\nkarbohidrato asko den ala ez kontuan hartu gabe,"
    } ]
  }, {
    "tStartMs": 2319810,
    "dDurationMs": 2490,
    "segs": [ {
      "utf8": "orokorrean\nlo gehiago sentituko zara."
    } ]
  }, {
    "tStartMs": 2322300,
    "dDurationMs": 4290,
    "segs": [ {
      "utf8": "Orain, jende askok,\ndenek barne, janaria erabiltzen dute,"
    } ]
  }, {
    "tStartMs": 2326590,
    "dDurationMs": 3010,
    "segs": [ {
      "utf8": "beren\nkitzikapen autonomiko maila modulatzeko."
    } ]
  }, {
    "tStartMs": 2329600,
    "dDurationMs": 3660,
    "segs": [ {
      "utf8": "Eta normalean jateak\nlasaitasun-egoerarantz mugitzen gaitu."
    } ]
  }, {
    "tStartMs": 2333260,
    "dDurationMs": 2670,
    "segs": [ {
      "utf8": "Eta baraualdiak erne-egoerarantz mugitzen gaitu\n."
    } ]
  }, {
    "tStartMs": 2335930,
    "dDurationMs": 2080,
    "segs": [ {
      "utf8": "Eta hauek ekintza behar duten elikagaiak\n"
    } ]
  }, {
    "tStartMs": 2338010,
    "dDurationMs": 3200,
    "segs": [ {
      "utf8": "aurkitzeko beharrari eta nahiari lotutako zirkuitu zorrotzak dira\n,"
    } ]
  }, {
    "tStartMs": 2341210,
    "dDurationMs": 2310,
    "segs": [ {
      "utf8": "edo"
    } ]
  }, {
    "tStartMs": 2343520,
    "dDurationMs": 2090,
    "segs": [ {
      "utf8": "gure baliabideak"
    } ]
  }, {
    "tStartMs": 2345610,
    "dDurationMs": 1400,
    "segs": [ {
      "utf8": "eta gure energia digestiorantz desbideratzen dituen atseden eta digestio sistema deritzona."
    } ]
  }, {
    "tStartMs": 2347010,
    "dDurationMs": 1350,
    "segs": [ {
      "utf8": "Lasai sentiarazten gaitu."
    } ]
  }, {
    "tStartMs": 2348360,
    "dDurationMs": 3520,
    "segs": [ {
      "utf8": "Beraz, pertsonalki uraren, matearen"
    } ]
  }, {
    "tStartMs": 2351880,
    "dDurationMs": 3030,
    "segs": [ {
      "utf8": "eta kafe beltzarengan oinarritzen naiz eguneko lehenengo gauza,"
    } ]
  }, {
    "tStartMs": 2354910,
    "dDurationMs": 4300,
    "segs": [ {
      "utf8": "ariketa fisikoa egiteko eta\nlaneko lehen txandan sartzeko."
    } ]
  }, {
    "tStartMs": 2359210,
    "dDurationMs": 1550,
    "segs": [ {
      "utf8": "Erneegi nagoela ikusten badut,"
    } ]
  }, {
    "tStartMs": 2360760,
    "dDurationMs": 3040,
    "segs": [ {
      "utf8": "eta, oro har, jateko joera izango dut"
    } ]
  }, {
    "tStartMs": 2363800,
    "dDurationMs": 2320,
    "segs": [ {
      "utf8": "eta\nnire erne-maila jaitsiko dut,"
    } ]
  }, {
    "tStartMs": 2366120,
    "dDurationMs": 1370,
    "segs": [ {
      "utf8": "eta lanean jarraituko dut."
    } ]
  }, {
    "tStartMs": 2367490,
    "dDurationMs": 1250,
    "segs": [ {
      "utf8": "Orain hau auzo kontua da."
    } ]
  }, {
    "tStartMs": 2368740,
    "dDurationMs": 2580,
    "segs": [ {
      "utf8": "Eta jendeak zer egiten dudan galdetu didanez,"
    } ]
  }, {
    "tStartMs": 2371320,
    "dDurationMs": 1940,
    "segs": [ {
      "utf8": "eta ez dut esaten jendeak"
    } ]
  }, {
    "tStartMs": 2373260,
    "dDurationMs": 2120,
    "segs": [ {
      "utf8": "zehatz-mehatz jarraitzen duenik noski, ezta orokorrean ere,"
    } ]
  }, {
    "tStartMs": 2375380,
    "dDurationMs": 1230,
    "segs": [ {
      "utf8": "baina zer egiten dudan esango dizut."
    } ]
  }, {
    "tStartMs": 2376610,
    "dDurationMs": 3330,
    "segs": [ {
      "utf8": "Baliteke\nkafe beltza"
    } ]
  }, {
    "tStartMs": 2379940,
    "dDurationMs": 2330,
    "segs": [ {
      "utf8": "edo mate edaten ari bazara eta\nur asko irensten baduzu,"
    } ]
  }, {
    "tStartMs": 2382270,
    "dDurationMs": 2110,
    "segs": [ {
      "utf8": "\nzertxobait deshidratatzea,"
    } ]
  }, {
    "tStartMs": 2384380,
    "dDurationMs": 2220,
    "segs": [ {
      "utf8": "sodioaren iraizpena dela eta."
    } ]
  }, {
    "tStartMs": 2386600,
    "dDurationMs": 1410,
    "segs": [ {
      "utf8": "Hipertentsiorik ez baduzu,"
    } ]
  }, {
    "tStartMs": 2388010,
    "dDurationMs": 1660,
    "segs": [ {
      "utf8": "gatza oso ona da."
    } ]
  }, {
    "tStartMs": 2389670,
    "dDurationMs": 1870,
    "segs": [ {
      "utf8": "Jende askok uste du"
    } ]
  }, {
    "tStartMs": 2391540,
    "dDurationMs": 2310,
    "segs": [ {
      "utf8": "odoleko azukre gutxi dutela,\ndardarak daudelako eta ezin dutelako pentsatu"
    } ]
  }, {
    "tStartMs": 2393850,
    "dDurationMs": 2810,
    "segs": [ {
      "utf8": "edo buruko mina dutelako\nbenetan sodio gutxi dutenean."
    } ]
  }, {
    "tStartMs": 2396660,
    "dDurationMs": 2510,
    "segs": [ {
      "utf8": "Eta batez ere\nkafeina asko edaten baduzu."
    } ]
  }, {
    "tStartMs": 2399170,
    "dDurationMs": 1510,
    "segs": [ {
      "utf8": "Beraz, gatzaren fede handia dut."
    } ]
  }, {
    "tStartMs": 2400680,
    "dDurationMs": 1941,
    "segs": [ {
      "utf8": "Beraz, goizean ur gazia edaten dut"
    } ]
  }, {
    "tStartMs": 2402621,
    "dDurationMs": 1339,
    "segs": [ {
      "utf8": "\nkafe beltza edaten dudalako."
    } ]
  }, {
    "tStartMs": 2403960,
    "dDurationMs": 2000,
    "segs": [ {
      "utf8": "Eta horrek nire\naditasun maila oso ona mantentzen du."
    } ]
  }, {
    "tStartMs": 2405960,
    "dDurationMs": 2300,
    "segs": [ {
      "utf8": "Beti pentsatu nuen\nodoleko azukrea nahasi nuela."
    } ]
  }, {
    "tStartMs": 2408260,
    "dDurationMs": 3090,
    "segs": [ {
      "utf8": "Eskuak dardarka nituen eta\nez nekien zer gertatzen ari zen.  Kafe"
    } ]
  }, {
    "tStartMs": 2411350,
    "dDurationMs": 2210,
    "segs": [ {
      "utf8": "apur bat edango nuke\neta larriegia sentituko nuke."
    } ]
  }, {
    "tStartMs": 2413560,
    "dDurationMs": 2530,
    "segs": [ {
      "utf8": "Eta bilakatzen da sodio arazoa zela."
    } ]
  }, {
    "tStartMs": 2416090,
    "dDurationMs": 3130,
    "segs": [ {
      "utf8": "Eta\nitsasoko gatz apur batekin ura edaten banuen,"
    } ]
  }, {
    "tStartMs": 2419220,
    "dDurationMs": 2930,
    "segs": [ {
      "utf8": "edo baita mahaiko gatz tipiko batekin ere,"
    } ]
  }, {
    "tStartMs": 2422150,
    "dDurationMs": 2760,
    "segs": [ {
      "utf8": "\nodoleko azukreari dagokionez sendo eroriko nintzateke."
    } ]
  }, {
    "tStartMs": 2424910,
    "dDurationMs": 2800,
    "segs": [ {
      "utf8": "Orain, berriro ere, ez naiz\nmedikua, irakaslea naiz,"
    } ]
  }, {
    "tStartMs": 2427710,
    "dDurationMs": 2930,
    "segs": [ {
      "utf8": "beraz, ez dut ezer errezetatzen,\nbaina gauza asko profesatzen ditut."
    } ]
  }, {
    "tStartMs": 2430640,
    "dDurationMs": 2000,
    "segs": [ {
      "utf8": "Beraz, ez dut nahi diabetesa"
    } ]
  }, {
    "tStartMs": 2432640,
    "dDurationMs": 2240,
    "segs": [ {
      "utf8": "edo odoleko azukrea duten pertsonak errailetatik alde egitea."
    } ]
  }, {
    "tStartMs": 2434880,
    "dDurationMs": 2240,
    "segs": [ {
      "utf8": "\nZure osasunaren erantzule zara, ez ni."
    } ]
  }, {
    "tStartMs": 2437120,
    "dDurationMs": 2250,
    "segs": [ {
      "utf8": "Baina\nhausnartzeko"
    } ]
  }, {
    "tStartMs": 2439370,
    "dDurationMs": 1480,
    "segs": [ {
      "utf8": "eta esperimentatzeko parametro interesgarria da, badakizu,"
    } ]
  }, {
    "tStartMs": 2440850,
    "dDurationMs": 1520,
    "segs": [ {
      "utf8": "baldin eta zure medikuak ondo dagoela esaten badu,"
    } ]
  }, {
    "tStartMs": 2442370,
    "dDurationMs": 2110,
    "segs": [ {
      "utf8": "uste baitut jende askok,"
    } ]
  }, {
    "tStartMs": 2444480,
    "dDurationMs": 1220,
    "segs": [ {
      "utf8": "seguruenik, sodio gehiegi irensten duela,"
    } ]
  }, {
    "tStartMs": 2445700,
    "dDurationMs": 2360,
    "segs": [ {
      "utf8": "baina jende askok\nsodio eskasia izan dezake, batez ere."
    } ]
  }, {
    "tStartMs": 2448060,
    "dDurationMs": 2230,
    "segs": [ {
      "utf8": "barau egiten ari dira."
    } ]
  }, {
    "tStartMs": 2450290,
    "dDurationMs": 3673,
    "segs": [ {
      "utf8": "Normalean nire lehen\notordua egun erdi aldera jaten dut,"
    } ]
  }, {
    "tStartMs": 2454950,
    "dDurationMs": 1810,
    "segs": [ {
      "utf8": "ariketa fisikoa egin dudan ala ez."
    } ]
  }, {
    "tStartMs": 2456760,
    "dDurationMs": 2190,
    "segs": [ {
      "utf8": "Eta janariaren edukia"
    } ]
  }, {
    "tStartMs": 2458950,
    "dDurationMs": 2030,
    "segs": [ {
      "utf8": "benetan oso garrantzitsua da niretzat."
    } ]
  }, {
    "tStartMs": 2460980,
    "dDurationMs": 1630,
    "segs": [ {
      "utf8": "Ez dakit zergatik den hau."
    } ]
  }, {
    "tStartMs": 2462610,
    "dDurationMs": 2490,
    "segs": [ {
      "utf8": "Ez daukat\nhorretarako mekanismo zientifikorik"
    } ]
  }, {
    "tStartMs": 2465100,
    "dDurationMs": 2990,
    "segs": [ {
      "utf8": "baina bazkaltzeko janari beroa jaten badut\n, bazkalostean lo hartzen dut."
    } ]
  }, {
    "tStartMs": 2468090,
    "dDurationMs": 2350,
    "segs": [ {
      "utf8": "Beraz, orokorrean ez dut\njanari berorik jaten bazkaltzeko."
    } ]
  }, {
    "tStartMs": 2470440,
    "dDurationMs": 1820,
    "segs": [ {
      "utf8": "Baliteke zopa pixka bat\nedo horrelako zerbait hartzea."
    } ]
  }, {
    "tStartMs": 2472260,
    "dDurationMs": 3610,
    "segs": [ {
      "utf8": "Baina, oro har,\nkarbohidrato gutxiko bazkari batean oinarritzen naiz."
    } ]
  }, {
    "tStartMs": 2475870,
    "dDurationMs": 2230,
    "segs": [ {
      "utf8": "Haragia edo entsalada\nedo horren aldakuntzaren bat jango dut"
    } ]
  }, {
    "tStartMs": 2478100,
    "dDurationMs": 2483,
    "segs": [ {
      "utf8": "eta fruitu lehorrak eta koipeak eta horrelako gauzak fokuaren edukia"
    } ]
  }, {
    "tStartMs": 2480583,
    "dDurationMs": 2417,
    "segs": [ {
      "utf8": "[ezberdinagatik]\n,"
    } ]
  }, {
    "tStartMs": 2483000,
    "dDurationMs": 2490,
    "segs": [ {
      "utf8": "proteina ona delako nire ustetan."
    } ]
  }, {
    "tStartMs": 2485490,
    "dDurationMs": 1020,
    "segs": [ {
      "utf8": "Eta"
    } ]
  }, {
    "tStartMs": 2486510,
    "dDurationMs": 1160,
    "segs": [ {
      "utf8": "fruta eta barazkiak jaten sinesten dudalako.  Nik"
    } ]
  }, {
    "tStartMs": 2487670,
    "dDurationMs": 833,
    "segs": [ {
      "utf8": "ere hori egiten dut."
    } ]
  }, {
    "tStartMs": 2488503,
    "dDurationMs": 2377,
    "segs": [ {
      "utf8": "\nEgun hasieran oso ariketa gogorra egin badut,"
    } ]
  }, {
    "tStartMs": 2490880,
    "dDurationMs": 2830,
    "segs": [ {
      "utf8": "almidoiak irensten ditut oloa edo arroza"
    } ]
  }, {
    "tStartMs": 2493710,
    "dDurationMs": 1150,
    "segs": [ {
      "utf8": "eta fruta eta horrelakoak."
    } ]
  }, {
    "tStartMs": 2494860,
    "dDurationMs": 1220,
    "segs": [ {
      "utf8": "Orain, zergatik esaten dizut hau guztia?"
    } ]
  }, {
    "tStartMs": 2496080,
    "dDurationMs": 3890,
    "segs": [ {
      "utf8": "Zeren, 100s ez bada 1000 pertsonak galdetzen didate,"
    } ]
  }, {
    "tStartMs": 2499970,
    "dDurationMs": 2070,
    "segs": [ {
      "utf8": "baraualdia ona al da arretarako?"
    } ]
  }, {
    "tStartMs": 2502040,
    "dDurationMs": 2900,
    "segs": [ {
      "utf8": "Eta, hain zuzen ere, baraualdiak\nernetasuna areagotuko du,"
    } ]
  }, {
    "tStartMs": 2504940,
    "dDurationMs": 2750,
    "segs": [ {
      "utf8": "baina janariarekin hain gose\nedo kezkatuta bazaude eta"
    } ]
  }, {
    "tStartMs": 2507690,
    "dDurationMs": 1350,
    "segs": [ {
      "utf8": "ezin baduzu ondo zentratu,"
    } ]
  }, {
    "tStartMs": 2509040,
    "dDurationMs": 1390,
    "segs": [ {
      "utf8": "orduan ez da ona izango ikasteko."
    } ]
  }, {
    "tStartMs": 2510430,
    "dDurationMs": 2680,
    "segs": [ {
      "utf8": "Asaldurarako bakarrik izango da ona."
    } ]
  }, {
    "tStartMs": 2513110,
    "dDurationMs": 2960,
    "segs": [ {
      "utf8": "Orain, markatuko dut"
    } ]
  }, {
    "tStartMs": 2516070,
    "dDurationMs": 1420,
    "segs": [ {
      "utf8": "nire egunean martxa egiten jarraitzea."
    } ]
  }, {
    "tStartMs": 2517490,
    "dDurationMs": 2810,
    "segs": [ {
      "utf8": "Eta hau da, noski, bizi izan nuena."
    } ]
  }, {
    "tStartMs": 2520300,
    "dDurationMs": 1480,
    "segs": [ {
      "utf8": "Pertsona batzuk nahiko desberdinak dira."
    } ]
  }, {
    "tStartMs": 2521780,
    "dDurationMs": 2770,
    "segs": [ {
      "utf8": "Baina, aurkitzen dudana\n14:00 edo 15:00ak aldera da,"
    } ]
  }, {
    "tStartMs": 2524550,
    "dDurationMs": 1300,
    "segs": [ {
      "utf8": "pixka bat aztoratzen hasten naiz."
    } ]
  }, {
    "tStartMs": 2525850,
    "dDurationMs": 1740,
    "segs": [ {
      "utf8": "Lo pixka bat."
    } ]
  }, {
    "tStartMs": 2527590,
    "dDurationMs": 4510,
    "segs": [ {
      "utf8": "\n"
    } ]
  }, {
    "tStartMs": 2532100,
    "dDurationMs": 2800,
    "segs": [ {
      "utf8": "Iraupen-bide, emaitza,\nanalisi benetan zaindua"
    } ]
  }, {
    "tStartMs": 2534900,
    "dDurationMs": 2630,
    "segs": [ {
      "utf8": "eta joan-etorririk gabeko bidea aktibatzea eskatzen duen lanetik aldatzeko joera izango dut."
    } ]
  }, {
    "tStartMs": 2537530,
    "dDurationMs": 2250,
    "segs": [ {
      "utf8": "Hau da,\n"
    } ]
  }, {
    "tStartMs": 2539780,
    "dDurationMs": 2180,
    "segs": [ {
      "utf8": "nire telefonoa begiratzeko edo\nmezu elektronikoak erantzuteko edo beste gauza batzuk egiteko bulkada kentzen saiatzen ari naiz."
    } ]
  }, {
    "tStartMs": 2541960,
    "dDurationMs": 1870,
    "segs": [ {
      "utf8": "Horregatik ez\ndizut mezu elektronikorik bidali arratsaldeko 3:00ak arte"
    } ]
  }, {
    "tStartMs": 2543830,
    "dDurationMs": 1080,
    "segs": [ {
      "utf8": "."
    } ]
  }, {
    "tStartMs": 2544910,
    "dDurationMs": 1040,
    "segs": [ {
      "utf8": "Bide batez, edo"
    } ]
  }, {
    "tStartMs": 2545950,
    "dDurationMs": 2450,
    "segs": [ {
      "utf8": "zure testu-mezuei erantzun,\nedonor zauden hor kanpoan."
    } ]
  }, {
    "tStartMs": 2549630,
    "dDurationMs": 1930,
    "segs": [ {
      "utf8": "Arratsaldearen hasieran, ohiko zeregin arruntagoak"
    } ]
  }, {
    "tStartMs": 2551560,
    "dDurationMs": 3980,
    "segs": [ {
      "utf8": "egin ditzakedala iruditzen zait\n."
    } ]
  }, {
    "tStartMs": 2555540,
    "dDurationMs": 5000,
    "segs": [ {
      "utf8": "Zeren eta, zeregin horiek\nkarga kognitibo txikiagoa eskatzen dute."
    } ]
  }, {
    "tStartMs": 2562080,
    "dDurationMs": 3570,
    "segs": [ {
      "utf8": "Eta gehiago edo gutxiago egin daitezke\n, sekuentzian eta kanpoan."
    } ]
  }, {
    "tStartMs": 2565650,
    "dDurationMs": 1390,
    "segs": [ {
      "utf8": "Pare bat mezu elektroniko erantzun ditzaket hemen,"
    } ]
  }, {
    "tStartMs": 2567040,
    "dDurationMs": 1520,
    "segs": [ {
      "utf8": "agian hor erantzun."
    } ]
  }, {
    "tStartMs": 2568560,
    "dDurationMs": 2480,
    "segs": [ {
      "utf8": "Ez dut\nmodu lineal hutsean egin behar."
    } ]
  }, {
    "tStartMs": 2571040,
    "dDurationMs": 1410,
    "segs": [ {
      "utf8": "Edozein lan lineal"
    } ]
  }, {
    "tStartMs": 2572450,
    "dDurationMs": 2430,
    "segs": [ {
      "utf8": "edo ikaskuntza lanek\narreta handia izango dute."
    } ]
  }, {
    "tStartMs": 2574880,
    "dDurationMs": 3600,
    "segs": [ {
      "utf8": "Eta, normalean, 16:00ak aldera edo,"
    } ]
  }, {
    "tStartMs": 2578480,
    "dDurationMs": 1480,
    "segs": [ {
      "utf8": "bi gauza egiten ditut."
    } ]
  }, {
    "tStartMs": 2579960,
    "dDurationMs": 910,
    "segs": [ {
      "utf8": "Batzuetan pixka bat lehenago."
    } ]
  }, {
    "tStartMs": 2580870,
    "dDurationMs": 2430,
    "segs": [ {
      "utf8": "Batzuetan pixka bat geroago\nbaina bi gauza egiten ditut."
    } ]
  }, {
    "tStartMs": 2583300,
    "dDurationMs": 2190,
    "segs": [ {
      "utf8": "Bata, hidratatzen dudala ziurtatzen dudala da."
    } ]
  }, {
    "tStartMs": 2585490,
    "dDurationMs": 2060,
    "segs": [ {
      "utf8": "Zeren ariketa fisikoa egiten baduzu\neta jaten baduzu,"
    } ]
  }, {
    "tStartMs": 2587550,
    "dDurationMs": 2200,
    "segs": [ {
      "utf8": "janari hori digeritu behar duzu, etab.  Ziur"
    } ]
  }, {
    "tStartMs": 2589750,
    "dDurationMs": 890,
    "segs": [ {
      "utf8": "nago hidratatzen dudala."
    } ]
  }, {
    "tStartMs": 2590640,
    "dDurationMs": 1090,
    "segs": [ {
      "utf8": "Beraz, ura edaten dut.  Arratsaldean kafea"
    } ]
  }, {
    "tStartMs": 2591730,
    "dDurationMs": 2520,
    "segs": [ {
      "utf8": "edaten saiatzen naiz\n."
    } ]
  }, {
    "tStartMs": 2594250,
    "dDurationMs": 1210,
    "segs": [ {
      "utf8": "Hau gauza berria da niretzat."
    } ]
  }, {
    "tStartMs": 2595460,
    "dDurationMs": 3380,
    "segs": [ {
      "utf8": "Batzuetan egiten dut, baina\nsaiatzen naiz horri eusten."
    } ]
  }, {
    "tStartMs": 2598840,
    "dDurationMs": 2050,
    "segs": [ {
      "utf8": "Eta gero beti egiten dut,"
    } ]
  }, {
    "tStartMs": 2600890,
    "dDurationMs": 3050,
    "segs": [ {
      "utf8": "lorik gabeko atsedenerako protokolo bat\narratsaldean."
    } ]
  }, {
    "tStartMs": 2603940,
    "dDurationMs": 2980,
    "segs": [ {
      "utf8": "Hau batzuetan 10 minutuko\nyoga nidra motako protokoloa da,"
    } ]
  }, {
    "tStartMs": 2606920,
    "dDurationMs": 2850,
    "segs": [ {
      "utf8": "edo 30 minutuko yoga nidra motako protokoloa.  Harremanik"
    } ]
  }, {
    "tStartMs": 2609770,
    "dDurationMs": 2000,
    "segs": [ {
      "utf8": "\nez daukadan protokoloak dira,"
    } ]
  }, {
    "tStartMs": 2611770,
    "dDurationMs": 1850,
    "segs": [ {
      "utf8": "ez dudala negozio harremanik."
    } ]
  }, {
    "tStartMs": 2613620,
    "dDurationMs": 1950,
    "segs": [ {
      "utf8": "Urteak daramatzat haiek egiten."
    } ]
  }, {
    "tStartMs": 2615570,
    "dDurationMs": 2060,
    "segs": [ {
      "utf8": "Gidoi bat entzutea dakar."
    } ]
  }, {
    "tStartMs": 2617630,
    "dDurationMs": 1380,
    "segs": [ {
      "utf8": "Estekak berriro emango ditugu"
    } ]
  }, {
    "tStartMs": 2619010,
    "dDurationMs": 2570,
    "segs": [ {
      "utf8": "aurretik eman ditugun arren."
    } ]
  }, {
    "tStartMs": 2621580,
    "dDurationMs": 2420,
    "segs": [ {
      "utf8": "Edo hipnosi protokolo bat egingo dut,"
    } ]
  }, {
    "tStartMs": 2624000,
    "dDurationMs": 1000,
    "segs": [ {
      "utf8": "Reveri Health-etik,"
    } ]
  }, {
    "tStartMs": 2625000,
    "dDurationMs": 3170,
    "segs": [ {
      "utf8": "hau da, nire lankide\nDavid Spiegel-en webgunea, entzun ditzakezun"
    } ]
  }, {
    "tStartMs": 2628170,
    "dDurationMs": 2320,
    "segs": [ {
      "utf8": "doako hipnosi aplikazio"
    } ]
  }, {
    "tStartMs": 2630490,
    "dDurationMs": 2480,
    "segs": [ {
      "utf8": "edo gidoi hauek dituena."
    } ]
  }, {
    "tStartMs": 2632970,
    "dDurationMs": 3610,
    "segs": [ {
      "utf8": "Eta horiek\noso sakoneko atseden egoerara eramaten naute."
    } ]
  }, {
    "tStartMs": 2636580,
    "dDurationMs": 1990,
    "segs": [ {
      "utf8": "Batzuetan hainbeste non lokartzen naizen."
    } ]
  }, {
    "tStartMs": 2638570,
    "dDurationMs": 1080,
    "segs": [ {
      "utf8": "Eta beti jartzen dut alarma bat,"
    } ]
  }, {
    "tStartMs": 2639650,
    "dDurationMs": 1870,
    "segs": [ {
      "utf8": "\n90 minutu baino gehiago lo ez egiteko."
    } ]
  }, {
    "tStartMs": 2641520,
    "dDurationMs": 2100,
    "segs": [ {
      "utf8": "Baina normalean\n30 minutu inguru irauten du."
    } ]
  }, {
    "tStartMs": 2643620,
    "dDurationMs": 1850,
    "segs": [ {
      "utf8": "Eta hori egiten dut, niretzat"
    } ]
  }, {
    "tStartMs": 2645470,
    "dDurationMs": 2320,
    "segs": [ {
      "utf8": "arratsaldeko 4:30ak aldera,"
    } ]
  }, {
    "tStartMs": 2647790,
    "dDurationMs": 2520,
    "segs": [ {
      "utf8": "funtsean ezer ez egiteko gai naizelako."
    } ]
  }, {
    "tStartMs": 2650310,
    "dDurationMs": 2460,
    "segs": [ {
      "utf8": "Costello osoa naiz."
    } ]
  }, {
    "tStartMs": 2652770,
    "dDurationMs": 3100,
    "segs": [ {
      "utf8": "Ezin dut pentsatu, ezin dut egin,"
    } ]
  }, {
    "tStartMs": 2655870,
    "dDurationMs": 1470,
    "segs": [ {
      "utf8": "ezin diot mezu elektronikoari erantzun.  Funtzionatzeko gaitasuna"
    } ]
  }, {
    "tStartMs": 2657340,
    "dDurationMs": 4150,
    "segs": [ {
      "utf8": "erabat gainditu dut\n."
    } ]
  }, {
    "tStartMs": 2661490,
    "dDurationMs": 2430,
    "segs": [ {
      "utf8": "Pertsonalki akats bat iruditzen zait,"
    } ]
  }, {
    "tStartMs": 2663920,
    "dDurationMs": 1070,
    "segs": [ {
      "utf8": "puntu horretara arte,"
    } ]
  }, {
    "tStartMs": 2664990,
    "dDurationMs": 3250,
    "segs": [ {
      "utf8": "espresso bikoitza jaistea\neta oso gogorra kargatzea."
    } ]
  }, {
    "tStartMs": 2668240,
    "dDurationMs": 1110,
    "segs": [ {
      "utf8": "Niri ez dit balio.  Lo egiteko ordutegia"
    } ]
  }, {
    "tStartMs": 2669350,
    "dDurationMs": 2070,
    "segs": [ {
      "utf8": "benetan eten egiten dut\n."
    } ]
  }, {
    "tStartMs": 2671420,
    "dDurationMs": 1710,
    "segs": [ {
      "utf8": "\nGauza ezberdin asko asaldatzen ditut azkenean."
    } ]
  }, {
    "tStartMs": 2673130,
    "dDurationMs": 3310,
    "segs": [ {
      "utf8": "Beraz, niretzat,\nlorik gabeko atseden sakoneko protokoloa egiten dut."
    } ]
  }, {
    "tStartMs": 2676440,
    "dDurationMs": 2790,
    "segs": [ {
      "utf8": "Benetan laguntzen dit gero\nlo hartu behar dudanean.  Era"
    } ]
  }, {
    "tStartMs": 2679230,
    "dDurationMs": 3150,
    "segs": [ {
      "utf8": "guztietako\ngauzetan laguntzen du, lehen aipatu dudan bezala."
    } ]
  }, {
    "tStartMs": 2682380,
    "dDurationMs": 3250,
    "segs": [ {
      "utf8": "Baina normalean\nhortik apur bat aztoratuta ateratzen naiz"
    } ]
  }, {
    "tStartMs": 2685630,
    "dDurationMs": 4340,
    "segs": [ {
      "utf8": "edo beste\negun osoa dudala sentitzen dut, bigarren haizea."
    } ]
  }, {
    "tStartMs": 2689970,
    "dDurationMs": 1830,
    "segs": [ {
      "utf8": "Lan egin nezakeen bezala."
    } ]
  }, {
    "tStartMs": 2691800,
    "dDurationMs": 3620,
    "segs": [ {
      "utf8": "Eta gero\nbigarren ikaskuntza bat egingo dut.  Zerbaiten analisi lineala dakarren lan"
    } ]
  }, {
    "tStartMs": 2695420,
    "dDurationMs": 1390,
    "segs": [ {
      "utf8": "mota bat egingo dut"
    } ]
  }, {
    "tStartMs": 2696810,
    "dDurationMs": 3580,
    "segs": [ {
      "utf8": "\n."
    } ]
  }, {
    "tStartMs": 2700390,
    "dDurationMs": 4323,
    "segs": [ {
      "utf8": "Beraz, agian zenbakizko lana edo\nzerbait ikasten saiatzen ari naiz."
    } ]
  }, {
    "tStartMs": 2705550,
    "dDurationMs": 2280,
    "segs": [ {
      "utf8": "Orokorrean saiatzen naiz eta benetan erabiltzen"
    } ]
  }, {
    "tStartMs": 2707830,
    "dDurationMs": 3050,
    "segs": [ {
      "utf8": "90 minutuko fokatze-energea\nlorik gabeko atsedenaldi sakonaren ondoren."
    } ]
  }, {
    "tStartMs": 2710880,
    "dDurationMs": 1640,
    "segs": [ {
      "utf8": "Eta aurreko ataletan aipatu dudan bezala,"
    } ]
  }, {
    "tStartMs": 2712520,
    "dDurationMs": 1510,
    "segs": [ {
      "utf8": "froga asko dago,"
    } ]
  }, {
    "tStartMs": 2714030,
    "dDurationMs": 2480,
    "segs": [ {
      "utf8": "lorik gabeko\natsedenerako protokolo hauek"
    } ]
  }, {
    "tStartMs": 2716510,
    "dDurationMs": 1660,
    "segs": [ {
      "utf8": "plastikotasuna hobetu eta bizkortu dezaketela."
    } ]
  }, {
    "tStartMs": 2718170,
    "dDurationMs": 1700,
    "segs": [ {
      "utf8": "Azkena eta deigarriena iruditzen zait"
    } ]
  }, {
    "tStartMs": 2719870,
    "dDurationMs": 4830,
    "segs": [ {
      "utf8": "\nazken aldian"
    } ]
  }, {
    "tStartMs": 2724700,
    "dDurationMs": 1100,
    "segs": [ {
      "utf8": "epigrafeen oharretan aipatu dugun azterketa."
    } ]
  }, {
    "tStartMs": 2725800,
    "dDurationMs": 1910,
    "segs": [ {
      "utf8": "Cell Press artikulua zen,"
    } ]
  }, {
    "tStartMs": 2727710,
    "dDurationMs": 2130,
    "segs": [ {
      "utf8": "Cell Reports, aldizkari bikaina."
    } ]
  }, {
    "tStartMs": 2729840,
    "dDurationMs": 1923,
    "segs": [ {
      "utf8": "20 minutuko"
    } ]
  }, {
    "tStartMs": 2732740,
    "dDurationMs": 2270,
    "segs": [ {
      "utf8": "siesta txiki hauek eta lorik\ngabeko atseden sakon hauek"
    } ]
  }, {
    "tStartMs": 2735010,
    "dDurationMs": 2560,
    "segs": [ {
      "utf8": "zentzumen-motorearen ikaskuntza erraztu dezaketela erakusten ari zen."
    } ]
  }, {
    "tStartMs": 2737570,
    "dDurationMs": 1380,
    "segs": [ {
      "utf8": "Beraz, kafeinarik gabeko beste ikasketa bat sartuko dut"
    } ]
  }, {
    "tStartMs": 2738950,
    "dDurationMs": 1380,
    "segs": [ {
      "utf8": "."
    } ]
  }, {
    "tStartMs": 2740330,
    "dDurationMs": 3000,
    "segs": [ {
      "utf8": "Ikasketa saio hau\ngoizekoa baino oso ezberdina da."
    } ]
  }, {
    "tStartMs": 2743330,
    "dDurationMs": 1790,
    "segs": [ {
      "utf8": "Hau lan-saio bat edo ikaskuntza-atal bat da, foku arrunteko erregimen"
    } ]
  }, {
    "tStartMs": 2745120,
    "dDurationMs": 2330,
    "segs": [ {
      "utf8": "argian gehiago dagoena,\n"
    } ]
  }, {
    "tStartMs": 2747450,
    "dDurationMs": 2770,
    "segs": [ {
      "utf8": "\nlorik gabeko atseden sakon honetatik atera naizelako."
    } ]
  }, {
    "tStartMs": 2750220,
    "dDurationMs": 2300,
    "segs": [ {
      "utf8": "Ez dut kafeina hartzen,\n"
    } ]
  }, {
    "tStartMs": 2752520,
    "dDurationMs": 2490,
    "segs": [ {
      "utf8": "\ngau horretan ondo lo egin dezakedala ziurtatu nahi dudalako."
    } ]
  }, {
    "tStartMs": 2755010,
    "dDurationMs": 4090,
    "segs": [ {
      "utf8": "Eta hori gehiago izan ohi da\nsormen motako lanak egiten ditudanean."
    } ]
  }, {
    "tStartMs": 2759100,
    "dDurationMs": 2140,
    "segs": [ {
      "utf8": "Orain, sormena laster\n"
    } ]
  }, {
    "tStartMs": 2761240,
    "dDurationMs": 3000,
    "segs": [ {
      "utf8": "hilabete osoa emango dugun gaia da."
    } ]
  }, {
    "tStartMs": 2764240,
    "dDurationMs": 3970,
    "segs": [ {
      "utf8": "Baina sormena oso\negoera interesgarria da, eta"
    } ]
  }, {
    "tStartMs": 2768210,
    "dDurationMs": 1760,
    "segs": [ {
      "utf8": "bertan dauden elementuak hartzen ari gara."
    } ]
  }, {
    "tStartMs": 2769970,
    "dDurationMs": 2270,
    "segs": [ {
      "utf8": "Dagoeneko ezagutzen ditugun gauzak,"
    } ]
  }, {
    "tStartMs": 2772240,
    "dDurationMs": 3750,
    "segs": [ {
      "utf8": "eta\nberritzaileak diren moduan berrantolatzea."
    } ]
  }, {
    "tStartMs": 2775990,
    "dDurationMs": 3300,
    "segs": [ {
      "utf8": "Eta esango nuke, tira,\nhori da sormena."
    } ]
  }, {
    "tStartMs": 2779290,
    "dDurationMs": 2060,
    "segs": [ {
      "utf8": "Baina sormenak bi zati ditu."
    } ]
  }, {
    "tStartMs": 2781350,
    "dDurationMs": 4250,
    "segs": [ {
      "utf8": "Sormenezko aurkikuntza\nmodua du, non"
    } ]
  }, {
    "tStartMs": 2785600,
    "dDurationMs": 1160,
    "segs": [ {
      "utf8": "gauzak"
    } ]
  }, {
    "tStartMs": 2786760,
    "dDurationMs": 2230,
    "segs": [ {
      "utf8": "oso modu lasaian nahasten dituzun\neta jolasteko"
    } ]
  }, {
    "tStartMs": 2788990,
    "dDurationMs": 2650,
    "segs": [ {
      "utf8": "edo konfigurazio desberdinak arakatzen dituzun."
    } ]
  }, {
    "tStartMs": 2791640,
    "dDurationMs": 2290,
    "segs": [ {
      "utf8": "Eta gero sormenak ere badu,"
    } ]
  }, {
    "tStartMs": 2793930,
    "dDurationMs": 2960,
    "segs": [ {
      "utf8": "inplementazio modu guztiz lineala."
    } ]
  }, {
    "tStartMs": 2796890,
    "dDurationMs": 930,
    "segs": [ {
      "utf8": "Bertan,"
    } ]
  }, {
    "tStartMs": 2797820,
    "dDurationMs": 2390,
    "segs": [ {
      "utf8": "asmatu duzun ideia edo diseinua hartzen duzu"
    } ]
  }, {
    "tStartMs": 2800210,
    "dDurationMs": 3440,
    "segs": [ {
      "utf8": "eta\noso sendoa eta konkretua sortzen duzun."
    } ]
  }, {
    "tStartMs": 2803650,
    "dDurationMs": 2860,
    "segs": [ {
      "utf8": "Eta beraz, sormena\nbenetan bi zati da."
    } ]
  }, {
    "tStartMs": 2806510,
    "dDurationMs": 1560,
    "segs": [ {
      "utf8": "Eta"
    } ]
  }, {
    "tStartMs": 2808070,
    "dDurationMs": 2830,
    "segs": [ {
      "utf8": "\nkonfigurazio desberdinak aktiboki esploratzeko lehen zatia,"
    } ]
  }, {
    "tStartMs": 2810900,
    "dDurationMs": 1690,
    "segs": [ {
      "utf8": "batzuetan modu ludikoan, beste"
    } ]
  }, {
    "tStartMs": 2812590,
    "dDurationMs": 3650,
    "segs": [ {
      "utf8": "batzuetan ia ausaz"
    } ]
  }, {
    "tStartMs": 2816240,
    "dDurationMs": 2120,
    "segs": [ {
      "utf8": "eta esploratzeko modu batean."
    } ]
  }, {
    "tStartMs": 2818360,
    "dDurationMs": 4010,
    "segs": [ {
      "utf8": "Egoera hori, zalantzarik gabe,"
    } ]
  }, {
    "tStartMs": 2822370,
    "dDurationMs": 3870,
    "segs": [ {
      "utf8": "lasai eta ia logura izateak errazten du."
    } ]
  }, {
    "tStartMs": 2826240,
    "dDurationMs": 1020,
    "segs": [ {
      "utf8": "Hori ez da"
    } ]
  }, {
    "tStartMs": 2827260,
    "dDurationMs": 2460,
    "segs": [ {
      "utf8": "pertsonalki\noso ondo sar dezakedan egoera egunean goiz.  Lotik irteten"
    } ]
  }, {
    "tStartMs": 2829720,
    "dDurationMs": 1880,
    "segs": [ {
      "utf8": "saiatu naiz sartzen,\n"
    } ]
  }, {
    "tStartMs": 2831600,
    "dDurationMs": 940,
    "segs": [ {
      "utf8": "batek esango lukeelako,"
    } ]
  }, {
    "tStartMs": 2832540,
    "dDurationMs": 1380,
    "segs": [ {
      "utf8": "oraindik logura duzu egunean goiz"
    } ]
  }, {
    "tStartMs": 2833920,
    "dDurationMs": 833,
    "segs": [ {
      "utf8": "eta ez du funtzionatzen."
    } ]
  }, {
    "tStartMs": 2834753,
    "dDurationMs": 1047,
    "segs": [ {
      "utf8": "Idazten ditudan gehienak.  Egiten"
    } ]
  }, {
    "tStartMs": 2835800,
    "dDurationMs": 2280,
    "segs": [ {
      "utf8": "dudan gehiena zabor osoa da."
    } ]
  }, {
    "tStartMs": 2838080,
    "dDurationMs": 2790,
    "segs": [ {
      "utf8": "Eta, beraz, aurkitu dudana da\narratsaldeko"
    } ]
  }, {
    "tStartMs": 2840870,
    "dDurationMs": 3710,
    "segs": [ {
      "utf8": "90 minutu inguruko bloke hau non,\nsormen motako idazketa"
    } ]
  }, {
    "tStartMs": 2844580,
    "dDurationMs": 2453,
    "segs": [ {
      "utf8": "edo sormen motako irudimena egin dezaket, egin ditzakegun"
    } ]
  }, {
    "tStartMs": 2848380,
    "dDurationMs": 2390,
    "segs": [ {
      "utf8": "ideia zientifikoak edo\nesperimentuak."
    } ]
  }, {
    "tStartMs": 2850770,
    "dDurationMs": 1830,
    "segs": [ {
      "utf8": "Zientzia ez dirudi zuetako askori\nsormen-ahalegin bat"
    } ]
  }, {
    "tStartMs": 2852600,
    "dDurationMs": 1280,
    "segs": [ {
      "utf8": ", baina hala da."
    } ]
  }, {
    "tStartMs": 2853880,
    "dDurationMs": 2650,
    "segs": [ {
      "utf8": "Asko imajinatzen du zer gertatuko balitz hau,"
    } ]
  }, {
    "tStartMs": 2856530,
    "dDurationMs": 1170,
    "segs": [ {
      "utf8": "edo hori konbina genezake"
    } ]
  }, {
    "tStartMs": 2857700,
    "dDurationMs": 3260,
    "segs": [ {
      "utf8": "eta kontzeptu\nedo gauzak antolatzeko modu berrietan pentsatzea."
    } ]
  }, {
    "tStartMs": 2860960,
    "dDurationMs": 1450,
    "segs": [ {
      "utf8": "Beraz, bide"
    } ]
  }, {
    "tStartMs": 2862410,
    "dDurationMs": 2100,
    "segs": [ {
      "utf8": "komun argi horretan aurkitzen zarenean,"
    } ]
  }, {
    "tStartMs": 2864510,
    "dDurationMs": 2980,
    "segs": [ {
      "utf8": "sormen lanak\noso ondo atera ohi dira"
    } ]
  }, {
    "tStartMs": 2867490,
    "dDurationMs": 1530,
    "segs": [ {
      "utf8": "erregimen horietan."
    } ]
  }, {
    "tStartMs": 2869020,
    "dDurationMs": 1700,
    "segs": [ {
      "utf8": "Orain badakit jende asko dagoela\n"
    } ]
  }, {
    "tStartMs": 2870720,
    "dDurationMs": 2403,
    "segs": [ {
      "utf8": "substantzietan oinarritzen dela sormen-egoeretara sartzeko."
    } ]
  }, {
    "tStartMs": 2873970,
    "dDurationMs": 1460,
    "segs": [ {
      "utf8": "Ez naiz marihuana kontsumitzailea."
    } ]
  }, {
    "tStartMs": 2875430,
    "dDurationMs": 3600,
    "segs": [ {
      "utf8": "Niretzat ez da droga\n, hainbat arrazoirengatik."
    } ]
  }, {
    "tStartMs": 2879030,
    "dDurationMs": 1460,
    "segs": [ {
      "utf8": "Ez naiz edalea."
    } ]
  }, {
    "tStartMs": 2880490,
    "dDurationMs": 3560,
    "segs": [ {
      "utf8": "Ez da niretzat funtsa\nhainbat arrazoirengatik."
    } ]
  }, {
    "tStartMs": 2884050,
    "dDurationMs": 2170,
    "segs": [ {
      "utf8": "Badakizu, ez naiz polizia, ez nago hemen"
    } ]
  }, {
    "tStartMs": 2886220,
    "dDurationMs": 1970,
    "segs": [ {
      "utf8": "jendeari zer\negin behar duten edo zer ez den esatera."
    } ]
  }, {
    "tStartMs": 2888190,
    "dDurationMs": 3590,
    "segs": [ {
      "utf8": "\nSormena sartzeko substantziak erabiltzearen arazoa"
    } ]
  }, {
    "tStartMs": 2891780,
    "dDurationMs": 4120,
    "segs": [ {
      "utf8": "da, oro har,\njendea erlaxatzen duten substantziek"
    } ]
  }, {
    "tStartMs": 2895900,
    "dDurationMs": 1060,
    "segs": [ {
      "utf8": "aukera emango dietela"
    } ]
  }, {
    "tStartMs": 2896960,
    "dDurationMs": 2860,
    "segs": [ {
      "utf8": "ideia-jasa sortzaile horretan sartzeko,"
    } ]
  }, {
    "tStartMs": 2899820,
    "dDurationMs": 4023,
    "segs": [ {
      "utf8": "baina ez hain onak\ninplementazio modu linealean."
    } ]
  }, {
    "tStartMs": 2904700,
    "dDurationMs": 2740,
    "segs": [ {
      "utf8": "Badakizu, lehengo egunean lagun batekin esaten ari nintzen"
    } ]
  }, {
    "tStartMs": 2907440,
    "dDurationMs": 3480,
    "segs": [ {
      "utf8": "badirela\niragarki batzuk,"
    } ]
  }, {
    "tStartMs": 2910920,
    "dDurationMs": 3980,
    "segs": [ {
      "utf8": "urteetan ikusi ditudan iragarki batzuk,\nikaragarriak direla."
    } ]
  }, {
    "tStartMs": 2914900,
    "dDurationMs": 920,
    "segs": [ {
      "utf8": "Besterik ez dizut esango zer diren."
    } ]
  }, {
    "tStartMs": 2915820,
    "dDurationMs": 1110,
    "segs": [ {
      "utf8": "Beraz, ez dago kriptikoa edo ezer."
    } ]
  }, {
    "tStartMs": 2916930,
    "dDurationMs": 2059,
    "segs": [ {
      "utf8": "Hemen nire gustuak azaltzen ari naiz."
    } ]
  }, {
    "tStartMs": 2918989,
    "dDurationMs": 3361,
    "segs": [ {
      "utf8": "\nSpike Jones-ek egin duen lurrin-iragarki bat"
    } ]
  }, {
    "tStartMs": 2922350,
    "dDurationMs": 1750,
    "segs": [ {
      "utf8": "dago harrigarria."
    } ]
  }, {
    "tStartMs": 2924100,
    "dDurationMs": 2723,
    "segs": [ {
      "utf8": "Esteka bat jarriko dut\noso polita delako."
    } ]
  }, {
    "tStartMs": 2929412,
    "dDurationMs": 833,
    "segs": [ {
      "utf8": "Ez dut amaiera eman nahi"
    } ]
  }, {
    "tStartMs": 2930245,
    "dDurationMs": 1918,
    "segs": [ {
      "utf8": "baina\n"
    } ]
  }, {
    "tStartMs": 2932163,
    "dDurationMs": 2768,
    "segs": [ {
      "utf8": "neurozientzialari gisa bereziki interesgarria den ezaugarri bat du."
    } ]
  }, {
    "tStartMs": 2934931,
    "dDurationMs": 1429,
    "segs": [ {
      "utf8": "Eta oso polita da."
    } ]
  }, {
    "tStartMs": 2936360,
    "dDurationMs": 1410,
    "segs": [ {
      "utf8": "Skategintzan hazi nintzenez\n, Spikeren pelikulen eta skatearen"
    } ]
  }, {
    "tStartMs": 2937770,
    "dDurationMs": 2030,
    "segs": [ {
      "utf8": "inguruan apur bat banekien\n"
    } ]
  }, {
    "tStartMs": 2939800,
    "dDurationMs": 2480,
    "segs": [ {
      "utf8": "eta, noski, film\noso ikusgarriak eta"
    } ]
  }, {
    "tStartMs": 2942280,
    "dDurationMs": 2950,
    "segs": [ {
      "utf8": "ezagunak ere egin ditu,\nluzemetraiak."
    } ]
  }, {
    "tStartMs": 2945230,
    "dDurationMs": 2670,
    "segs": [ {
      "utf8": "Ez dut pertsonalki ezagutzen,\nberaz, hau ez da entxufe bat."
    } ]
  }, {
    "tStartMs": 2947900,
    "dDurationMs": 2490,
    "segs": [ {
      "utf8": "Ez da\nezertarako nire babesa behar duenik."
    } ]
  }, {
    "tStartMs": 2950390,
    "dDurationMs": 4410,
    "segs": [ {
      "utf8": "Baina iragarki honen gauza harrigarria\n"
    } ]
  }, {
    "tStartMs": 2954800,
    "dDurationMs": 2530,
    "segs": [ {
      "utf8": "da inoiz pentsatuko ez zenukeen gauza bilduma bat dela"
    } ]
  }, {
    "tStartMs": 2957330,
    "dDurationMs": 2590,
    "segs": [ {
      "utf8": "\n,"
    } ]
  }, {
    "tStartMs": 2959920,
    "dDurationMs": 1540,
    "segs": [ {
      "utf8": "eta mugimendu-abiadura desberdinak"
    } ]
  }, {
    "tStartMs": 2961460,
    "dDurationMs": 2170,
    "segs": [ {
      "utf8": "eta era guztietako efektuak dakartza."
    } ]
  }, {
    "tStartMs": 2963630,
    "dDurationMs": 1290,
    "segs": [ {
      "utf8": "Esan nahi dut,"
    } ]
  }, {
    "tStartMs": 2964920,
    "dDurationMs": 2080,
    "segs": [ {
      "utf8": "Spike Jones-en entrega atsegina bezalako benetako klasiko bat da."
    } ]
  }, {
    "tStartMs": 2967000,
    "dDurationMs": 3270,
    "segs": [ {
      "utf8": "Baina, sinestezina dena da,"
    } ]
  }, {
    "tStartMs": 2970270,
    "dDurationMs": 2620,
    "segs": [ {
      "utf8": "\nnorbaitek hori imajinatu behar zuela ez ezik,"
    } ]
  }, {
    "tStartMs": 2972890,
    "dDurationMs": 4210,
    "segs": [ {
      "utf8": "\nhori sortzeko urratsak benetan inplementatzeaz pentsatzen duzunean,"
    } ]
  }, {
    "tStartMs": 2977100,
    "dDurationMs": 2730,
    "segs": [ {
      "utf8": "hau ikusten duzunean\nlan pila bat zela konturatuko zara."
    } ]
  }, {
    "tStartMs": 2979830,
    "dDurationMs": 1720,
    "segs": [ {
      "utf8": "Ezin duzu hori ausaz elkartu."
    } ]
  }, {
    "tStartMs": 2981550,
    "dDurationMs": 3890,
    "segs": [ {
      "utf8": "Eta, beraz, jende asko, ez Spike argi,"
    } ]
  }, {
    "tStartMs": 2985440,
    "dDurationMs": 5000,
    "segs": [ {
      "utf8": "baina\nideia"
    } ]
  }, {
    "tStartMs": 2990619,
    "dDurationMs": 2861,
    "segs": [ {
      "utf8": "eta gauzen antolamendu berrietarako gogo ikaragarria duen jende asko,"
    } ]
  }, {
    "tStartMs": 2993480,
    "dDurationMs": 2580,
    "segs": [ {
      "utf8": "bikainak dira\negoera horretara iristeko baina ez hain onak"
    } ]
  }, {
    "tStartMs": 2996060,
    "dDurationMs": 1993,
    "segs": [ {
      "utf8": "ezarpen egoerara sartzeko."
    } ]
  }, {
    "tStartMs": 2999070,
    "dDurationMs": 1870,
    "segs": [ {
      "utf8": "Egia da, halaber, jende asko"
    } ]
  }, {
    "tStartMs": 3000940,
    "dDurationMs": 2490,
    "segs": [ {
      "utf8": "eta\n"
    } ]
  }, {
    "tStartMs": 3003430,
    "dDurationMs": 3360,
    "segs": [ {
      "utf8": "Aspergerren edo\nautismoaren espektroaren amaieran deitzen geniokeen mota batean erortzeko joera dutenak"
    } ]
  }, {
    "tStartMs": 3006790,
    "dDurationMs": 2390,
    "segs": [ {
      "utf8": "oso onak direla ezarpen linealean."
    } ]
  }, {
    "tStartMs": 3009180,
    "dDurationMs": 2060,
    "segs": [ {
      "utf8": "Orain ez naiz autismo mota guztiei buruz ari\nnoski."
    } ]
  }, {
    "tStartMs": 3011240,
    "dDurationMs": 1030,
    "segs": [ {
      "utf8": ""
    } ]
  }, {
    "tStartMs": 3012270,
    "dDurationMs": 2030,
    "segs": [ {
      "utf8": "Espektroan forma asko daudelako sentikorra naiz."
    } ]
  }, {
    "tStartMs": 3014300,
    "dDurationMs": 3470,
    "segs": [ {
      "utf8": "Baina, pertsona batzuk oso\nonak dira inplementazio linealean."
    } ]
  }, {
    "tStartMs": 3017770,
    "dDurationMs": 4450,
    "segs": [ {
      "utf8": "Eta hau da,\nestatu sortzaile batetik aparteko egoera."
    } ]
  }, {
    "tStartMs": 3022220,
    "dDurationMs": 2410,
    "segs": [ {
      "utf8": "Beraz, arratsaldeko bloke hori arratsaldean"
    } ]
  }, {
    "tStartMs": 3024630,
    "dDurationMs": 3390,
    "segs": [ {
      "utf8": "\n"
    } ]
  }, {
    "tStartMs": 3028020,
    "dDurationMs": 2010,
    "segs": [ {
      "utf8": "\nberanduago datorren nekearekin lotuta dagoen pentsamolde askeagoan sartzen saiatzen naiz"
    } ]
  }, {
    "tStartMs": 3030030,
    "dDurationMs": 1013,
    "segs": [ {
      "utf8": "."
    } ]
  }, {
    "tStartMs": 3032010,
    "dDurationMs": 2920,
    "segs": [ {
      "utf8": "Eta, zuetako batzuentzat,"
    } ]
  }, {
    "tStartMs": 3034930,
    "dDurationMs": 2410,
    "segs": [ {
      "utf8": "sormena\neta sormen-ikaskuntzaren alde egiten duen egoera hori,"
    } ]
  }, {
    "tStartMs": 3037340,
    "dDurationMs": 1740,
    "segs": [ {
      "utf8": "baliteke goizean hobea izatea."
    } ]
  }, {
    "tStartMs": 3039080,
    "dDurationMs": 833,
    "segs": [ {
      "utf8": "Ez dakit."
    } ]
  }, {
    "tStartMs": 3039913,
    "dDurationMs": 833,
    "segs": [ {
      "utf8": "Erabaki beharko duzu."
    } ]
  }, {
    "tStartMs": 3040746,
    "dDurationMs": 2004,
    "segs": [ {
      "utf8": "Zuetako batzuentzat\nberandu joango zarete."
    } ]
  }, {
    "tStartMs": 3042750,
    "dDurationMs": 1630,
    "segs": [ {
      "utf8": "Zuetako batzuk goizez aldatuko zarete."
    } ]
  }, {
    "tStartMs": 3044380,
    "dDurationMs": 1400,
    "segs": [ {
      "utf8": "Baina ernetasuna daukagun tokietan,"
    } ]
  }, {
    "tStartMs": 3045780,
    "dDurationMs": 2120,
    "segs": [ {
      "utf8": "orokorrean onak gara\ninplementazio linealean,"
    } ]
  }, {
    "tStartMs": 3047900,
    "dDurationMs": 2290,
    "segs": [ {
      "utf8": "onak gara ez-joko bidea aktibatzen"
    } ]
  }, {
    "tStartMs": 3050190,
    "dDurationMs": 1090,
    "segs": [ {
      "utf8": "eta ekintza kentzen,"
    } ]
  }, {
    "tStartMs": 3051280,
    "dDurationMs": 3408,
    "segs": [ {
      "utf8": "eta onak gara\nhelburu"
    } ]
  }, {
    "tStartMs": 3054688,
    "dDurationMs": 2172,
    "segs": [ {
      "utf8": "eta estrategia zehatzak lortzeko."
    } ]
  }, {
    "tStartMs": 3056860,
    "dDurationMs": 2150,
    "segs": [ {
      "utf8": "Eta non lasaiago egon ohi garen,"
    } ]
  }, {
    "tStartMs": 3059010,
    "dDurationMs": 2110,
    "segs": [ {
      "utf8": "eta ia\nlogura moduko batean egon ohi gara."
    } ]
  }, {
    "tStartMs": 3061120,
    "dDurationMs": 833,
    "segs": [ {
      "utf8": "Beraz, niretzat,"
    } ]
  }, {
    "tStartMs": 3061953,
    "dDurationMs": 2897,
    "segs": [ {
      "utf8": "lorik gabeko,\natseden sakoneko modu edo lo horietako batetik aterata,"
    } ]
  }, {
    "tStartMs": 3064850,
    "dDurationMs": 3890,
    "segs": [ {
      "utf8": "orduan\nhobeak izaten gara"
    } ]
  }, {
    "tStartMs": 3068740,
    "dDurationMs": 2440,
    "segs": [ {
      "utf8": "lehendik dauden elementuen konfigurazio berrietan, hau da, sormena."
    } ]
  }, {
    "tStartMs": 3071180,
    "dDurationMs": 4110,
    "segs": [ {
      "utf8": "Eta honek\ndenbora guztian egiten dudan galdera bat dakar."
    } ]
  }, {
    "tStartMs": 3075290,
    "dDurationMs": 2970,
    "segs": [ {
      "utf8": "Zein da, zer psikodelikoekin?"
    } ]
  }, {
    "tStartMs": 3078260,
    "dDurationMs": 4270,
    "segs": [ {
      "utf8": "Beraz, psikodelikoetan aditu batzuekin hitz egingo dut\n."
    } ]
  }, {
    "tStartMs": 3082530,
    "dDurationMs": 2000,
    "segs": [ {
      "utf8": "Horietako batzuk ekartzea espero dut."
    } ]
  }, {
    "tStartMs": 3084530,
    "dDurationMs": 1640,
    "segs": [ {
      "utf8": "Egia esan, datozen pertsonei buruz"
    } ]
  }, {
    "tStartMs": 3086170,
    "dDurationMs": 1323,
    "segs": [ {
      "utf8": "edo datozen izakiez ari naiz."
    } ]
  }, {
    "tStartMs": 3088360,
    "dDurationMs": 1870,
    "segs": [ {
      "utf8": "\n"
    } ]
  }, {
    "tStartMs": 3090230,
    "dDurationMs": 3030,
    "segs": [ {
      "utf8": "Inongo beharrik ez duen psikodelikoetan ez dagoen izaki bat da\nCostello eta iritsi berri da."
    } ]
  }, {
    "tStartMs": 3093260,
    "dDurationMs": 2530,
    "segs": [ {
      "utf8": "Lo-egoeran dagoela dirudi\ngehienetan, denbora guztian."
    } ]
  }, {
    "tStartMs": 3095790,
    "dDurationMs": 1990,
    "segs": [ {
      "utf8": "Aupa laguna, zer moduz zaude?"
    } ]
  }, {
    "tStartMs": 3097780,
    "dDurationMs": 1900,
    "segs": [ {
      "utf8": "Sartu zara, bai.  Eguneko"
    } ]
  }, {
    "tStartMs": 3099680,
    "dDurationMs": 4580,
    "segs": [ {
      "utf8": "15. loaren\natseden sakoneko atala egiten ari da, eta,"
    } ]
  }, {
    "tStartMs": 3104260,
    "dDurationMs": 1090,
    "segs": [ {
      "utf8": "oro har,"
    } ]
  }, {
    "tStartMs": 3105350,
    "dDurationMs": 3270,
    "segs": [ {
      "utf8": "10 eta 12 orduko atseden sakoneko atal bat jarraitzen du,"
    } ]
  }, {
    "tStartMs": 3108620,
    "dDurationMs": 2770,
    "segs": [ {
      "utf8": "ia REMz osatua soilik."
    } ]
  }, {
    "tStartMs": 3111390,
    "dDurationMs": 1580,
    "segs": [ {
      "utf8": "Eta hori badakit bere begiak irekita daudelako,"
    } ]
  }, {
    "tStartMs": 3112970,
    "dDurationMs": 1820,
    "segs": [ {
      "utf8": "hain erorita daudelako,\nezin ditu bide guztia itxi"
    } ]
  }, {
    "tStartMs": 3114790,
    "dDurationMs": 1640,
    "segs": [ {
      "utf8": "eta begiak horrela doaz."
    } ]
  }, {
    "tStartMs": 3116430,
    "dDurationMs": 1730,
    "segs": [ {
      "utf8": "Eta zenbaketara jaisten ari da."
    } ]
  }, {
    "tStartMs": 3118160,
    "dDurationMs": 3450,
    "segs": [ {
      "utf8": "Beraz, bai, aharrausi polita eta handia."
    } ]
  }, {
    "tStartMs": 3121610,
    "dDurationMs": 2940,
    "segs": [ {
      "utf8": "Ados, psikodelikoak."
    } ]
  }, {
    "tStartMs": 3124550,
    "dDurationMs": 1740,
    "segs": [ {
      "utf8": "Lehenik eta behin, oso argi izan nahi dut."
    } ]
  }, {
    "tStartMs": 3126290,
    "dDurationMs": 3390,
    "segs": [ {
      "utf8": "Ez naiz bultzatzaile bat,\nezta"
    } ]
  }, {
    "tStartMs": 3129680,
    "dDurationMs": 3550,
    "segs": [ {
      "utf8": "\npsikodelikoen balizko rola baztertzen duen norbait ere."
    } ]
  }, {
    "tStartMs": 3133230,
    "dDurationMs": 2710,
    "segs": [ {
      "utf8": "Hala ere, uste dut psikodelikoak"
    } ]
  }, {
    "tStartMs": 3135940,
    "dDurationMs": 2240,
    "segs": [ {
      "utf8": "bereziki arriskutsuak izan daitezkeela"
    } ]
  }, {
    "tStartMs": 3138180,
    "dDurationMs": 3110,
    "segs": [ {
      "utf8": "aldez aurretik\narazo psikologikoak dituzten pertsonentzat,"
    } ]
  }, {
    "tStartMs": 3141290,
    "dDurationMs": 3070,
    "segs": [ {
      "utf8": "eta\nziurtatutako batzordearekin,"
    } ]
  }, {
    "tStartMs": 3144360,
    "dDurationMs": 1800,
    "segs": [ {
      "utf8": "psikiatrarekin edo medikuarekin lan egiten ez dutenentzat,"
    } ]
  }, {
    "tStartMs": 3146160,
    "dDurationMs": 3560,
    "segs": [ {
      "utf8": "baita funtsean ume guztientzat ere."
    } ]
  }, {
    "tStartMs": 3149720,
    "dDurationMs": 2350,
    "segs": [ {
      "utf8": "Uste dut garun gaztea, funtsean,"
    } ]
  }, {
    "tStartMs": 3152070,
    "dDurationMs": 2723,
    "segs": [ {
      "utf8": "bere\negoera psikodelikoan dagoela eta modu naturalean."
    } ]
  }, {
    "tStartMs": 3155660,
    "dDurationMs": 1900,
    "segs": [ {
      "utf8": "Eta txantxak alde batera utzita,"
    } ]
  }, {
    "tStartMs": 3157560,
    "dDurationMs": 1380,
    "segs": [ {
      "utf8": "uste dut garun gaztea"
    } ]
  }, {
    "tStartMs": 3158940,
    "dDurationMs": 3030,
    "segs": [ {
      "utf8": "hain dagoela neuroplastikotasunaren menpe,"
    } ]
  }, {
    "tStartMs": 3161970,
    "dDurationMs": 3940,
    "segs": [ {
      "utf8": "non drogak, psikodelikoak\noso indartsuak diren bezala,"
    } ]
  }, {
    "tStartMs": 3165910,
    "dDurationMs": 2380,
    "segs": [ {
      "utf8": "\ngaratzen ari den garunarentzat kaltegarriak izan daitezkeela."
    } ]
  }, {
    "tStartMs": 3168290,
    "dDurationMs": 1050,
    "segs": [ {
      "utf8": "Hori nire jarrera besterik ez da."
    } ]
  }, {
    "tStartMs": 3169340,
    "dDurationMs": 2550,
    "segs": [ {
      "utf8": "Norbait nirekin ados ez badago,\natsegin handiz hitz egingo nuke zurekin"
    } ]
  }, {
    "tStartMs": 3171890,
    "dDurationMs": 3620,
    "segs": [ {
      "utf8": "adeitasunez eta diskurtso batean."
    } ]
  }, {
    "tStartMs": 3175510,
    "dDurationMs": 1030,
    "segs": [ {
      "utf8": "Pozik entzungo dut,"
    } ]
  }, {
    "tStartMs": 3176540,
    "dDurationMs": 3143,
    "segs": [ {
      "utf8": "baita datuetan oinarrituta zergatik uste dudan gehiago kontatuko dizut\n."
    } ]
  }, {
    "tStartMs": 3181390,
    "dDurationMs": 2410,
    "segs": [ {
      "utf8": "Psikodelikoak aipatzen ari naiz\nzuetako askok galdetu duzuelako."
    } ]
  }, {
    "tStartMs": 3183800,
    "dDurationMs": 2170,
    "segs": [ {
      "utf8": "Hona hemen psikodelikoen tratua."
    } ]
  }, {
    "tStartMs": 3185970,
    "dDurationMs": 1880,
    "segs": [ {
      "utf8": "Hona behintzat nola funtzionatzen duten."
    } ]
  }, {
    "tStartMs": 3187850,
    "dDurationMs": 4600,
    "segs": [ {
      "utf8": "Laburbilduz, psikodelikoek"
    } ]
  }, {
    "tStartMs": 3192450,
    "dDurationMs": 4150,
    "segs": [ {
      "utf8": "zentzumenen prozesamendua askatzen zutela\neta gutxiago iragazten zutela pentsatu zen."
    } ]
  }, {
    "tStartMs": 3196600,
    "dDurationMs": 1730,
    "segs": [ {
      "utf8": "\nGure begietatik,"
    } ]
  }, {
    "tStartMs": 3198330,
    "dDurationMs": 1890,
    "segs": [ {
      "utf8": "belarrietatik, sudurretik,\ngure zaporetik, eta abar hainbat sarrera ditugu,"
    } ]
  }, {
    "tStartMs": 3200220,
    "dDurationMs": 1960,
    "segs": [ {
      "utf8": "\ndenbora guztian paraleloan etortzen zarela."
    } ]
  }, {
    "tStartMs": 3202180,
    "dDurationMs": 3490,
    "segs": [ {
      "utf8": "Eta\nzenbait mekanismo kendu eta gertatzen ari diren"
    } ]
  }, {
    "tStartMs": 3205670,
    "dDurationMs": 1650,
    "segs": [ {
      "utf8": "gauzetan soilik zentratu ahal izateko aukera ematen digute\n."
    } ]
  }, {
    "tStartMs": 3207320,
    "dDurationMs": 3050,
    "segs": [ {
      "utf8": "Ikusmenean, oro har, ez dugu\nsinestesiarik gutako"
    } ]
  }, {
    "tStartMs": 3210370,
    "dDurationMs": 1710,
    "segs": [ {
      "utf8": "batzuek sinestesia izaten ez badute behintzat."
    } ]
  }, {
    "tStartMs": 3212080,
    "dDurationMs": 3550,
    "segs": [ {
      "utf8": "Ez dugu ikusten duguna eta\nentzuten duguna nolabait nahasten,"
    } ]
  }, {
    "tStartMs": 3215630,
    "dDurationMs": 1690,
    "segs": [ {
      "utf8": "hori nahasgarria da guretzat."
    } ]
  }, {
    "tStartMs": 3217320,
    "dDurationMs": 1780,
    "segs": [ {
      "utf8": "Badakigu zer ari den sortzen soinuak eta badakigu"
    } ]
  }, {
    "tStartMs": 3219100,
    "dDurationMs": 3590,
    "segs": [ {
      "utf8": "zer den ikusizko estimulua."
    } ]
  }, {
    "tStartMs": 3222690,
    "dDurationMs": 3560,
    "segs": [ {
      "utf8": "Psikodelikoei buruz, jendeak\nkoloreak usaintzeko gai direla"
    } ]
  }, {
    "tStartMs": 3226250,
    "dDurationMs": 3600,
    "segs": [ {
      "utf8": "edo, badakizu, zuhaitzak entzuteko, etab."
    } ]
  }, {
    "tStartMs": 3229850,
    "dDurationMs": 2070,
    "segs": [ {
      "utf8": "Eta hori da\nsentsorial nahasketa asko dagoelako."
    } ]
  }, {
    "tStartMs": 3231920,
    "dDurationMs": 2570,
    "segs": [ {
      "utf8": "Horrek uste okerra ekarri du,"
    } ]
  }, {
    "tStartMs": 3234490,
    "dDurationMs": 3630,
    "segs": [ {
      "utf8": "zentzumenen nahasketa bera\nsormen prozesu bat dela."
    } ]
  }, {
    "tStartMs": 3238120,
    "dDurationMs": 3500,
    "segs": [ {
      "utf8": "\nZentzumenen nahasketari buruz ez dago ezer sortzailerik."
    } ]
  }, {
    "tStartMs": 3241620,
    "dDurationMs": 4000,
    "segs": [ {
      "utf8": "Badakizu badagoela,\nsormen prozesu baten funtsa, elementuen konfigurazio"
    } ]
  }, {
    "tStartMs": 3245620,
    "dDurationMs": 3380,
    "segs": [ {
      "utf8": "nobela bat dela,\n"
    } ]
  }, {
    "tStartMs": 3249000,
    "dDurationMs": 2070,
    "segs": [ {
      "utf8": "piano bateko notak izan ala ez,"
    } ]
  }, {
    "tStartMs": 3251070,
    "dDurationMs": 1370,
    "segs": [ {
      "utf8": "edo orrialde bateko hitzak izan ala ez,"
    } ]
  }, {
    "tStartMs": 3252440,
    "dDurationMs": 1170,
    "segs": [ {
      "utf8": "zenbakiak izan ala ez,"
    } ]
  }, {
    "tStartMs": 3253610,
    "dDurationMs": 1460,
    "segs": [ {
      "utf8": "mugimendua izan ala ez."
    } ]
  }, {
    "tStartMs": 3255070,
    "dDurationMs": 5000,
    "segs": [ {
      "utf8": "Horiek\nmodu berriren batean konfiguratzen diren,"
    } ]
  }, {
    "tStartMs": 3260870,
    "dDurationMs": 3310,
    "segs": [ {
      "utf8": "algoritmoak, konfiguratzeko moduak"
    } ]
  }, {
    "tStartMs": 3264180,
    "dDurationMs": 3130,
    "segs": [ {
      "utf8": "\nzentzua duela behatzailearentzat."
    } ]
  }, {
    "tStartMs": 3267310,
    "dDurationMs": 1240,
    "segs": [ {
      "utf8": "Eta hau funtsezko gauza bat da."
    } ]
  }, {
    "tStartMs": 3268550,
    "dDurationMs": 833,
    "segs": [ {
      "utf8": "Iruditzen zait"
    } ]
  }, {
    "tStartMs": 3269383,
    "dDurationMs": 2437,
    "segs": [ {
      "utf8": "jendeak bere bizipen psikodelikoak salatzen dituenean\n"
    } ]
  }, {
    "tStartMs": 3271820,
    "dDurationMs": 1050,
    "segs": [ {
      "utf8": "askoz zentzu handiagoa duela"
    } ]
  }, {
    "tStartMs": 3272870,
    "dDurationMs": 3040,
    "segs": [ {
      "utf8": "bizitzen duenarentzat\n, behatzailearentzat baino."
    } ]
  }, {
    "tStartMs": 3275910,
    "dDurationMs": 5000,
    "segs": [ {
      "utf8": "Beraz, sormen-lanak definizioz\n"
    } ]
  }, {
    "tStartMs": 3281220,
    "dDurationMs": 2450,
    "segs": [ {
      "utf8": "gauzak konfiguratzeko modu berriak dira, behatzailearen aldetik ulermen"
    } ]
  }, {
    "tStartMs": 3283670,
    "dDurationMs": 4300,
    "segs": [ {
      "utf8": "handiagoa edo handiagoa edo\nsakonagoa edo berritzailea izateko aukera ematen dutenak"
    } ]
  }, {
    "tStartMs": 3287970,
    "dDurationMs": 1900,
    "segs": [ {
      "utf8": "."
    } ]
  }, {
    "tStartMs": 3289870,
    "dDurationMs": 3050,
    "segs": [ {
      "utf8": "Eta zentzumenen nahasketak\nez du hori lortuko."
    } ]
  }, {
    "tStartMs": 3292920,
    "dDurationMs": 2070,
    "segs": [ {
      "utf8": "Orain egia da eta aldizkariaren gelaxkan berrikuspen bikaina dago"
    } ]
  }, {
    "tStartMs": 3294990,
    "dDurationMs": 1940,
    "segs": [ {
      "utf8": ","
    } ]
  }, {
    "tStartMs": 3296930,
    "dDurationMs": 1430,
    "segs": [ {
      "utf8": "psikodelikoen funtzionamenduari buruzko aldizkari bikaina."
    } ]
  }, {
    "tStartMs": 3298360,
    "dDurationMs": 2610,
    "segs": [ {
      "utf8": "Eta, ondorioz, ez dute bakarrik funtzionatzen,"
    } ]
  }, {
    "tStartMs": 3300970,
    "dDurationMs": 2910,
    "segs": [ {
      "utf8": "zentzumenen nahasketa gehiago ahalbidetuz.  Hala"
    } ]
  }, {
    "tStartMs": 3303880,
    "dDurationMs": 1950,
    "segs": [ {
      "utf8": "egiten dute,"
    } ]
  }, {
    "tStartMs": 3305830,
    "dDurationMs": 2620,
    "segs": [ {
      "utf8": "\nserotonina-hartzaile batzuk aktibatzeko moduagatik eta abar."
    } ]
  }, {
    "tStartMs": 3308450,
    "dDurationMs": 3230,
    "segs": [ {
      "utf8": "\n"
    } ]
  }, {
    "tStartMs": 3311680,
    "dDurationMs": 3270,
    "segs": [ {
      "utf8": "Garuneko eremu desberdinen arteko alboko konektibitate gehiagorako ematen dute,\nelkarte berriagoak."
    } ]
  }, {
    "tStartMs": 3314950,
    "dDurationMs": 1930,
    "segs": [ {
      "utf8": "Beraz, printzipioz,"
    } ]
  }, {
    "tStartMs": 3316880,
    "dDurationMs": 1930,
    "segs": [ {
      "utf8": "printzipioz,\nez dut zertan esan behar praktikan,"
    } ]
  }, {
    "tStartMs": 3318810,
    "dDurationMs": 3490,
    "segs": [ {
      "utf8": "baina printzipioz, garunaren\neremu desberdinak onartzen dituzte"
    } ]
  }, {
    "tStartMs": 3322300,
    "dDurationMs": 833,
    "segs": [ {
      "utf8": "."
    } ]
  }, {
    "tStartMs": 3323133,
    "dDurationMs": 833,
    "segs": [ {
      "utf8": "Agian garunaren bi aldeak ere"
    } ]
  }, {
    "tStartMs": 3323966,
    "dDurationMs": 1904,
    "segs": [ {
      "utf8": ""
    } ]
  }, {
    "tStartMs": 3325870,
    "dDurationMs": 1820,
    "segs": [ {
      "utf8": "normalean baino zabalago komunikatzeko."
    } ]
  }, {
    "tStartMs": 3327690,
    "dDurationMs": 4200,
    "segs": [ {
      "utf8": "Beraz,\nsormenari buruz hitz egiten duten zenbait elementu ditu horrek."
    } ]
  }, {
    "tStartMs": 3331890,
    "dDurationMs": 2090,
    "segs": [ {
      "utf8": "Baina, ezin da besterik gabe"
    } ]
  }, {
    "tStartMs": 3333980,
    "dDurationMs": 3550,
    "segs": [ {
      "utf8": "psikodelikoak\nsormenaren ataria izatea."
    } ]
  }, {
    "tStartMs": 3337530,
    "dDurationMs": 2260,
    "segs": [ {
      "utf8": "Sormenak, lehen aipatu dudan bezala,"
    } ]
  }, {
    "tStartMs": 3339790,
    "dDurationMs": 4050,
    "segs": [ {
      "utf8": "eleberri-\nelkarteak eta"
    } ]
  }, {
    "tStartMs": 3343840,
    "dDurationMs": 4590,
    "segs": [ {
      "utf8": "espazio-denborazko arauak haustea ez ezik,\ngauzak birkonfiguratzea ere dakar,"
    } ]
  }, {
    "tStartMs": 3348430,
    "dDurationMs": 3690,
    "segs": [ {
      "utf8": "hala nola norberak sortzen duen espazio-denbora-arau berria\n"
    } ]
  }, {
    "tStartMs": 3352120,
    "dDurationMs": 2290,
    "segs": [ {
      "utf8": "interesgarria, estimulagarria"
    } ]
  }, {
    "tStartMs": 3354410,
    "dDurationMs": 3720,
    "segs": [ {
      "utf8": "eta atsegina da.  kasu askotan,\nbehatzailearentzat gozagarria."
    } ]
  }, {
    "tStartMs": 3358130,
    "dDurationMs": 3320,
    "segs": [ {
      "utf8": "Eta horregatik, askok\ndiote, badakizu,"
    } ]
  }, {
    "tStartMs": 3361450,
    "dDurationMs": 4840,
    "segs": [ {
      "utf8": "psikodelikoek plastikotasuna irekitzen dutela\nedo sormena areagotzen dutela."
    } ]
  }, {
    "tStartMs": 3366290,
    "dDurationMs": 2410,
    "segs": [ {
      "utf8": "Hori ez da nahikoa niretzat pertsonalki."
    } ]
  }, {
    "tStartMs": 3368700,
    "dDurationMs": 1060,
    "segs": [ {
      "utf8": "Jakin-mina daukat,"
    } ]
  }, {
    "tStartMs": 3369760,
    "dDurationMs": 3320,
    "segs": [ {
      "utf8": "ez al du\npentsamendu sortzailearen prozesua irekitzen,"
    } ]
  }, {
    "tStartMs": 3373080,
    "dDurationMs": 1600,
    "segs": [ {
      "utf8": "konfigurazio prozesu berri hau?"
    } ]
  }, {
    "tStartMs": 3374680,
    "dDurationMs": 1510,
    "segs": [ {
      "utf8": "Baina"
    } ]
  }, {
    "tStartMs": 3376190,
    "dDurationMs": 2880,
    "segs": [ {
      "utf8": "\nsormen-lanak gauzatzeko ere ematen al du?"
    } ]
  }, {
    "tStartMs": 3379070,
    "dDurationMs": 2200,
    "segs": [ {
      "utf8": "Eta erantzuna, ezetz da."
    } ]
  }, {
    "tStartMs": 3381270,
    "dDurationMs": 2580,
    "segs": [ {
      "utf8": "Kasu gehienetan ez du zerikusirik"
    } ]
  }, {
    "tStartMs": 3383850,
    "dDurationMs": 1790,
    "segs": [ {
      "utf8": "sormenaren ezarpenarekin."
    } ]
  }, {
    "tStartMs": 3385640,
    "dDurationMs": 3600,
    "segs": [ {
      "utf8": "Orain, uste dut une bat iritsi daitekeela"
    } ]
  }, {
    "tStartMs": 3389240,
    "dDurationMs": 1310,
    "segs": [ {
      "utf8": "eta, zalantzarik gabe, entsegu klinikoak daude"
    } ]
  }, {
    "tStartMs": 3390550,
    "dDurationMs": 1070,
    "segs": [ {
      "utf8": "orain gertatzen ari direnak,"
    } ]
  }, {
    "tStartMs": 3391620,
    "dDurationMs": 1340,
    "segs": [ {
      "utf8": "non psikodelikoak"
    } ]
  }, {
    "tStartMs": 3392960,
    "dDurationMs": 2000,
    "segs": [ {
      "utf8": "helburu kliniko jakin batzuetarako baliatzen diren."
    } ]
  }, {
    "tStartMs": 3394960,
    "dDurationMs": 1430,
    "segs": [ {
      "utf8": "Eta orain gertatzen ari den Johns Hopkins-en lanari eman nahi diot txapela"
    } ]
  }, {
    "tStartMs": 3396390,
    "dDurationMs": 2590,
    "segs": [ {
      "utf8": "\n, eta"
    } ]
  }, {
    "tStartMs": 3398980,
    "dDurationMs": 1443,
    "segs": [ {
      "utf8": "horrek benetan ematen du"
    } ]
  }, {
    "tStartMs": 3401790,
    "dDurationMs": 1707,
    "segs": [ {
      "utf8": "aurretiazko datuak"
    } ]
  }, {
    "tStartMs": 3403497,
    "dDurationMs": 1073,
    "segs": [ {
      "utf8": "eta ateratzen ari diren zenbait paper"
    } ]
  }, {
    "tStartMs": 3404570,
    "dDurationMs": 1960,
    "segs": [ {
      "utf8": "\nbenetan zoragarriak direla pentsatzea.  Testuinguru kliniko jakin batzuetan psikodeliko batzuentzat"
    } ]
  }, {
    "tStartMs": 3406530,
    "dDurationMs": 4010,
    "segs": [ {
      "utf8": "\nrol bikainak egon daitezkeela erakutsiz"
    } ]
  }, {
    "tStartMs": 3410540,
    "dDurationMs": 2670,
    "segs": [ {
      "utf8": "\n.  Psikiatra bat bertaratutakoarekin"
    } ]
  }, {
    "tStartMs": 3413210,
    "dDurationMs": 3640,
    "segs": [ {
      "utf8": "egindako azterketa klinikoak dira\n,"
    } ]
  }, {
    "tStartMs": 3416850,
    "dDurationMs": 1590,
    "segs": [ {
      "utf8": "horretarako baimenduta dagoena."
    } ]
  }, {
    "tStartMs": 3418440,
    "dDurationMs": 2600,
    "segs": [ {
      "utf8": "Horrek jendeari lagundu diezaioke\ndepresioa, trauma eta abar."
    } ]
  }, {
    "tStartMs": 3421040,
    "dDurationMs": 870,
    "segs": [ {
      "utf8": "Eta denbora asko emango dugu horri"
    } ]
  }, {
    "tStartMs": 3421910,
    "dDurationMs": 2410,
    "segs": [ {
      "utf8": "buruz hitz egiten\n,"
    } ]
  }, {
    "tStartMs": 3424320,
    "dDurationMs": 2090,
    "segs": [ {
      "utf8": "ikasketa horiek egiten dituzten pertsona batzuekin barne."
    } ]
  }, {
    "tStartMs": 3426410,
    "dDurationMs": 1510,
    "segs": [ {
      "utf8": "Beraz, hori espero dugu."
    } ]
  }, {
    "tStartMs": 3427920,
    "dDurationMs": 2130,
    "segs": [ {
      "utf8": "Beraz, hau guztia esan nahi du,"
    } ]
  }, {
    "tStartMs": 3430050,
    "dDurationMs": 3380,
    "segs": [ {
      "utf8": "ez, ez dut psikodelikorik hartzen\negoera sortzaileetara sartzeko."
    } ]
  }, {
    "tStartMs": 3433430,
    "dDurationMs": 4170,
    "segs": [ {
      "utf8": "Ez da hor nire ustez\nrol nagusia, psikodelikoen paper garrantzitsua"
    } ]
  }, {
    "tStartMs": 3437600,
    "dDurationMs": 3170,
    "segs": [ {
      "utf8": "ager daitekeela\ngizateriarentzat.  Testuinguru klinikoan eginkizun garrantzitsu horiek"
    } ]
  }, {
    "tStartMs": 3440770,
    "dDurationMs": 2740,
    "segs": [ {
      "utf8": "izan ditzakeela uste dut\n"
    } ]
  }, {
    "tStartMs": 3443510,
    "dDurationMs": 1300,
    "segs": [ {
      "utf8": "."
    } ]
  }, {
    "tStartMs": 3444810,
    "dDurationMs": 2610,
    "segs": [ {
      "utf8": "Legez eta segurtasunez egiten bada."
    } ]
  }, {
    "tStartMs": 3447420,
    "dDurationMs": 1535,
    "segs": [ {
      "utf8": "Uste dut,"
    } ]
  }, {
    "tStartMs": 3448955,
    "dDurationMs": 4265,
    "segs": [ {
      "utf8": "sormen-prozesua\nbi fasetako prozesua izateak esan nahi duela,"
    } ]
  }, {
    "tStartMs": 3453220,
    "dDurationMs": 1720,
    "segs": [ {
      "utf8": "pertsonalki,"
    } ]
  }, {
    "tStartMs": 3454940,
    "dDurationMs": 4540,
    "segs": [ {
      "utf8": "\nkontzeptuen esplorazio ez-linealaren aldi hau izateak balio dudala."
    } ]
  }, {
    "tStartMs": 3459480,
    "dDurationMs": 3210,
    "segs": [ {
      "utf8": "Dena dela,\narratsaldean lanean ari naiz."
    } ]
  }, {
    "tStartMs": 3462690,
    "dDurationMs": 2310,
    "segs": [ {
      "utf8": "Baina gero lan hori baztertuko dut."
    } ]
  }, {
    "tStartMs": 3465000,
    "dDurationMs": 3170,
    "segs": [ {
      "utf8": "Alde batera utziko dut eta, ondoren,\nhurrengo egunean"
    } ]
  }, {
    "tStartMs": 3468170,
    "dDurationMs": 1320,
    "segs": [ {
      "utf8": "edo hurrengo egunean berriro aztertuko dut,"
    } ]
  }, {
    "tStartMs": 3469490,
    "dDurationMs": 4070,
    "segs": [ {
      "utf8": "\nlana bera prest dagoen ala ez ikusteko,"
    } ]
  }, {
    "tStartMs": 3473560,
    "dDurationMs": 2030,
    "segs": [ {
      "utf8": ""
    } ]
  }, {
    "tStartMs": 3475590,
    "dDurationMs": 1080,
    "segs": [ {
      "utf8": ""
    } ]
  }, {
    "tStartMs": 3476670,
    "dDurationMs": 1860,
    "segs": [ {
      "utf8": "egoera oso fokatu horietako batean egin nahi nukeen nahita inplementazio lineal baterako.  ."
    } ]
  }, {
    "tStartMs": 3478530,
    "dDurationMs": 3130,
    "segs": [ {
      "utf8": "Beraz, hau esateko modu luzea eta laburra\nda,"
    } ]
  }, {
    "tStartMs": 3481660,
    "dDurationMs": 3000,
    "segs": [ {
      "utf8": "oso erne gaudenean,\neragiketa mota linealak egitea."
    } ]
  }, {
    "tStartMs": 3484660,
    "dDurationMs": 2730,
    "segs": [ {
      "utf8": "\nLoguratsu eta lasaiago egoteko joera dugunean,"
    } ]
  }, {
    "tStartMs": 3487390,
    "dDurationMs": 3770,
    "segs": [ {
      "utf8": "orduan\nsor daitezke lehenik sormen lanak,"
    } ]
  }, {
    "tStartMs": 3491160,
    "dDurationMs": 4300,
    "segs": [ {
      "utf8": "baina haiek gauzatzeak\nernetasun handia eskatzen du."
    } ]
  }, {
    "tStartMs": 3495460,
    "dDurationMs": 1890,
    "segs": [ {
      "utf8": "Orain, horrek gehiago garamatza"
    } ]
  }, {
    "tStartMs": 3497350,
    "dDurationMs": 3000,
    "segs": [ {
      "utf8": "arratsalde amaierako ilunabarrera."
    } ]
  }, {
    "tStartMs": 3500350,
    "dDurationMs": 2060,
    "segs": [ {
      "utf8": "Orain, lehen aipatu dudan bezala,"
    } ]
  }, {
    "tStartMs": 3502410,
    "dDurationMs": 3710,
    "segs": [ {
      "utf8": "\narratsaldean ere eguzki-argia jasotzearen aldekoa naiz."
    } ]
  }, {
    "tStartMs": 3506120,
    "dDurationMs": 4340,
    "segs": [ {
      "utf8": "\nLehen aipatu ez dudan gauza kritikoa da."
    } ]
  }, {
    "tStartMs": 3510460,
    "dDurationMs": 1720,
    "segs": [ {
      "utf8": "Hona hemen nola funtzionatzen duen."
    } ]
  }, {
    "tStartMs": 3512180,
    "dDurationMs": 2380,
    "segs": [ {
      "utf8": "Jende askok entzun didate egunean\ngoiz argitzea"
    } ]
  }, {
    "tStartMs": 3514560,
    "dDurationMs": 1370,
    "segs": [ {
      "utf8": "garrantzitsua dela esaten."
    } ]
  }, {
    "tStartMs": 3515930,
    "dDurationMs": 2790,
    "segs": [ {
      "utf8": "Baina horrek norberaren erlojua aurreratuko du.  Biharamunean goiz"
    } ]
  }, {
    "tStartMs": 3518720,
    "dDurationMs": 2840,
    "segs": [ {
      "utf8": "jaikitzeko gogoa emango dizu\n."
    } ]
  }, {
    "tStartMs": 3521560,
    "dDurationMs": 1490,
    "segs": [ {
      "utf8": "Arratsaldean argia eginez"
    } ]
  }, {
    "tStartMs": 3523050,
    "dDurationMs": 1590,
    "segs": [ {
      "utf8": "bi gauza lortzen ditu niretzat."
    } ]
  }, {
    "tStartMs": 3524640,
    "dDurationMs": 2790,
    "segs": [ {
      "utf8": "Lehenik eta behin,\nez naizela goizegi jaikitzen ziurtatzen du"
    } ]
  }, {
    "tStartMs": 3527430,
    "dDurationMs": 2020,
    "segs": [ {
      "utf8": "\ngoizeko 3:00etan edo 4:00etan ez naizela esnatzen,"
    } ]
  }, {
    "tStartMs": 3529450,
    "dDurationMs": 2710,
    "segs": [ {
      "utf8": "erlojua aldatuko duelako."
    } ]
  }, {
    "tStartMs": 3532160,
    "dDurationMs": 1740,
    "segs": [ {
      "utf8": "Pixka bat atzeratuko du."
    } ]
  }, {
    "tStartMs": 3533900,
    "dDurationMs": 2010,
    "segs": [ {
      "utf8": "Eta, beraz, hau benetan garrantzitsua da."
    } ]
  }, {
    "tStartMs": 3535910,
    "dDurationMs": 3920,
    "segs": [ {
      "utf8": "Zure\nordutegia errutina arrunt batean mantendu nahi baduzu,"
    } ]
  }, {
    "tStartMs": 3539830,
    "dDurationMs": 2520,
    "segs": [ {
      "utf8": "24 orduko ziklo erregularrean"
    } ]
  }, {
    "tStartMs": 3542350,
    "dDurationMs": 1520,
    "segs": [ {
      "utf8": "eta zure"
    } ]
  }, {
    "tStartMs": 3543870,
    "dDurationMs": 2110,
    "segs": [ {
      "utf8": "loaren eta esna-erritmo zirkadianoak\nleku guztietatik noraezean ez edukitzea"
    } ]
  }, {
    "tStartMs": 3545980,
    "dDurationMs": 1320,
    "segs": [ {
      "utf8": "eta"
    } ]
  }, {
    "tStartMs": 3547300,
    "dDurationMs": 1500,
    "segs": [ {
      "utf8": "zure adimenak nola funtzionatuko duen aurreikusteko nolakoa den nahi baduzu"
    } ]
  }, {
    "tStartMs": 3548800,
    "dDurationMs": 2270,
    "segs": [ {
      "utf8": "optimizatzeko.\nikaskuntza eta errendimendua."
    } ]
  }, {
    "tStartMs": 3551070,
    "dDurationMs": 2260,
    "segs": [ {
      "utf8": "Beno, orduan goizeko argia lortu behar duzu\narratsaldeko edozein argi."
    } ]
  }, {
    "tStartMs": 3553330,
    "dDurationMs": 1840,
    "segs": [ {
      "utf8": "Goizeko argiak\nnire erlojua aurreratuko du."
    } ]
  }, {
    "tStartMs": 3555170,
    "dDurationMs": 1740,
    "segs": [ {
      "utf8": "Egin nire sistemak lehenago jaiki nahi izatea."
    } ]
  }, {
    "tStartMs": 3556910,
    "dDurationMs": 1540,
    "segs": [ {
      "utf8": "Eta arratsaldeko argiak"
    } ]
  }, {
    "tStartMs": 3558450,
    "dDurationMs": 2230,
    "segs": [ {
      "utf8": "nire erlojua pixka bat atzeratuko du,\nbatez beste"
    } ]
  }, {
    "tStartMs": 3560680,
    "dDurationMs": 2440,
    "segs": [ {
      "utf8": "nire\nmekanismo zirkadianoak oztopatzeko."
    } ]
  }, {
    "tStartMs": 3563120,
    "dDurationMs": 2180,
    "segs": [ {
      "utf8": "Eta, funtsean,"
    } ]
  }, {
    "tStartMs": 3565300,
    "dDurationMs": 2680,
    "segs": [ {
      "utf8": "\ngauero ordu berean lo egin eta esnatu nahi dut,"
    } ]
  }, {
    "tStartMs": 3567980,
    "dDurationMs": 2250,
    "segs": [ {
      "utf8": "\ngoizero ordu berean gutxi gorabehera."
    } ]
  }, {
    "tStartMs": 3570230,
    "dDurationMs": 1210,
    "segs": [ {
      "utf8": "Horrela funtzionatzen du."
    } ]
  }, {
    "tStartMs": 3571440,
    "dDurationMs": 2600,
    "segs": [ {
      "utf8": "Eta hori kable gogorra den mekanismoa da."
    } ]
  }, {
    "tStartMs": 3574040,
    "dDurationMs": 2500,
    "segs": [ {
      "utf8": "Hori ez da nire\nburuari esaten diodan gauza subjektiboa."
    } ]
  }, {
    "tStartMs": 3576540,
    "dDurationMs": 2210,
    "segs": [ {
      "utf8": "Hori kableatutako mekanismo bat da."
    } ]
  }, {
    "tStartMs": 3578750,
    "dDurationMs": 2390,
    "segs": [ {
      "utf8": "Beraz, arratsaldera eramaten gaitu."
    } ]
  }, {
    "tStartMs": 3581140,
    "dDurationMs": 1940,
    "segs": [ {
      "utf8": "Eta, oro har,\narratsaldean argi hori aterako dut"
