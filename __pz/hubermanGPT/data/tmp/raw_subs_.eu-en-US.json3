  }, {
    "tStartMs": 3583080,
    "dDurationMs": 990,
    "segs": [ {
      "utf8": "kalera ateratzean"
    } ]
  }, {
    "tStartMs": 3584070,
    "dDurationMs": 940,
    "segs": [ {
      "utf8": "edo batzuetan"
    } ]
  }, {
    "tStartMs": 3585010,
    "dDurationMs": 2130,
    "segs": [ {
      "utf8": "argi artifizialak argi piztuz egingo dut,"
    } ]
  }, {
    "tStartMs": 3587140,
    "dDurationMs": 1880,
    "segs": [ {
      "utf8": "eta gero arratsalderako iluntzen hasiko naiz,\n"
    } ]
  }, {
    "tStartMs": 3589020,
    "dDurationMs": 1560,
    "segs": [ {
      "utf8": "lehen aipatu dudan bezala."
    } ]
  }, {
    "tStartMs": 3590580,
    "dDurationMs": 1610,
    "segs": [ {
      "utf8": "eta ez dut kontua landuko."
    } ]
  }, {
    "tStartMs": 3592190,
    "dDurationMs": 2220,
    "segs": [ {
      "utf8": "Argiaren esposizioa minimizatu nahi duzu,"
    } ]
  }, {
    "tStartMs": 3594410,
    "dDurationMs": 1920,
    "segs": [ {
      "utf8": "batez ere goitiko argi distiratsuaren esposizioa,"
    } ]
  }, {
    "tStartMs": 3596330,
    "dDurationMs": 2280,
    "segs": [ {
      "utf8": "\nargi urdina den ala ez kontuan hartu gabe."
    } ]
  }, {
    "tStartMs": 3598610,
    "dDurationMs": 2530,
    "segs": [ {
      "utf8": "Arratsaldean\n22:00etatik 4:00etara."
    } ]
  }, {
    "tStartMs": 3601140,
    "dDurationMs": 1240,
    "segs": [ {
      "utf8": "Batzuek galdetu duzue,"
    } ]
  }, {
    "tStartMs": 3602380,
    "dDurationMs": 2220,
    "segs": [ {
      "utf8": "itxaron 23:00etatik 4:00etara zela uste nuen?"
    } ]
  }, {
    "tStartMs": 3604600,
    "dDurationMs": 3270,
    "segs": [ {
      "utf8": "Tira, hala da, baina 22:00etatik\n4:00etara are hobea da."
    } ]
  }, {
    "tStartMs": 3607870,
    "dDurationMs": 4130,
    "segs": [ {
      "utf8": "Hasieran\n22:00etatik 4:00etara esan nionean,"
    } ]
  }, {
    "tStartMs": 3612000,
    "dDurationMs": 1280,
    "segs": [ {
      "utf8": "jendea horrela zegoenez, ezinezkoa da"
